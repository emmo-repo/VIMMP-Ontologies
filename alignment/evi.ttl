
### EMMO-VIMMP INTEGRATION (EVI) ###

### VERSION 0.4.3 ###

### DATED 24TH APRIL 2020 ###

@prefix evi: <http://www.vimmp.eu/semantics/alignment/evi.ttl#>.

@prefix emmo1s: <http://www.vimmp.eu/semantics/alignment/emmo1s.ttl#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.
@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.
@prefix viso: <http://www.vimmp.eu/semantics/viso/viso-general.ttl#>.
@prefix viso-am: <http://www.vimmp.eu/semantics/viso/viso-atomistic-mesoscopic.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/evi/evi.ttl> a owl:Ontology.


#########################
# METAONTOLOGICAL LEVEL #
#########################

evi:meta_emmo a owl:Class;
   owl:disjointUnionOf (emmo1s:EMMO evi:non_emmo);
   rdfs:comment "this class contains EMMO entities as well as non-spacetime entities that are not covered by the EMMO"^^xs:string.
evi:non_emmo a owl:Class;
   rdfs:subClassOf evi:meta_emmo;
   rdfs:comment "any non-spacetime entities (hence, not covered by the EMMO) are to be subsumed under this class"^^xs:string.


####################
# EVMPO CATEGORIES #
####################

evmpo:marketplace_related_entity a owl:Class;
   rdfs:subClassOf evi:meta_emmo.
evmpo:paradigmatic_entity a owl:Class;
   rdfs:subClassOf evi:meta_emmo, evmpo:marketplace_related_entity.

# category (1): assessment
#
evmpo:assessment a owl:Class;
   rdfs:subClassOf emmo1s:Sign, evmpo:paradigmatic_entity;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty evi:is_sign_for;
      owl:someValuesFrom evmpo:information_content_entity
   ].

# category (2): calendar_event
#
evmpo:calendar_event a owl:Class;
   rdfs:subClassOf emmo1s:Sign, evmpo:paradigmatic_entity;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty evi:is_sign_for;
      owl:someValuesFrom emmo1s:Process
   ].

# category (3): communication
#
evmpo:communication a owl:Class;
   rdfs:subClassOf emmo1s:Sign, evmpo:paradigmatic_entity.

# category (4): information_content_entity
#
evmpo:information_content_entity a owl:Class;
   rdfs:subClassOf emmo1s:Symbolic, evmpo:paradigmatic_entity.
evmpo:document a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity.

# category (5): infrastructure
#
# unsure what to do with this
#
evmpo:infrastructure a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity.

# category (6): interpreter
#
evmpo:interpreter a owl:Class;
   owl:equivalentClass emmo1s:Interpreter;
   rdfs:subClassOf evmpo:paradigmatic_entity.

# category (7): material
#
evmpo:material a owl:Class;
   owl:equivalentClass emmo1s:Material;
   rdfs:subClassOf evmpo:paradigmatic_entity.

# category (8): model
#
evmpo:model a owl:Class;
   owl:equivalentClass emmo1s:Model;
   rdfs:subClassOf evmpo:paradigmatic_entity.

# category (9): process
#
evmpo:process a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity.
evmpo:business_process a owl:Class;
   rdfs:subClassOf evmpo:model, evmpo:process;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty evi:is_model_for;
      owl:someValuesFrom emmo1s:Process
   ].
evmpo:physical_process a owl:Class;
   owl:equivalentClass emmo1s:Process;
   rdfs:subClassOf evmpo:process.
evmpo:simulation a owl:Class;
   rdfs:subClassOf emmo1s:Semiosis, evmpo:process.

# category (10): product
#
evmpo:product a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity.
evmpo:good a owl:Class;
   rdfs:subClassOf emmo1s:Engineered, evmpo:product.
evmpo:service a owl:Class;
   rdfs:subClassOf emmo1s:Sign, evmpo:product;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty evi:is_sign_for;
      owl:someValuesFrom emmo1s:Process
   ].

# category (11): property
#
evmpo:property a owl:Class;
   owl:equivalentClass emmo1s:Property;
   rdfs:subClassOf evmpo:paradigmatic_entity.

# category (0): annotation
#
evmpo:annotation a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity.


##################
# MEREOSEMIOTICS #
##################

evi:has_mereosemiotic_relation_with a owl:ObjectProperty, owl:SymmetricProperty, owl:TransitiveProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO.

evi:is_proper_part_of a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   owl:inverseOf emmo1s:hasProperPart;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with.
emmo1s:hasProperPart rdfs:subPropertyOf evi:has_mereosemiotic_relation_with.
evi:is_sign_for a owl:ObjectProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:Object;
   owl:inverseOf emmo1s:hasSign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with.
evi:is_model_for a owl:ObjectProperty;
   rdfs:domain emmo1s:Model;
   rdfs:range emmo1s:Object;
   owl:inverseOf emmo1s:hasModel;
   rdfs:subPropertyOf evi:is_sign_for.
emmo1s:hasSign rdfs:subPropertyOf evi:has_mereosemiotic_relation_with.

# :a evi:mereosemiotics_hp_ip :b  <=>  :a emmo1s:hasProperPart [evi:is_proper_part_of :b].
#
evi:mereosemiotics_hp_ip a owl:ObjectProperty, owl:SymmetricProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:equivalentProperty emmo1s:hasOverlapWith;
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:is_proper_part_of).

# :a evi:mereosemiotics_ip_is :b  <=>  :a evi:is_proper_part_of [evi:is_sign_for :b].
#
evi:mereosemiotics_ip_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:is_proper_part_of evi:is_sign_for).

evi:mereosemiotics_ip_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:is_proper_part_of emmo1s:hasSign).

evi:mereosemiotics_hp_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:is_sign_for).

evi:mereosemiotics_hp_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (emmo1s:hasProperPart emmo1s:hasSign).

evi:mereosemiotics_is_is a owl:ObjectProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:is_sign_for evi:is_sign_for).

evi:mereosemiotics_is_hs a owl:ObjectProperty, owl:SymmetricProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:is_sign_for emmo1s:hasSign).

evi:mereosemiotics_hs_is a owl:ObjectProperty, owl:SymmetricProperty;
   rdfs:domain emmo1s:Object;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (emmo1s:hasSign evi:is_sign_for).

# :a evi:mereosemiotics_hp_ip_is :b  <=>  :a emmo1s:hasProperPart [evi:is_proper_part_of [evi:is_sign_for :b]].
#
evi:mereosemiotics_hp_ip_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_ip evi:is_sign_for);
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:mereosemiotics_ip_is).

# :a evi:mereosemiotics_hp_ip_hs :b  <=>  :a emmo1s:hasProperPart [evi:is_proper_part_of [emmo1s:hasSign :b]].
#
evi:mereosemiotics_hp_ip_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_ip emmo1s:hasSign);
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:mereosemiotics_ip_hs).

evi:mereosemiotics_ip_is_ip a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_is evi:is_proper_part_of);
   owl:propertyChainAxiom (evi:is_proper_part_of [owl:inverseOf evi:mereosemiotics_hp_hs]).

evi:mereosemiotics_ip_is_hp a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_is emmo1s:hasProperPart);
   owl:propertyChainAxiom (evi:is_proper_part_of [owl:inverseOf evi:mereosemiotics_ip_hs]).

evi:mereosemiotics_ip_is_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_is evi:is_sign_for);
   owl:propertyChainAxiom (evi:is_proper_part_of evi:mereosemiotics_is_is).

evi:mereosemiotics_ip_is_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_is emmo1s:hasSign);
   owl:propertyChainAxiom (evi:is_proper_part_of evi:mereosemiotics_is_hs).

evi:mereosemiotics_ip_hs_ip a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_hs evi:is_proper_part_of);
   owl:propertyChainAxiom (evi:is_proper_part_of [owl:inverseOf evi:mereosemiotics_hp_is]).

evi:mereosemiotics_ip_hs_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_hs evi:is_sign_for);
   owl:propertyChainAxiom (evi:is_proper_part_of evi:mereosemiotics_hs_is).

evi:mereosemiotics_ip_hs_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_ip_hs emmo1s:hasSign);
   owl:propertyChainAxiom (evi:is_proper_part_of [owl:inverseOf evi:mereosemiotics_is_is]).

evi:mereosemiotics_hp_is_ip a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:EMMO;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_is evi:is_proper_part_of);
   owl:propertyChainAxiom (emmo1s:hasProperPart [owl:inverseOf evi:mereosemiotics_hp_hs]).

evi:mereosemiotics_hp_is_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_is evi:is_sign_for);
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:mereosemiotics_is_is).

evi:mereosemiotics_hp_is_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_is emmo1s:hasSign);
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:mereosemiotics_is_hs).

evi:mereosemiotics_hp_hs_is a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_hs evi:is_sign_for);
   owl:propertyChainAxiom (emmo1s:hasProperPart evi:mereosemiotics_hs_is).

evi:mereosemiotics_hp_hs_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:EMMO;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom (evi:mereosemiotics_hp_hs emmo1s:hasSign);
   owl:propertyChainAxiom (emmo1s:hasProperPart [owl:inverseOf evi:mereosemiotics_is_is]).

evi:mereosemiotics_is_ip_is a owl:ObjectProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom ([owl:inverseOf evi:mereosemiotics_hp_hs] evi:is_sign_for);
   owl:propertyChainAxiom (evi:is_sign_for evi:mereosemiotics_ip_is).

evi:mereosemiotics_is_ip_hs a owl:ObjectProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:Sign;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom ([owl:inverseOf evi:mereosemiotics_hp_hs] emmo1s:hasSign);
   owl:propertyChainAxiom (evi:is_sign_for evi:mereosemiotics_ip_hs).

evi:mereosemiotics_is_hp_is a owl:ObjectProperty;
   rdfs:domain emmo1s:Sign;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom ([owl:inverseOf evi:mereosemiotics_ip_hs] evi:is_sign_for);
   owl:propertyChainAxiom (evi:is_sign_for evi:mereosemiotics_hp_is).

evi:mereosemiotics_hs_ip_is a owl:ObjectProperty;
   rdfs:domain emmo1s:Object;
   rdfs:range emmo1s:Object;
   rdfs:subPropertyOf evi:has_mereosemiotic_relation_with;
   owl:propertyChainAxiom ([owl:inverseOf evi:mereosemiotics_hp_is] evi:is_sign_for);
   owl:propertyChainAxiom (emmo1s:hasSign evi:mereosemiotics_ip_is).


#########################
# OSMO-EMMO INTEGRATION #
#########################

# the OSMO concept "materials_relation" and the EMMO concept "material_relation"
# both refer to the same entity, namely, a materials relation (MR) according to RoMM
#
osmo:materials_relation a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass emmo1s:MaterialRelation.


##########################
# OTRAS-EMMO INTEGRATION #
##########################

# a citation is a sign for an information content entity
#
otras:citation a owl:Class;
   rdfs:subClassOf emmo1s:Sign, evmpo:information_content_entity;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty evi:is_sign_for;
      owl:someValuesFrom evmpo:information_content_entity
   ].

# a course is a process
#
otras:course a owl:Class;
   rdfs:subClassOf emmo1s:Process, evmpo:process.

# a focus is a symbolic entity
#
otras:focus a owl:Class;
   rdfs:subClassOf emmo1s:Symbolic, evmpo:annotation.

# a topic is a sign
#
otras:topic a owl:Class;
   rdfs:subClassOf emmo1s:Sign, otras:focus.

otras:has_abstract a owl:ObjectProperty;
   rdfs:subPropertyOf emmo1s:hasProperPart.
otras:has_offered_course a owl:ObjectProperty;
   rdfs:subPropertyOf [owl:inverseOf evi:mereosemiotics_ip_hs], evi:has_mereosemiotic_relation_with.
otras:is_citation_about_document a owl:ObjectProperty;
   rdfs:subPropertyOf evi:is_sign_for.
otras:is_cited_as a owl:ObjectProperty;
   rdfs:subPropertyOf emmo1s:hasSign.


#########################
# VISO-EMMO INTEGRATION #
#########################

# an interaction site is a symbol
# (i.e., it is an element of the alphabet for an appropriately specified formal language)
#
viso-am:interaction_site a owl:Class;
   rdfs:subClassOf emmo1s:Symbol, viso:model_object.

# a model object is a symbolic entity
#
viso:model_object a owl:Class;
   rdfs:subClassOf emmo1s:Symbolic, evmpo:marketplace_related_entity.

viso:has_part a owl:ObjectProperty;
   rdfs:subPropertyOf emmo1s:hasPart.

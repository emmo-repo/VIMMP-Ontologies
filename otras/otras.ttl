   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "apply"^^xs:string;
   otras:is_defined_by "use a known idea, equation, principle, theory, or law in a new situation"^^xs:string.

otras:KMK_OPERATOR_240 a otras:kmk_operator_specifier;
   otras:has_operator_code 240;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "calculate"^^xs:string;
   otras:is_defined_by "insert the corresponding values into an equation and generate the result"^^xs:string.

otras:KMK_OPERATOR_245 a otras:kmk_operator_specifier;
   otras:has_operator_code 245;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "describe"^^xs:string;
   otras:is_defined_by "give a detailed and structured description of something using the appropriate terminology"^^xs:string.

otras:KMK_OPERATOR_250 a otras:kmk_operator_specifier;
   otras:has_operator_code 250;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "find"^^xs:string;
   otras:is_defined_by "generate a result from data given (graphically or numerically)"^^xs:string.

otras:KMK_OPERATOR_255 a otras:kmk_operator_specifier;
   otras:has_operator_code 255;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "explain"^^xs:string;
   otras:is_defined_by "give a detailed account of causes, reasons, or mechanisms to illuminate structures, processes, and relationships"^^xs:string.

otras:KMK_OPERATOR_260 a otras:kmk_operator_specifier;
   otras:has_operator_code 260;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "describe and explain"^^xs:string;
   otras:is_defined_by "describe, explain, and give examples"^^xs:string.

otras:KMK_OPERATOR_265 a otras:kmk_operator_specifier;
   otras:has_operator_code 265;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "formulate"^^xs:string;
   otras:is_defined_by "represent processes and facts verbally or symbolically"^^xs:string.

otras:KMK_OPERATOR_270 a otras:kmk_operator_specifier;
   otras:has_operator_code 270;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "derive"^^xs:string;
   otras:is_defined_by "manipulate mathematical relationships to obtain a new equation or relationship, commenting the main steps on the way"^^xs:string.

otras:KMK_OPERATOR_275 a otras:kmk_operator_specifier;
   otras:has_operator_code 275;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "sort/group/classify"^^xs:string;
   otras:is_defined_by "arrange systematically, in proper order or appropriately grouped, based on existing principles or according to certain features"^^xs:string.

otras:KMK_OPERATOR_280 a otras:kmk_operator_specifier;
   otras:has_operator_code 280;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "test/verify"^^xs:string;
   otras:is_defined_by "check facts and interrelations for quality, performance, and/or reliability"^^xs:string.

otras:KMK_OPERATOR_285 a otras:kmk_operator_specifier;
   otras:has_operator_code 285;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "investigate/examine"^^xs:string;
   otras:is_defined_by "research a subject to discover information and facts"^^xs:string.

otras:KMK_OPERATOR_290 a otras:kmk_operator_specifier;
   otras:has_operator_code 290;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "generalize"^^xs:string;
   otras:is_defined_by "formulate or derive a general statement"^^xs:string.

otras:KMK_OPERATOR_295 a otras:kmk_operator_specifier;
   otras:has_operator_code 295;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "summarize"^^xs:string;
   otras:is_defined_by "give a brief account or summary"^^xs:string.

otras:KMK_OPERATOR_300 a otras:kmk_operator_specifier;
   otras:has_operator_code 300;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "KMK level III operator (unspecified)"^^xs:string;
   otras:is_defined_by "an unspecified learning operator at the advanced level, as categorized by the KMK document"^^xs:string.

otras:KMK_OPERATOR_320 a otras:kmk_operator_specifier;
   otras:has_operator_code 320;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "propose a hypothesis"^^xs:string;
   otras:is_defined_by "suggest or construct a clearly focused and justified assumption"^^xs:string.

otras:KMK_OPERATOR_330 a otras:kmk_operator_specifier;
   otras:has_operator_code 330;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "evaluate"^^xs:string;
   otras:is_defined_by "process data and results, deduce a relationship between variables, make general statements, and assess implications"^^xs:string.

otras:KMK_OPERATOR_340 a otras:kmk_operator_specifier;
   otras:has_operator_code 340;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "justify/give reasons"^^xs:string;
   otras:is_defined_by "relate pheonomena to underlying rules, (physical) laws, and causal relationships"^^xs:string.

otras:KMK_OPERATOR_350 a otras:kmk_operator_specifier;
   otras:has_operator_code 350;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "comment on/assess"^^xs:string;
   otras:is_defined_by "judge something on the basis of scientific criteria or methods"^^xs:string.

otras:KMK_OPERATOR_360 a otras:kmk_operator_specifier;
   otras:has_operator_code 360;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "prove"^^xs:string;
   otras:is_defined_by "show something by means of factual argumentation and logical reasoning"^^xs:string.

otras:KMK_OPERATOR_370 a otras:kmk_operator_specifier;
   otras:has_operator_code 370;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "discuss"^^xs:string;
   otras:is_defined_by "investigate or examine by argument, give and weigh arguments for and against something"^^xs:string.

otras:KMK_OPERATOR_380 a otras:kmk_operator_specifier;
   otras:has_operator_code 380;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "interpret"^^xs:string;
   otras:is_defined_by "find explanations for phenomena/data to reach conclusions"^^xs:string.

otras:KMK_OPERATOR_390 a otras:kmk_operator_specifier;
   otras:has_operator_code 390;
   otras:has_kmk_level otras:KMK_LEVEL_III;
   otras:is_expressed_by "plan"^^xs:string;
   otras:is_defined_by "come up with a design for an experiment or a structured approach to investigate a problem"^^xs:string.


otras:citation a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity, iao:IAO_0000030;  # iao label "information content entity"
   rdfs:comment "citation as defined by IAO; n.b., OTRAS adds some properties to this beyond what is specified by the IAO"^^xs:string;
   owl:equivalentClass iao:IAO_0000301.  # iao label "citation"
otras:op_citation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_citation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_information_content_entity rdfs:subClassOf otras:op_citation.
evmpo:dp_information_content_entity rdfs:subClassOf otras:dp_citation.

otras:has_author_map a otras:op_citation;
   rdfs:domain otras:citation;
   rdfs:range otras:author_map;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.

otras:has_doi a otras:dp_citation;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_first_author_surname a otras:dp_citation;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_publication_title a otras:dp_citation;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_publication_year a otras:dp_citation;
   rdfs:domain otras:citation;
   rdfs:range xs:gYear.

otras:is_about a evmpo:op_information_content_entity, owl:ObjectProperty;
   rdfs:domain evmpo:information_content_entity;
   rdfs:range evmpo:marketplace_related_entity;
   rdfs:subPropertyOf iao:IAO_0000136.  # iao property labelled "is_about"
otras:is_citation_about_document a otras:op_citation;
   rdfs:domain otras:citation;
   rdfs:range otras:document;
   rdfs:subPropertyOf otras:is_about;
   owl:inverseOf otras:is_cited_as.

otras:R_HAS_ONE_FIRST_AUTHOR_SURNAME a owl:Restriction;  # important to ensure that any set of citations can be sorted by first-author surname
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_first_author_surname;
   owl:cardinality 1.
otras:R_HAS_ONE_PUBLICATION_YEAR a owl:Restriction;  # important to ensure that any set of citations can be sorted by year
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_year;
   owl:cardinality 1.
otras:R_IS_CITATION_ABOUT_A_DOCUMENT a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_citation_about_document;
   owl:someValuesFrom otras:document.
otras:citation rdfs:subClassOf otras:R_HAS_ONE_PUBLICATION_YEAR, otras:R_IS_CITATION_ABOUT_A_DOCUMENT.

otras:doi_citation a owl:Class;
   rdfs:subClassOf otras:citation;
   rdfs:comment "DOI citation, i.e., a citation that has a DOI"^^xs:string.
otras:op_doi_citation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_doi_citation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_citation rdfs:subClassOf otras:op_doi_citation.
evmpo:dp_citation rdfs:subClassOf otras:dp_doi_citation.

otras:R_IS_ABOUT_ONE_ENTITY a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_about;
   owl:cardinality 1.
otras:R_HAS_A_DOI a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_doi;
   owl:someValuesFrom xs:string.
otras:doi_citation
   rdfs:subClassOf otras:R_IS_ABOUT_ONE_ENTITY;
   owl:intersectionOf (otras:citation otras:R_HAS_A_DOI).

otras:journal_article_citation a owl:Class;
   rdfs:subClassOf otras:doi_citation;
   rdfs:comment "journal article citation, i.e., a DOI citation that is about a journal article"^^xs:string.
otras:op_journal_article_citation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_journal_article_citation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_doi_citation rdfs:subClassOf otras:op_journal_article_citation.
evmpo:dp_doi_citation rdfs:subClassOf otras:dp_journal_article_citation.

otras:R_IS_CITATION_ABOUT_A_JOURNAL_ARTICLE a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_citation_about_document;
   owl:someValuesFrom otras:journal_article.
otras:journal_article_citation owl:intersectionOf (otras:doi_citation otras:R_IS_CITATION_ABOUT_A_JOURNAL_ARTICLE).

otras:has_publication_journal_identifier a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.
otras:has_publication_journal_name a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_abbreviation a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_issn a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.

otras:has_publication_journal_volume a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_issue a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_pages a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.

otras:R_HAS_A_PUBLICATION_JOURNAL_IDENTIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_identifier;
   owl:someValuesFrom xs:string.
otras:R_HAS_A_PUBLICATION_JOURNAL_VOLUME a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_volume;
   owl:someValuesFrom xs:positiveInteger.
otras:R_HAS_PUBLICATION_JOURNAL_PAGES a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_pages;
   owl:someValuesFrom xs:string.
otras:journal_article_citation rdfs:subClassOf otras:R_HAS_A_PUBLICATION_JOURNAL_IDENTIFIER, otras:R_HAS_A_PUBLICATION_JOURNAL_VOLUME, otras:R_HAS_PUBLICATION_JOURNAL_PAGES.

otras:author_identification_entity a owl:Class;
   rdfs:subClassOf otras:training_entity, evmpo:annotation;
   rdfs:comment "author identification entity, i.e., an entity related to the identification of authors"^^xs:string.
otras:op_author_identification_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_identification_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_training_entity rdfs:subClassOf otras:op_author_identification_entity.
otras:dp_training_entity rdfs:subClassOf otras:dp_author_identification_entity.
evmpo:op_annotation rdfs:subClassOf otras:op_author_identification_entity.
evmpo:dp_annotation rdfs:subClassOf otras:dp_author_identification_entity.

otras:author_identifier a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author identifier, i.e., a descriptor by which an author can be identified"^^xs:string.
otras:op_author_identifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_identifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_identifier.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_identifier.

otras:has_identified_given_name a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_given_name_initials a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_person a otras:op_author_identifier;
   rdfs:domain otras:author_identifier;
   rdfs:range cviii:person.
otras:has_identified_surname a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.

otras:R_HAS_ONE_IDENTIFIED_SURNAME a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_identified_surname;
   owl:cardinality 1.

otras:author_map a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map, i.e., an indexed collection of author identifiers (n.b., not a collection of the persons themselves, but of identifiers)"^^xs:string.
otras:op_author_map a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_map a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_map.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_map.

otras:has_author_map_element a otras:op_author_map;
   rdfs:domain otras:author_map;
   rdfs:range otras:author_map_element;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:author_map_element a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map element, i.e., a numbered entry within an indexed collection of author identifiers"^^xs:string.
otras:op_author_map_element a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_map_element a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_map_element.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_map_element.

otras:has_ame_identifier a otras:op_author_map_element, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range otras:author_identifier;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
otras:has_ame_index a otras:dp_author_map_element, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range xs:nonNegativeInteger.

otras:R_HAS_ONE_AME_IDENTIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_ame_identifier;
   owl:cardinality 1.
otras:R_HAS_ONE_AME_INDEX a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_ame_index;
   owl:cardinality 1.
otras:author_map_element rdfs:subClassOf otras:R_HAS_ONE_AME_IDENTIFIER, otras:R_HAS_ONE_AME_INDEX.

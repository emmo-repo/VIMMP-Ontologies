otras:kmk_operator_specifier rdfs:subClassOf otras:R_HAS_EXACTLY_ONE_OPERATOR_CODE.

otras:KMK_OPERATOR_100 a otras:kmk_operator_specifier;
   otras:has_operator_code 100;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "KMK level I operator (unspecified)"^^xs:string;
   otras:is_defined_by "an unspecified learning operator at the basic level, as categorized by the KMK document"^^xs:string.

otras:KMK_OPERATOR_120 a otras:kmk_operator_specifier;
   otras:has_operator_code 120;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "name/label"^^xs:string;
   otras:is_defined_by "assign the specific terms to a given structure"^^xs:string.

otras:KMK_OPERATOR_130 a otras:kmk_operator_specifier;
   otras:has_operator_code 130;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "outline/present"^^xs:string;
   otras:is_defined_by "give the main features or general principles in a structured way"^^xs:string.

otras:KMK_OPERATOR_140 a otras:kmk_operator_specifier;
   otras:has_operator_code 140;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "list/give"^^xs:string;
   otras:is_defined_by "give names/terms or other brief answers, leaving out the explanation"^^xs:string.

otras:KMK_OPERATOR_150 a otras:kmk_operator_specifier;
   otras:has_operator_code 150;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "write a lab report/data log"^^xs:string;
   otras:is_defined_by "write down procedures, observations and results (or discussions and conclusions), if applicable, following scientific conventions"^^xs:string.

otras:KMK_OPERATOR_160 a otras:kmk_operator_specifier;
   otras:has_operator_code 160;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "sketch"^^xs:string;
   otras:is_defined_by "clearly lay out facts/structures/results by means of a graph/diagram/table or similar"^^xs:string.

otras:KMK_OPERATOR_170 a otras:kmk_operator_specifier;
   otras:has_operator_code 170;
   otras:has_kmk_level otras:KMK_LEVEL_I;
   otras:is_expressed_by "draw"^^xs:string;
   otras:is_defined_by "create an exact graphical representation"^^xs:string.

otras:KMK_OPERATOR_200 a otras:kmk_operator_specifier;

otras:operator_level a owl:Class;
   rdfs:comment "operator level, i.e., a requirement level for competency statement operators"^^xs:string;
   rdfs:subClassOf otras:focus.
otras:op_operator_level a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_operator_level a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_focus rdfs:subClassOf otras:op_operator_level.
otras:dp_focus rdfs:subClassOf otras:dp_operator_level.

otras:is_at_numerical_level a otras:dp_operator_level;
   rdfs:domain otras:operator_level;
   rdfs:range xs:nonNegativeInteger;
   rdfs:comment "value which is 0 (special case: below basic), 1 (basic), 2 (intermediate), 3 (advanced), 4 (expert), or 5 (special case: exceptional distinction or unique skill)".

otras:EXPERT_LEVEL a otras:operator_level;
   otras:is_at_numerical_level 4.

otras:kmk_operator_level a owl:Class;
   rdfs:comment "KMK operator level, i.e., a requirement level corresponding to one of the headings from the KMK catalogue of operators for statements of learning outcomes in natural-science subjects"^^xs:string;
   rdfs:subClassOf otras:operator_level.
otras:op_kmk_operator_level a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_kmk_operator_level a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_operator_level rdfs:subClassOf otras:op_kmk_operator_level.
otras:dp_operator_level rdfs:subClassOf otras:dp_kmk_operator_level.

otras:R_IS_AT_ONE_NUMERICAL_LEVEL a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_at_numerical_level;
   owl:cardinality 1.
otras:kmk_operator_level rdfs:subClassOf otras:R_IS_AT_ONE_NUMERICAL_LEVEL.

otras:kmk_operator_level owl:oneOf (otras:KMK_LEVEL_I otras:KMK_LEVEL_II otras:KMK_LEVEL_III).

otras:KMK_LEVEL_I a otras:kmk_operator_level;
   otras:is_at_numerical_level 1.
otras:KMK_LEVEL_II a otras:kmk_operator_level;
   otras:is_at_numerical_level 2.
otras:KMK_LEVEL_III a otras:kmk_operator_level;
   otras:is_at_numerical_level 3.

otras:kmk_operator_specifier a owl:Class;
   rdfs:comment "KMK operator specifier, i.e., an operator from the catalogue for natural-science subjects agreed upon by the German Federal Conference of State Cultural Ministries (KMK)"^^xs:string;
   rdfs:subClassOf otras:operator_specifier.
otras:op_kmk_operator_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_kmk_operator_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_operator_specifier rdfs:subClassOf otras:op_kmk_operator_specifier.
otras:dp_operator_specifier rdfs:subClassOf otras:dp_kmk_operator_specifier.

otras:has_kmk_level a otras:op_kmk_operator_specifier;
   rdfs:domain otras:kmk_operator_specifier;
   rdfs:range otras:kmk_operator_level;
   rdfs:subPropertyOf otras:has_operator_level.

otras:R_HAS_A_KMK_LEVEL a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_kmk_level;
   owl:someValuesFrom otras:kmk_operator_level.
otras:R_HAS_ONE_OPERATOR_CODE a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_operator_code;
   owl:cardinality 1.
otras:R_IS_DEFINED_BY_A_STRING a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_defined_by;
   owl:someValuesFrom xs:string.
otras:kmk_operator_specifier rdfs:subClassOf otras:R_HAS_A_KMK_LEVEL, otras:R_HAS_ONE_OPERATOR_CODE, otras:R_IS_DEFINED_BY_A_STRING.

otras:expert_operator_specifier a owl:Class;
   rdfs:comment "expert operator specifier, i.e., an competency statement operator specifier for expert-level competencies"^^xs:string;
   rdfs:subClassOf otras:operator_specifier.
otras:op_expert_operator_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_expert_operator_specifier a owl:Class;

   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "estimate"^^xs:string;
   otras:is_defined_by "find an approximate and reasonable value for an unknown quantity"^^xs:string.

otras:KMK_OPERATOR_230 a otras:kmk_operator_specifier;

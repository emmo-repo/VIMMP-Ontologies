otras:citation a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity, iao:IAO_0000030;  # iao label "information content entity"
   rdfs:comment "citation as defined by IAO; n.b., OTRAS adds some properties to this beyond what is specified by the IAO"^^xs:string;
   owl:equivalentClass iao:IAO_0000301.  # iao label "citation"

otras:has_author_map a owl:ObjectProperty;
   rdfs:domain otras:citation;
   rdfs:range otras:author_map;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.

otras:has_doi a owl:DatatypeProperty;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_first_author_surname a owl:DatatypeProperty;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_publication_title a owl:DatatypeProperty;
   rdfs:domain otras:citation;
   rdfs:range xs:string.
otras:has_publication_year a owl:DatatypeProperty;
   rdfs:domain otras:citation;
   rdfs:range xs:year.
otras:has_url a owl:DatatypeProperty;
   rdfs:domain otras:citation;
   rdfs:range xs:string.

otras:is_about a owl:ObjectProperty;
   rdfs:domain evmpo:information_content_entity;
   rdfs:range evmpo:marketplace_related_entity;
   rdfs:subPropertyOf iao:IAO_0000136.  # iao property labelled "is_about"
otras:is_citation_about_document a owl:ObjectProperty;
   rdfs:domain otras:citation;
   rdfs:range evmpo:document;
   rdfs:subPropertyOf otras:is_about;
   owl:inverseOf otras:is_cited_as.

otras:has_document_topic a owl:ObjectProperty;
   rdfs:domain evmpo:document;
   rdfs:range otras:topic;
   owl:inverseOf otras:is_discussed_by_document;
   rdfs:subPropertyOf otras:is_about, evmpo:has_information_content_entity_annotation.

otras:citation rdfs:subClassOf [
      a owl:Restriction;  # important to ensure that any set of citations can be sorted by first-author surname
      owl:onProperty otras:has_first_author_surname;
      owl:cardinality 1
   ], [
      a owl:Restriction;  # important to ensure that any set of citations can be sorted by year
      owl:onProperty otras:has_publication_year;
      owl:cardinality 1
   ], [
      a owl:Restriction;
      owl:onProperty otras:is_citation_about_document;
      owl:someValuesFrom evmpo:document
   ].

otras:doi_citation a owl:Class;
   rdfs:subClassOf otras:citation;
   rdfs:comment "DOI citation, i.e., a citation that has a DOI"^^xs:string.

otras:doi_citation rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:is_about;
      owl:cardinality 1
   ];
   owl:intersectionOf (otras:citation [a owl:Restriction; owl:onProperty otras:has_doi; owl:someValuesFrom xs:string]).

otras:book_citation a owl:Class;
   rdfs:subClassOf otras:citation;
   rdfs:comment "book citation, i.e., a citation that is about a book"^^xs:string.

otras:book_citation owl:intersectionOf (otras:citation [a owl:Restriction; owl:onProperty otras:is_citation_about_document; owl:someValuesFrom otras:book]).

otras:has_cited_book_isbn a owl:DatatypeProperty;
   rdfs:domain otras:book_citation;
   rdfs:range xs:string.
otras:has_cited_book_publisher_name a owl:DatatypeProperty;
   rdfs:domain otras:book_citation;
   rdfs:range xs:string.
otras:has_cited_book_publisher_city a owl:DatatypeProperty;
   rdfs:domain otras:book_citation;
   rdfs:range xs:string.
otras:has_cited_book_publisher_country a owl:ObjectProperty;
   rdfs:domain otras:book_citation;
   rdfs:range vico:country.
otras:has_cited_book_edition a owl:DatatypeProperty;
   rdfs:domain otras:book_citation;
   rdfs:range xs:nonNegativeInteger.

otras:book_citation rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:has_cited_book_publisher_name;
      owl:cardinality 1
   ], [
      a owl:Restriction;
      owl:onProperty otras:has_cited_book_publisher_city;
      owl:someValuesFrom xs:string
   ], [
      a owl:Restriction;
      owl:onProperty otras:has_cited_book_publisher_country;
      owl:someValuesFrom vico:country
   ].

otras:journal_article_citation a owl:Class;
   rdfs:subClassOf otras:citation;
   rdfs:comment "journal article citation, i.e., a citation that is about a journal article"^^xs:string.

otras:journal_article_citation owl:intersectionOf (otras:citation [a owl:Restriction; owl:onProperty otras:is_citation_about_document; owl:someValuesFrom otras:journal_article]).

otras:has_publication_journal_identifier a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.
otras:has_publication_journal_name a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_abbreviation a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_issn a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.

otras:has_publication_journal_volume a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_issue a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_pages a owl:DatatypeProperty;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.

otras:journal_article_citation rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:has_publication_journal_identifier;
      owl:someValuesFrom xs:string
   ], [
      a owl:Restriction;
      owl:onProperty otras:has_publication_journal_volume;
      owl:someValuesFrom xs:positiveInteger
   ], [
      a owl:Restriction;
      owl:onProperty otras:has_publication_journal_pages;
      owl:someValuesFrom xs:string
   ].

otras:author_identification_entity a owl:Class;
   rdfs:subClassOf otras:training_entity, evmpo:annotation;
   rdfs:comment "author identification entity, i.e., an entity related to the identification of authors"^^xs:string.

otras:author_identifier a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author identifier, i.e., a descriptor by which an author can be identified"^^xs:string.

otras:has_identified_given_name a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_given_name_initials a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_person a owl:ObjectProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range vico:person.
otras:has_identified_surname a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.

otras:author_identifier rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:has_identified_surname;
      owl:cardinality 1
   ].

otras:author_map a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map, i.e., an indexed collection of author identifiers (n.b., not a collection of the persons themselves, but of identifiers)"^^xs:string.

otras:has_author_map_element a owl:ObjectProperty;
   rdfs:domain otras:author_map;
   rdfs:range otras:author_map_element;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:author_map_element a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map element, i.e., a numbered entry within an indexed collection of author identifiers"^^xs:string.

otras:has_ame_identifier a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range otras:author_identifier;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
otras:has_ame_index a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range xs:nonNegativeInteger.

otras:author_map_element rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:has_ame_identifier;
      owl:cardinality 1
   ], [
      a owl:Restriction;
      owl:onProperty otras:has_ame_index;
      owl:cardinality 1
   ].

otras:instructor a owl:Class;
   rdfs:subClassOf evmpo:expert, vico:training_provider, ccso:Professor;
   rdfs:comment "instructor, i.e., an individual human agent with expertise who can take responsibility for courses, training units, or training services"^^xs:string.
ccso:Professor rdfs:subClassOf vico:agent.

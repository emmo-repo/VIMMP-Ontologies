   owl:onProperty otras:has_operator_level;
   owl:hasValue otras:EXPERT_LEVEL.
otras:expert_operator_specifier rdfs:subClassOf otras:R_HAS_EXPERT_LEVEL, otras:R_HAS_ONE_OPERATOR_CODE.



### ONTOLOGY FOR TRAINING SERVICES (OTRAS) ###

### VERSION 0.2.5 ###

### DATED 25TH APRIL 2019 ###

@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.

@prefix ccso: <https://w3id.org/ccso/ccso#>.
@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix iao: <http://purl.obolibrary.org/obo/iao.owl#>.
@prefix ical: <https://www.w3.org/2002/12/cal/icaltzd#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/otras/otras.ttl#> a owl:Ontology.

otras:training_entity a owl:Class;
   rdfs:comment "training entity, i.e., an entity related to training"^^xs:string;
   rdfs:subClassOf evmpo:marketplace_related_entity.
otras:op_training_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_training_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf otras:op_training_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf otras:dp_training_entity.

otras:constrained_entity a owl:Class;
   rdfs:subClassOf otras:training_entity.

otras:carrier a owl:Class;
   rdfs:comment "carrier, e.g, material, activity, event that carries the contents of a course"^^xs:string;
   rdfs:subClassOf otras:training_entity.
otras:op_carrier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_carrier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_training_entity rdfs:subClassOf otras:op_carrier.
otras:dp_training_entity rdfs:subClassOf otras:dp_carrier.

otras:document a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity, iao:IAO_0000030;  # iao label "information content entity"
   rdfs:comment "document as defined by EVMPO and IAO"^^xs:string;
   owl:equivalentClass evmpo:document, iao:IAO_0000310.  # iao label "document"
otras:op_document a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_document.
otras:dp_document a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_document.
evmpo:op_information_content_entity rdfs:subClassOf otras:op_document.
evmpo:dp_information_content_entity rdfs:subClassOf otras:dp_document.

otras:has_citation_representation a otras:op_document;
   rdfs:domain otras:document;
   rdfs:range otras:citation;
   owl:inverseOf otras:is_citation_about_document.

otras:scientific_publication a owl:Class;
   rdfs:subClassOf otras:document;
   rdfs:comment "scientific publication, i.e., publication about an investigation, as defined by the IAO"^^xs:string;
   owl:equivalentClass iao:IAO_0000312.  # iao label "scientific publication" or equivalently "publication about an investigation"
otras:op_scientific_publication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_scientific_publication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_document rdfs:subClassOf otras:op_scientific_publication.
evmpo:dp_document rdfs:subClassOf otras:dp_scientific_publication.

otras:R_HAS_A_CITATION_REPRESENTATION a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_citation_representation;
   owl:someValuesFrom otras:citation.
otras:scientific_publication rdfs:subClassOf otras:R_HAS_A_CITATION_REPRESENTATION.

otras:journal_article a owl:Class;
   rdfs:subClassOf otras:scientific_publication;
   rdfs:comment "a journal article"^^xs:string.
otras:op_journal_article a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_journal_article a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_scientific_publication rdfs:subClassOf otras:op_journal_article.
evmpo:dp_scientific_publication rdfs:subClassOf otras:dp_journal_article.

otras:training_document a owl:Class;
   rdfs:comment "a training document is a document (as defined by IAO) that is related to training content on a virtual marketplace"^^xs:string;
   rdfs:subClassOf otras:document, otras:carrier;
   rdfs:subClassOf iao:IAO_0000310;  # iao label "document"
   owl:intersectionOf (otras:document otras:carrier).
otras:op_training_document a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_training_document a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_document rdfs:subClassOf otras:op_training_document.
evmpo:dp_document rdfs:subClassOf otras:dp_training_document.
otras:op_carrier rdfs:subClassOf otras:op_training_document.
otras:dp_carrier rdfs:subClassOf otras:dp_training_document.

otras:training_event a owl:Class;
   rdfs:comment "a training event is a (potential) virtual marketplace calendar event at which training activities occur, equivalent to Event from CCSO"^^xs:string;
   rdfs:subClassOf evmpo:calendar_event, ical:Vevent, otras:carrier;
   owl:equivalentClass ccso:Event;
   owl:intersectionOf (evmpo:calendar_event otras:carrier).
otras:op_training_event a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_training_event a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_calendar_event rdfs:subClassOf otras:op_training_event.
evmpo:dp_calendar_event rdfs:subClassOf otras:dp_training_event.
otras:op_carrier rdfs:subClassOf otras:op_training_event.
otras:dp_carrier rdfs:subClassOf otras:dp_training_event.

otras:tradeable_object a owl:Class;
   rdfs:comment "a tradeable object is a service that is offered, or can be offered, on a virtual marketplace"^^xs:string;
   owl:equivalentClass evmpo:tradeable_object;
   rdfs:subClassOf evmpo:service.
otras:op_tradeable_object a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_tradeable_object.
otras:dp_tradeable_object a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_tradeable_object.
evmpo:op_service rdfs:subClassOf otras:op_tradeable_object.
evmpo:dp_service rdfs:subClassOf otras:dp_tradeable_object.

otras:training_service a owl:Class;
   rdfs:comment "a training service is a tradeable object that has a content related to training activities on a virtual marketplace"^^xs:string;
   rdfs:subClassOf otras:tradeable_object.
otras:op_training_service a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_training_service a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_tradeable_object rdfs:subClassOf otras:op_training_service.
otras:dp_tradeable_object rdfs:subClassOf otras:dp_training_service.

otras:has_offered_course a otras:op_training_service;
   rdfs:domain otras:training_service;
   rdfs:range otras:course;
   rdfs:subPropertyOf evmpo:has_product_annotation.

otras:focus a owl:Class;
   rdfs:comment "focus, i.e., a studied object or topic or training objective (or an aspect or constitutive part thereof)";
   rdfs:subClassOf otras:training_entity, evmpo:annotation.
otras:op_focus a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_focus a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_training_entity rdfs:subClassOf otras:op_focus.
otras:dp_training_entity rdfs:subClassOf otras:dp_focus.
evmpo:op_annotation rdfs:subClassOf otras:op_focus.
evmpo:dp_annotation rdfs:subClassOf otras:dp_focus.

otras:has_focus_descriptor a otras:dp_focus;
   rdfs:domain otras:focus;
   rdfs:range rdfs:Literal.

otras:topic a owl:Class;
   rdfs:comment "a topic as defined by CCSO"^^xs:string;
   rdfs:subClassOf otras:focus;
   owl:equivalentClass ccso:Topic.
otras:op_topic a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_topic a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_focus rdfs:subClassOf otras:op_topic.
otras:dp_focus rdfs:subClassOf otras:dp_topic.

otras:has_topic_descriptor a otras:dp_topic;
   rdfs:domain otras:topic;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_focus_descriptor.

otras:specifier a owl:Class;
   rdfs:comment "specifier, i.e., a constitutive element of a learning outcome (acquired competency) description"^^xs:string;
   rdfs:subClassOf otras:focus, otras:HAS_A_SPECIFIER_DESCRIPTOR.
otras:op_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_focus rdfs:subClassOf otras:op_specifier.
otras:dp_focus rdfs:subClassOf otras:dp_specifier.

otras:has_specifier_descriptor a otras:dp_specifier;
   rdfs:domain otras:specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_focus_descriptor.
otras:HAS_A_SPECIFIER_DESCRIPTOR a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_specifier_descriptor;
   owl:someValuesFrom rdfs:Literal.

otras:operator_specifier a owl:Class;
   rdfs:comment "operator specifier, i.e., the type of activity facilitated by an acquisition of the specified competency"^^xs:string;
   rdfs:subClassOf otras:specifier.
otras:op_operator_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_operator_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_specifier rdfs:subClassOf otras:op_operator_specifier.
otras:dp_specifier rdfs:subClassOf otras:dp_operator_specifier.

otras:is_expressed_by a otras:dp_operator_specifier;
   rdfs:domain otras:operator_specifier;
   rdfs:range rdfs:Literal.
otras:R_IS_EXPRESSED a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_expressed_by;
   owl:someValuesFrom rdfs:Literal.
otras:operator_specifier rdfs:subClassOf otras:R_IS_EXPRESSED.
otras:is_defined_by a otras:dp_specifier;
   rdfs:domain otras:operator_specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_specifier_descriptor.

otras:has_operator_descriptor a otras:dp_operator_specifier;
   rdfs:domain otras:operator_specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_specifier_descriptor;
   owl:equivalentProperty otras:is_expressed_by.

otras:has_kmk_level a otras:op_operator_specifier;
   rdfs:domain otras:operator_specifier;
   rdfs:range otras:kmk_operator_level;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:operand_specifier a owl:Class;
   rdfs:comment "operand specifier, i.e., an entity acted upon by an operator specifier"^^xs:string;
   rdfs:subClassOf otras:specifier.
otras:op_operand_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_operand_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_specifier rdfs:subClassOf otras:op_operand_specifier.
otras:dp_specifier rdfs:subClassOf otras:dp_operand_specifier.

otras:has_operand_descriptor a otras:dp_operand_specifier;
   rdfs:domain otras:operand_specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_specifier_descriptor.
otras:has_operand_topic a otras:op_operand_specifier;
   rdfs:domain otras:operand_specifier;
   rdfs:range otras:topic;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:implementation_specifier a owl:Class;
   rdfs:comment "implementation specifier, i.e., a process or procedural aspect of the specified competency"^^xs:string;
   rdfs:subClassOf otras:specifier.
otras:op_implementation_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_implementation_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_specifier rdfs:subClassOf otras:op_implementation_specifier.
otras:dp_specifier rdfs:subClassOf otras:dp_implementation_specifier.

otras:has_implementation_descriptor a otras:dp_implementation_specifier;
   rdfs:domain otras:implementation_specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_specifier_descriptor.

otras:example_specifier a owl:Class;
   rdfs:comment "example specifier, i.e., a concrete example of how the competency may be exercised in practice"^^xs:string;
   rdfs:subClassOf otras:specifier.
otras:op_example_specifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_example_specifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_specifier rdfs:subClassOf otras:op_example_specifier.
otras:dp_specifier rdfs:subClassOf otras:dp_example_specifier.

otras:has_example_descriptor a otras:dp_example_specifier;
   rdfs:domain otras:example_specifier;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_specifier_descriptor.
otras:has_example_topic a otras:op_example_specifier;
   rdfs:domain otras:example_specifier;
   rdfs:range otras:topic;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:learning_outcome a owl:Class;
   rdfs:comment "a learning outcome, equivalent to LearningOutcome from CCSO, further characterized by OTRAS"^^xs:string;
   rdfs:subClassOf otras:focus;
   owl:equivalentClass ccso:LearningOutcome.
otras:op_learning_outcome a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_learning_outcome a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_focus rdfs:subClassOf otras:op_learning_outcome.
otras:dp_focus rdfs:subClassOf otras:dp_learning_outcome.

otras:has_l_o_descriptor a otras:dp_specifier;
   rdfs:domain otras:learning_outcome;
   rdfs:range rdfs:Literal;
   rdfs:subPropertyOf otras:has_focus_descriptor.

otras:has_specifier a otras:op_learning_outcome;
   rdfs:domain otras:learning_outcome;
   rdfs:range otras:specifier;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
otras:has_operator_specifier a otras:op_learning_outcome;
   rdfs:domain otras:learning_outcome;
   rdfs:range otras:operator_specifier;
   rdfs:subPropertyOf otras:has_specifier.
otras:R_HAS_AN_OPERATOR_SPECIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_operator_specifier;
   owl:someValuesFrom otras:operator_specifier.
otras:learning_outcome rdfs:subClassOf otras:R_HAS_AN_OPERATOR_SPECIFIER.
otras:has_operand_specifier a otras:op_learning_outcome;
   rdfs:domain otras:learning_outcome;
   rdfs:range otras:operand_specifier;
   rdfs:subPropertyOf otras:has_specifier.
otras:R_HAS_AN_OPERAND_SPECIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_operand_specifier;
   owl:someValuesFrom otras:operand_specifier.
otras:learning_outcome rdfs:subClassOf otras:R_HAS_AN_OPERAND_SPECIFIER.
otras:has_implementation_specifier a otras:op_learning_outcome;
   rdfs:domain otras:learning_outcome;
   rdfs:range otras:implementation_specifier;
   rdfs:subPropertyOf otras:has_specifier.
otras:has_example_specifier a otras:op_learning_outcome;
   rdfs:domain otras:learning_outcome;
   rdfs:range otras:example_specifier;
   rdfs:subPropertyOf otras:has_specifier.

otras:syllabus a owl:Class;
   rdfs:comment "a syllabus as defined by CCSO"^^xs:string;
   rdfs:subClassOf evmpo:document;
   owl:equivalentClass ccso:Syllabus.
otras:op_syllabus a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_syllabus a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_document rdfs:subClassOf otras:op_syllabus.
evmpo:dp_document rdfs:subClassOf otras:dp_syllabus.

otras:aims_to a otras:op_syllabus;
   rdfs:domain otras:syllabus;
   rdfs:range otras:learning_outcome;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation;
   owl:equivalentProperty ccso:aimsToLO.
otras:covers_topic a otras:op_syllabus;
   rdfs:domain otras:syllabus;
   rdfs:range otras:topic;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation;
   owl:equivalentProperty ccso:coversTopic.
otras:has_syllabus_resource a otras:op_syllabus;
   rdfs:domain otras:syllabus;
   rdfs:range rdfs:Resource.

otras:course a owl:Class;
   rdfs:comment "a course as defined by CCSO"^^xs:string;
   rdfs:subClassOf otras:training_entity, evmpo:annotation;
   owl:equivalentClass ccso:Course.
otras:op_course a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_course a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_training_entity rdfs:subClassOf otras:op_course.
otras:dp_training_entity rdfs:subClassOf otras:dp_course.
evmpo:op_annotation rdfs:subClassOf otras:op_course.
evmpo:dp_annotation rdfs:subClassOf otras:dp_course.

otras:has_carrier a otras:op_course;
   rdfs:domain otras:course;
   rdfs:range otras:carrier.
otras:has_document a otras:op_course;
   rdfs:domain otras:course;
   rdfs:range otras:training_document;
   rdfs:subPropertyOf otras:has_carrier.
otras:has_event a otras:op_course;
   rdfs:domain otras:course;
   rdfs:range otras:training_event;
   rdfs:subPropertyOf otras:has_carrier.
otras:has_syllabus a otras:op_course, owl:FunctionalProperty;
   rdfs:domain otras:course;
   rdfs:range otras:syllabus;
   owl:equivalentProperty ccso:hasSyllabus.
otras:is_offered_by a otras:op_course;
   rdfs:domain otras:course;
   rdfs:range otras:training_service;
   owl:inverseOf otras:has_offered_course.

otras:training_unit a owl:Class;
   rdfs:comment "a training unit is an elementary (part of a) course that is not further subdivided into (proper) parts"^^xs:string;
   rdfs:subClassOf otras:course.
otras:op_training_unit a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_training_unit a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_course rdfs:subClassOf otras:op_training_unit.
otras:dp_course rdfs:subClassOf otras:dp_training_unit.

otras:is_part_of_course a otras:op_training_unit, owl:FunctionalProperty;
   rdfs:domain otras:training_unit;
   rdfs:range otras:course.
otras:has_training_unit a otras:op_course;
   rdfs:domain otras:course;
   rdfs:range otras:training_unit;
   rdfs:subPropertyOf evmpo:has_meta_annotation;
   owl:inverseOf otras:is_part_of_course.
otras:R_HAS_NO_TRAINING_UNIT a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_training_unit;
   owl:cardinality 0.
otras:training_unit owl:intersectionOf (otras:course otras:R_HAS_NO_TRAINING_UNIT).

otras:mm_topic a owl:Class;
   rdfs:comment "materials modelling topic, i.e., a topic related to the subject area of materials modelling, understood broadly"^^xs:string;
   rdfs:subClassOf otras:topic.
otras:op_mm_topic a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_mm_topic a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_topic rdfs:subClassOf otras:op_mm_topic.
evmpo:dp_topic rdfs:subClassOf otras:dp_mm_topic.

otras:has_topic_code a otras:dp_mm_topic;
   rdfs:domain otras:mm_topic;
   rdfs:range xs:nonNegativeInteger.
otras:R_HAS_EXACTLY_ONE_TOPIC_CODE a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_topic_code;
   owl:cardinality 1.
otras:mm_topic rdfs:subClassOf otras:R_HAS_EXACTLY_ONE_TOPIC_CODE.
otras:R_HAS_A_TOPIC_DESCRIPTOR a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_topic_descriptor;
   owl:someValuesFrom rdfs:Literal.
otras:mm_topic rdfs:subClassOf otras:R_HAS_A_TOPIC_DESCRIPTOR.

otras:mm_topic_basic a owl:Class;
   rdfs:comment "basic prerequisites for materials modelling studies (codes 1XXX and 2XXX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic.
otras:mm_topic_basic_chemical a owl:Class;
   rdfs:comment "basic prerequisites in chemical and process engineering, biotechnology, petrochemistry, and organic chemistry (code 11XX or 21XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_economics a owl:Class;
   rdfs:comment "basic prerequisites in economics, marketing, and business administration (code 12XX or 22XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_information a owl:Class;
   rdfs:comment "basic prerequisites in information technology, computer science, and progamming (code 13XX or 23XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_materials a owl:Class;
   rdfs:comment "basic prerequisites in materials science and general or inorganic chemistry (code 14XX or 24XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_mathematics a owl:Class;
   rdfs:comment "basic prerequisites in mathematics (code 15XX or 25XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_mechanical a owl:Class;
   rdfs:comment "basic prerequisites in mechanical, aerospace, civil, electrical, and general engineering (code 16XX or 26XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_physics a owl:Class;
   rdfs:comment "basic prerequisites in physics (code 17XX or 27XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_interdisciplinary a owl:Class;
   rdfs:comment "basic prerequisites that are best described as equally belonging to multiple fields (code 18XX or 28XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.
otras:mm_topic_basic_secondary a owl:Class;
   rdfs:comment "secondary or pre-undergraduate level education topics and general areas of basic knowledge (code 19XX or 29XX)"^^xs:string;
   rdfs:subClassOf otras:mm_topic_basic.

otras:mm_topic_computational a owl:Class;
   rdfs:comment "a topic related to computational and numerical aspects of materials modelling (code 3XXX if unique, 8XXX if interdisciplinary)"^^xs:string;
   rdfs:subClassOf otras:mm_topic.

otras:mm_topic_data a owl:Class;
   rdfs:comment "a topic related to data science and technology aspects of materials modelling (code 4XXX if unique, 8XXX if interdisciplinary)"^^xs:string;
   rdfs:subClassOf otras:mm_topic.

otras:mm_topic_materials a owl:Class;
   rdfs:comment "a topic related to materials, including but not limited to fluids, and their properties (code 5XXX if unique, 8XXX if interdisciplinary)"^^xs:string;

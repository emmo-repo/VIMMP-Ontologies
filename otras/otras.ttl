   otras:is_defined_by "an unspecified learning operator at intermediate level, as categorized by the KMK document"^^xs:string.

otras:KMK_OPERATOR_215 a otras:kmk_operator_specifier;
   otras:has_operator_code 215;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "compare"^^xs:string;
   otras:is_defined_by "give a criteria-based account of similarities and differences between phenomena, objects, or processes"^^xs:string.

otras:KMK_OPERATOR_220 a otras:kmk_operator_specifier;
   otras:has_operator_code 220;
   otras:has_kmk_level otras:KMK_LEVEL_II;
   otras:is_expressed_by "deduce"^^xs:string;
   otras:is_defined_by "reach a conclusion from the information given"^^xs:string.

otras:KMK_OPERATOR_225 a otras:kmk_operator_specifier;

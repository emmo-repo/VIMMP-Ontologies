   rdfs:comment "the two titles refer to the same degree, only that this one is a longer written form of it than the other"^^xs:string;
   rdfs:subPropertyOf vico:corresponds_to;
   owl:inverseOf vico:is_shortened_form_of.
vico:is_gendered_form_of a owl:ObjectProperty, owl:TransitiveProperty, owl:SymmetricProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "this relation applies if the difference between the two given titles essentially reduces to gender (n.b., this also holds if one of the two forms does not denote gender explicitly)"^^xs:string;
   rdfs:subPropertyOf vico:corresponds_to.
vico:is_shortened_form_of a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "the two titles refer to the same degree, only that this one is a shorter written form of it than the other"^^xs:string;
   rdfs:subPropertyOf vico:corresponds_to;
   owl:inverseOf vico:is_extended_form_of.
vico:is_translated_form_of a owl:ObjectProperty, owl:TransitiveProperty, owl:SymmetricProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "this relation applies if the difference between the two given titles essentially reduces to a choice of language or country"^^xs:string;
   rdfs:subPropertyOf vico:corresponds_to.

vico:has_prefix_element a owl:DatatypeProperty;
   rdfs:subPropertyOf vico:dp_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range xs:boolean;
   rdfs:comment "is true if there is a part of the title that goes before the name, such as Dr."^^xs:string.
vico:has_prefix_text a owl:DatatypeProperty;
   rdfs:subPropertyOf vico:dp_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range xs:string;
   rdfs:comment "title (or part of it) that goes before the name"^^xs:string.
vico:has_suffix_element a owl:DatatypeProperty;
   rdfs:subPropertyOf vico:dp_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range xs:boolean;
   rdfs:comment "is true if there is a part of the title that goes after the name, such as jr."^^xs:string.
vico:has_suffix_text a owl:DatatypeProperty;
   rdfs:subPropertyOf vico:dp_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range xs:string;
   rdfs:comment "title (or part of it) that goes after the name"^^xs:string.
vico:is_local_to a owl:ObjectProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:country;
   rdfs:comment "points to a country where this title is recognized and used in the given form"^^xs:string.
vico:prefix_is_prior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "relation that determines the order in which title prefixes are written down"^^xs:string;
   owl:inverseOf vico:prefix_is_posterior_to.
vico:prefix_is_posterior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "relation that determines the order in which title prefixes are written down"^^xs:string;
   owl:inverseOf vico:prefix_is_prior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
vico:suffix_is_prior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "relation that determines the order in which title suffixes are written down"^^xs:string;
   owl:inverseOf vico:suffix_is_posterior_to.
vico:suffix_is_posterior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_personal_title;
   rdfs:domain vico:personal_title;
   rdfs:range vico:personal_title;
   rdfs:comment "relation that determines the order in which title suffixes are written down"^^xs:string;
   owl:inverseOf vico:suffix_is_prior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

vico:titular_rank a owl:Class;
   rdfs:subClassOf vico:personal_title;
   rdfs:comment "titular rank, i.e., a personal title that is part of a hierarchy of titles"^^xs:string.
vico:op_titular_rank a owl:ObjectProperty.
vico:dp_titular_rank a owl:DatatypeProperty.
vico:op_personal_title rdfs:subPropertyOf vico:op_titular_rank.
vico:dp_personal_title rdfs:subPropertyOf vico:dp_titular_rank.

vico:is_inferior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_titular_rank;
   rdfs:domain vico:titular_rank;
   rdfs:range vico:titular_rank;
   rdfs:comment "relation corresponding to the hierarchical order of titles"^^xs:string;
   owl:inverseOf vico:is_superior_to.
vico:is_superior_to a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:subPropertyOf vico:op_titular_rank;
   rdfs:domain vico:titular_rank;
   rdfs:range vico:titular_rank;
   rdfs:comment "relation corresponding to the hierarchical order of titles"^^xs:string;
   owl:inverseOf vico:is_inferior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

vico:academic_title a owl:Class;
   rdfs:subClassOf vico:titular_rank;
   rdfs:comment "academic title, i.e., a titular rank that signifies possession of an academic degree"^^xs:string.
vico:op_academic_title a owl:ObjectProperty.
vico:dp_academic_title a owl:DatatypeProperty.
vico:op_titular_rank rdfs:subPropertyOf vico:op_academic_title.
vico:dp_titular_rank rdfs:subPropertyOf vico:dp_academic_title.

vico:academic_title owl:disjointUnionOf (vico:nondoctoral_title vico:doctoral_title vico:postdoctoral_title).

vico:postdoctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "postdoctoral title, i.e., an academic title that is usually only conferred on people with a doctorate"^^xs:string.
vico:op_postdoctoral_title a owl:ObjectProperty.
vico:dp_postdoctoral_title a owl:DatatypeProperty.
vico:op_academic_title rdfs:subPropertyOf vico:op_postdoctoral_title.
vico:dp_academic_title rdfs:subPropertyOf vico:dp_postdoctoral_title.

vico:doctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "doctoral title, i.e., an academic title that signifies the possession of a doctorate degree"^^xs:string.
vico:op_doctoral_title a owl:ObjectProperty.
vico:dp_doctoral_title a owl:DatatypeProperty.
vico:op_academic_title rdfs:subPropertyOf vico:op_doctoral_title.
vico:dp_academic_title rdfs:subPropertyOf vico:dp_doctoral_title.

vico:nondoctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "nondoctoral title, i.e., an academic title which does not confer a doctorate and also usually does not require a doctorate"^^xs:string.
vico:op_nondoctoral_title a owl:ObjectProperty.
vico:dp_nondoctoral_title a owl:DatatypeProperty.
vico:op_academic_title rdfs:subPropertyOf vico:op_nondoctoral_title.
vico:dp_academic_title rdfs:subPropertyOf vico:dp_nondoctoral_title.

vico:TITLE_FEMALE_GENERIC a vico:personal_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Ms"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_gendered_form_of vico:TITLE_MALE_GENERIC.
vico:TITLE_MALE_GENERIC a vico:personal_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Mr"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_gendered_form_of vico:TITLE_FEMALE_GENERIC.

vico:TITLE_DOCTOR_GENERIC a vico:doctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Dr"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean.

vico:TITLE_PROFESSOR_GENERIC_LONG a vico:postdoctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Professor"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_extended_form_of vico:TITLE_PROFESSOR_GENERIC_SHORT.
vico:TITLE_PROFESSOR_GENERIC_SHORT a vico:postdoctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Prof"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_shortened_form_of vico:TITLE_PROFESSOR_GENERIC_LONG.

_:C_PERSONAL_TITLE a owl:AllDifferent;
   owl:members (vico:TITLE_FEMALE_GENERIC vico:TITLE_MALE_GENERIC vico:TITLE_DOCTOR_GENERIC vico:TITLE_PROFESSOR_GENERIC_LONG vico:TITLE_PROFESSOR_GENERIC_SHORT).

# "no nondoctoral title is superior to any postdoctoral title"
#
vico:is_superior_to_any a owl:ObjectProperty;
   rdfs:subPropertyOf vico:op_titular_rank;
   rdfs:domain vico:titular_rank;
   rdfs:range owl:Class;
   owl:propertyChainAxiom (vico:is_superior_to rdf:type).
vico:nondoctoral_title owl:disjointWith [
   a owl:Restriction;
   owl:onProperty vico:is_superior_to_any;
   owl:hasValue vico:postdoctoral_title
].


### VIMMP COMMUNICATION ONTOLOGY (VICO) ###

### VERSION 1.0.0 ###

### DATED 25TH NOVEMBER 2019 ###

@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.

@prefix dcterms: <http://purl.org/dc/terms#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix lcc-cr: <https://www.omg.org/spec/LCC/Countries/CountryRepresentation/>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/vico/vico.ttl#> a owl:Ontology.

vico:contains a evmpo:op_communication, owl:TransitiveProperty;
   rdfs:domain evmpo:communication;
   rdfs:range evmpo:communication;
   rdfs:comment "The expression [A contains B] means that B is a proper part of A, where A and B are both communications"^^xs:string.
vico:has_access_rule a evmpo:op_communication, owl:FunctionalProperty;
   rdfs:domain evmpo:communication;
   rdfs:range vico:access_rule;
   rdfs:subPropertyOf evmpo:has_communication_annotation;
   rdfs:comment "regulates (read) access to the communicated content"^^xs:string.
vico:has_author a evmpo:op_communication, owl:FunctionalProperty;
   rdfs:domain evmpo:communication;
   rdfs:range vico:agent;
   owl:inverseOf vico:is_author_of.
vico:has_expiry_date a vico:op_interlocution, owl:FunctionalProperty;
   rdfs:domain evmpo:communication;
   rdfs:range dcterms:W3C-DTF.
vico:has_file_content_locator a evmpo:dp_communication;
   rdfs:domain evmpo:communication;
   rdfs:range xs:string;
   rdfs:comment "this may be a URL or a path where the file can be accessed"^^xs:string.
vico:has_file_content_format a evmpo:dp_communication;
   rdfs:domain evmpo:communication;
   rdfs:range macro:file_format.
vico:has_label a evmpo:op_communication;
   rdfs:domain evmpo:communication;
   rdfs:range vico:label;
   rdfs:subPropertyOf evmpo:has_communication_annotation.
vico:has_status a evmpo:op_communication;
   rdfs:domain evmpo:communication;
   rdfs:range vico:status;
   rdfs:subPropertyOf vico:has_label.
vico:has_submission_date a evmpo:op_communication, owl:FunctionalProperty;
   rdfs:domain evmpo:communication;
   rdfs:range dcterms:W3C-DTF.
vico:has_text_content a evmpo:dp_communication;
   rdfs:domain evmpo:communication;
   rdfs:range xs:string.
vico:has_title a evmpo:dp_communication;
   rdfs:domain evmpo:communication;
   rdfs:range xs:string.

vico:is_on_channel a evmpo:op_communication;
   rdfs:domain evmpo:communication;
   rdfs:range macro:channel.
vico:is_on_marketplace a evmpo:op_communication;
   rdfs:domain evmpo:communication;
   rdfs:range macro:virtual_marketplace;
   rdfs:subPropertyOf vico:is_on_channel.

evmpo:communication
   owl:disjointUnionOf (vico:message vico:appendix);
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vico:has_author;
      owl:someValuesFrom vico:agent
   ].

evmpo:statement owl:intersectionOf (evmpo:communication [a owl:Restriction; owl:onProperty vico:contains; owl:cardinality 0]).

vico:appendix a owl:Class;
   rdfs:subClassOf evmpo:statement;
   rdfs:comment "appendix, i.e., a statement that cannot stand alone and needs to be combined with a communication body"^^xs:string.
vico:op_appendix a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_appendix a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_statement rdfs:subClassOf vico:op_appendix.
evmpo:dp_statement rdfs:subClassOf vico:dp_appendix.

vico:validation_communication a owl:Class;
   rdfs:subClassOf evmpo:communication, vivo:validation_entity;
   owl:intersectionOf (evmpo:communication vivo:validation_entity);
   rdfs:comment "validation communication, i.e., a communication which, as its main feature or purpose, relates to validation"^^xs:string.
vico:op_validation_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_translation_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf vico:op_validation_communication.
evmpo:dp_communication rdfs:subClassOf vico:dp_validation_communication.
vivo:op_validation_entity rdfs:subClassOf vico:op_validation_communication.
vivo:dp_validation_entity rdfs:subClassOf vico:dp_validation_communication.

vico:access_rule a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "access rule, i.e., a communication annotation that specifies who is allowed to read the communicated content"^^xs:string.
vico:op_access_rule a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_access_rule a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vico:op_access_rule.
evmpo:dp_annotation rdfs:subClassOf vico:dp_access_rule.

vico:allows a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range vico:interlocutor;
   rdfs:comment "[A allows B] means that B, who is an interlocutor, must be granted access"^^xs:string.
vico:excludes a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range vico:interlocutor;
   rdfs:comment "[A excludes B] means that B, who is an interlocutor, must be denied access"^^xs:string.
vico:allows_all a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range vico:interlocutor_group;
   rdfs:comment "[A allows_all C] means that all interlocutors from group C must be granted access"^^xs:string.
vico:excludes_all a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range vico:interlocutor_group;
   rdfs:comment "[A excludes_all C] means that all interlocutors from group C must be denied access"^^xs:string.

vico:allows owl:propertyDisjointWith vico:excludes.
vico:allows_all owl:propertyDisjointWith vico:excludes_all.

vico:label a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "label, i.e., a remark or information item that can be carried by (i.e., associated with) a communication"^^xs:string.
vico:op_label a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_label a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vico:op_label.
evmpo:dp_annotation rdfs:subClassOf vico:dp_label.

vico:status a owl:Class;
   rdfs:subClassOf vico:label;
   rdfs:comment "status, i.e., a label that expresses information on the submission, reading, or exchange process of a communication"^^xs:string.
vico:op_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_label rdfs:subClassOf vico:op_status.
evmpo:dp_label rdfs:subClassOf vico:dp_status.

vico:has_status_timestamp a vico:op_status, owl:FunctionalProperty;
   rdfs:domain vico:status;
   rdfs:range dcterms:W3C-DTF.

vico:processed_status a owl:Class;
   rdfs:subClassOf vico:status;
   rdfs:comment "processed status, i.e., a status expressing to what extent a communication and/or its contents or associated tasks have been processed or carried out"^^xs:string.
vico:op_processed_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_processed_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_status rdfs:subClassOf vico:op_processed_status.
evmpo:dp_status rdfs:subClassOf vico:dp_processed_status.

vico:has_fractional_progress a vico:dp_processed_status;
   rdfs:domain vico:processed_status;
   rdfs:range xs:decimal;
   rdfs:comment "fractional extent to which the communication has been processed; minimum: 0 for zero progress, maximum: 1 for completion"^^xs:string.

vico:read_status a owl:Class;
   rdfs:subClassOf vico:status;
   rdfs:comment "read status, i.e., a status expressing that a communication has been read"^^xs:string.
vico:op_read_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_read_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_status rdfs:subClassOf vico:op_read_status.
evmpo:dp_status rdfs:subClassOf vico:dp_read_status.

vico:sent_status a owl:Class;
   rdfs:subClassOf vico:status;
   rdfs:comment "sent status, i.e., a status expressing that a communication has been sent"^^xs:string.
vico:op_sent_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_sent_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_status rdfs:subClassOf vico:op_sent_status.
evmpo:dp_status rdfs:subClassOf vico:dp_sent_status.

vico:message a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "message, i.e., a stand-alone communication (as opposed to an appendix)"^^xs:string.
vico:op_message a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_message a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf vico:op_message.
evmpo:dp_communication rdfs:subClassOf vico:dp_message.

vico:has_appendix a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:appendix;
   rdfs:subPropertyOf vico:contains.
vico:has_assignee a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:agent;
   owl:inverseOf vico:is_assignee_of.
vico:has_certificate a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vivo:certificate;
   rdfs:subPropertyOf vico:has_appendix.
vico:has_comment a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:comment;
   rdfs:subPropertyOf vico:is_followed_by;
   owl:inverseOf vico:has_commented_message.
vico:follows a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:message.
vico:has_listener a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:agent;
   owl:inverseOf vico:is_listener_of.
vico:has_signal a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:signal_statement;
   rdfs:subPropertyOf vico:is_followed_by.
vico:is_followed_by a vico:op_message;
   rdfs:domain vico:message;
   rdfs:range vico:message;
   owl:inverseOf vico:follows.

vico:message rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vico:has_access_rule;
      owl:cardinality 1
   ], [
      a owl:Restriction;
      owl:onProperty vico:is_on_marketplace;
      owl:someValuesFrom macro:virtual_marketplace
   ].
vico:message owl:disjointUnionOf (vico:declaration vico:interlocution).

vico:task a owl:Class;
   rdfs:subClassOf vico:message;
   rdfs:comment "task, i.e., a message that has an assignee"^^xs:string.
vico:op_task a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_task a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_message rdfs:subClassOf vico:op_task.
vico:dp_message rdfs:subClassOf vico:dp_task.

vico:task owl:intersectionOf (vico:message [a owl:Restriction; owl:onProperty vico:has_assignee; owl:someValuesFrom vico:agent]).

vico:declaration a owl:Class;
   rdfs:subClassOf vico:message;
   rdfs:comment "declaration, i.e., a message without recipient"^^xs:string;
   owl:equivalentClass evmpo:declaration.
vico:op_declaration a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_declaration.
vico:dp_declaration a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_declaration.
vico:op_message rdfs:subClassOf vico:op_declaration.
vico:dp_message rdfs:subClassOf vico:dp_declaration.

vico:declaration owl:intersectionOf (vico:message [a owl:Restriction; owl:onProperty vico:has_recipient; owl:cardinality 0]).

vico:interlocution a owl:Class;
   rdfs:subClassOf vico:message;
   rdfs:comment "interlocution, i.e., a message that has at least one recipient"^^xs:string;
   owl:equivalentClass evmpo:interlocution.
vico:op_interlocution a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_interlocution a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_message rdfs:subClassOf vico:op_interlocution.
vico:dp_message rdfs:subClassOf vico:dp_interlocution.

vico:has_recipient a vico:op_interlocution;
   rdfs:domain vico:interlocution;
   rdfs:range vico:agent;
   owl:inverseOf vico:is_recipient_of.

vico:interlocution owl:intersectionOf (vico:message [a owl:Restriction; owl:onProperty vico:has_recipient; owl:someValuesFrom vico:agent]).

vico:collocution a owl:Class;
   rdfs:subClassOf vico:interlocution;
   rdfs:comment "collocution, i.e., an interlocution which, as its main feature or purpose, comments or adds material or data to another message"^^xs:string.
vico:op_collocution a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_collocution a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_interlocution rdfs:subClassOf vico:op_collocution.
vico:dp_interlocution rdfs:subClassOf vico:dp_collocution.

vico:collocution rdfs:subClassOf [a owl:Restriction; owl:onProperty vico:follows; owl:someValuesFrom vico:message].

vico:comment a owl:Class;
   rdfs:subClassOf vico:collocution;
   rdfs:comment "comment, i.e., a collocution that serves the purpose of commenting on another message"^^xs:string.
vico:op_comment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_comment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_collocution rdfs:subClassOf vico:op_comment.
vico:dp_collocution rdfs:subClassOf vico:dp_comment.

vico:has_commented_message a vico:op_comment;
   rdfs:domain vico:comment;
   rdfs:range vico:message;
   rdfs:subPropertyOf vico:follows;
   owl:inverseOf vico:has_comment.

vico:comment rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vico:has_commented_message;
      owl:someValuesFrom vico:message
   ], [
      a owl:Restriction;
      owl:onProperty vico:has_text;
      owl:someValuesFrom xs:string
   ].

vico:signal_statement a owl:Class;
   rdfs:subClassOf vico:collocution, evmpo:statement;
   rdfs:comment "signal statement, i.e., a standardized shorthand reaction to another message"^^xs:string.
vico:op_signal_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_signal_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_collocution rdfs:subClassOf vico:op_signal_statement.
vico:dp_collocution rdfs:subClassOf vico:dp_signal_statement.
evmpo:op_statement rdfs:subClassOf vico:op_signal_statement.
evmpo:dp_statement rdfs:subClassOf vico:dp_signal_statement.

vico:signal_statement rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vico:follows;
      owl:cardinality 1
   ].

vico:signal_i_like a owl:Class;
   rdfs:subClassOf vico:signal_statement;
   rdfs:comment "signal i like, communicating that the author approves of the followed message"^^xs:string.

vico:translation_communication a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "translation communication, i.e., a communication which, as its main feature or purpose, relates to translation"^^xs:string.
vico:op_translation_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_translation_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf vico:op_translation_communication.
evmpo:dp_communication rdfs:subClassOf vico:dp_translation_communication.

vico:legal_communication a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "legal communication, i.e., a communication which, as its main feature or purpose, relates to legal, financial, or contractual issues"^^xs:string.
vico:op_legal_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_legal_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf vico:op_legal_communication.
evmpo:dp_communication rdfs:subClassOf vico:dp_legal_communication.

vico:legal_statement a owl:Class;
   rdfs:subClassOf evmpo:statement, vico:legal_communication;
   owl:intersectionOf (vico:legal_communication evmpo:statement);
   rdfs:comment "legal statement, i.e., a statement which, as its main feature or purpose, relates to legal, financial, or contractual issues"^^xs:string.
vico:op_legal_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_legal_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_legal_communication rdfs:subClassOf vico:op_legal_statement.
vico:dp_legal_communication rdfs:subClassOf vico:dp_legal_statement.
evmpo:op_statement rdfs:subClassOf vico:op_legal_statement.
evmpo:dp_statement rdfs:subClassOf vico:dp_legal_statement.


vico:address_entity a owl:Class;
   rdfs:subClassOf evmpo:annotation.
vico:op_address_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_address_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vico:op_address_entity.
evmpo:dp_annotation rdfs:subClassOf vico:dp_address_entity.

vico:has_address_content a vico:op_address_entity;
   rdfs:domain vico:address_entity, [owl:complementOf vico:address_token];
   rdfs:range vico:address_content;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

vico:address a owl:Class;
   rdfs:subClassOf vico:address_entity.
vico:op_address a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_address a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address_entity rdfs:subClassOf vico:op_address.
vico:dp_address_entity rdfs:subClassOf vico:dp_address.

vico:has_address_qualifier a vico:op_postal_address;
   rdfs:domain vico:address;
   rdfs:range vico:address_content;
   rdfs:comment "content that needs to be added to an address, e.g., additional address lines"^^xs:string;
   rdfs:subPropertyOf vico:has_address_content.
vico:is_in_country a vico:op_address;
   rdfs:domain vico:address;
   rdfs:range vico:country.

vico:address_content a owl:Class;
   rdfs:subClassOf vico:address_entity.
vico:op_address_content a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_address_content a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address_entity rdfs:subClassOf vico:op_address_content.
vico:dp_address_entity rdfs:subClassOf vico:dp_address_content.

vico:address_token a owl:Class;
   rdfs:subClassOf vico:address_content.
vico:op_address_token a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_address_token a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address_content rdfs:subClassOf vico:op_address_token.
vico:dp_address_content rdfs:subClassOf vico:dp_address_token.

vico:has_number_representation a vico:dp_address_token;
   rdfs:domain vico:address_token;
   rdfs:range xs:nonNegativeInteger.
vico:has_textual_representation a vico:dp_address_token;
   rdfs:domain vico:address_token;
   rdfs:range xs:string.
vico:has_token_index a vico:dp_address_token;
   rdfs:domain vico:address_token;
   rdfs:range xs:nonNegativeInteger.

vico:postal_address a owl:Class;
   rdfs:subClassOf vico:address.
vico:op_postal_address a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_postal_address a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address rdfs:subClassOf vico:op_postal_address.
vico:dp_address rdfs:subClassOf vico:dp_postal_address.

vico:is_po_box a vico:dp_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range xs:boolean.
vico:has_city_name a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_house_no a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_po_box_no a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_postal_code a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_region_name a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_street_name a vico:op_postal_address;
   rdfs:domain vico:postal_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.

vico:email_address a owl:Class;
   rdfs:subClassOf vico:address.
vico:op_email_address a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_email_address a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address rdfs:subClassOf vico:op_email_address.
vico:dp_address rdfs:subClassOf vico:dp_email_address.

vico:has_email_domain_label a vico:op_email_address;
   rdfs:domain vico:email_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.
vico:has_email_prefix a vico:op_email_address;
   rdfs:domain vico:email_address;
   rdfs:range vico:address_token;
   rdfs:subPropertyOf vico:has_address_content.

vico:internet_address a owl:Class;
   rdfs:subClassOf vico:address.
vico:op_internet_address a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_internet_address a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_address rdfs:subClassOf vico:op_internet_address.
vico:dp_address rdfs:subClassOf vico:dp_internet_address.


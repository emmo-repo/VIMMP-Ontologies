
### MARKETPLACE-ACCESSIBLE COMPUTATIONAL RESOURCE ONTOLOGY (MACRO) ###

### VERSION 0.0.5 ###

### DATED 7TH AUGUST 2019 ###

@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.

@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/macro/macro.ttl#> a owl:Ontology.

macro:infrastructure a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:infrastructure.
macro:op_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_infrastructure.
macro:dp_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_infrastructure.
evmpo:op_paradigmatic_entity rdfs:subClassOf macro:op_infrastructure.
evmpo:dp_paradigmatic_entity rdfs:subClassOf macro:dp_infrastructure.

macro:repository a owl:Class;
   rdfs:subClassOf macro:infrastructure, cviii:agent.
macro:op_repository a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_repository a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_infrastructure rdfs:subClassOf macro:op_repository.
macro:dp_infrastructure rdfs:subClassOf macro:dp_repository.
cviii:op_agent rdfs:subClassOf macro:op_repository.
cviii:dp_agent rdfs:subClassOf macro:dp_repository.

macro:VIMMP_CERTIFICATE_REGISTRY a macro:repository;
   cviii:acts_on macro:VIMMP_MARKETPLACE;
   cviii:has_name "VIMMP Marketplace Certificate Registry"^^xs:string.

macro:channel a owl:Class;
   rdfs:subClassOf macro:infrastructure.
macro:op_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_infrastructure rdfs:subClassOf macro:op_channel.
macro:dp_infrastructure rdfs:subClassOf macro:dp_channel.

macro:has_channel_member a macro:op_channel;
   rdfs:domain macro:channel;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:has_access_to.
macro:has_channel_name a macro:dp_channel;
   rdfs:domain macro:channel;
   rdfs:range xs:string.

macro:channel owl:disjointUnionOf (macro:unidirectional_channel macro:bidirectional_channel).

macro:unidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_unidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_unidirectional_channel.

macro:has_channel_owner a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range cviii:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf cviii:is_owner_of.
macro:has_channel_subscriber a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range cviii:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf cviii:is_subscribed_to.

macro:newsletter a owl:Class;
   rdfs:subClassOf macro:unidirectional_channel.
macro:op_newsletter a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_newsletter a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_unidirectional_channel rdfs:subClassOf macro:op_newsletter.
macro:dp_unidirectional_channel rdfs:subClassOf macro:dp_newsletter.

macro:EMMC_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "EMMC Newsletter"^^xs:string.
macro:VIMMP_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "VIMMP Newsletter"^^xs:string.

macro:bidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_bidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_bidirectional_channel.

macro:virtual_marketplace a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_virtual_marketplace.
macro:dp_channel rdfs:subClassOf macro:dp_virtual_marketplace.

macro:has_stakeholder a macro:op_virtual_marketplace;
   rdfs:domain macro:virtual_marketplace;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:acts_at;
   rdfs:subPropertyOf macro:has_channel_member.

macro:VIMMP_MARKETPLACE a macro:virtual_marketplace.
macro:MARKETPLACE_MARKETPLACE a macro:virtual_marketplace.
macro:MARKET40_MARKETPLACE a macro:virtual_marketplace.

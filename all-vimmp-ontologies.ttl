macro:dp_hardware_infrastructure a owl:DatatypeProperty;
   owl:equivalentProperty evmpo:dp_hardware_infrastructure.
macro:op_hardware_infrastructure rdfs:subPropertyOf macro:op_computational_resource.
macro:dp_hardware_infrastructure rdfs:subPropertyOf macro:dp_computational_resource.

macro:hpc_infrastructure a owl:Class;

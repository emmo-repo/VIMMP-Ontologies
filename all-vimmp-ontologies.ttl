   rdfs:subClassOf owl:ObjectProperty.
otras:dp_kmk_operator_level a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_focus rdfs:subClassOf otras:op_kmk_operator_level.
otras:dp_focus rdfs:subClassOf otras:dp_kmk_operator_level.

otras:is_numerical_level a otras:dp_kmk_operator_level;

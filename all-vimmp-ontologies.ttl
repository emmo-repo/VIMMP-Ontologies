osmo:workflow_graph owl:disjointUnionOf (osmo:concrete_graph osmo:virtual_graph).

osmo:considers a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:section, [a owl:Class; owl:unionOf (osmo:use_case osmo:materials_model)];
   rdfs:comment "points to the use case or materials model considered by a workflow (sub)graph"^^xs:string;
   owl:inverseOf osmo:applies_to.
osmo:considers_model a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:materials_model;
   rdfs:comment "points to the materials model considered by a workflow (sub)graph"^^xs:string;
   rdfs:subPropertyOf osmo:considers.
osmo:considers_use_case a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:use_case;
   rdfs:comment "points to the use case considered by a workflow (sub)graph"^^xs:string;
   rdfs:subPropertyOf osmo:considers, evmpo:has_annotation.

osmo:has_subgraph a osmo:op_workflow_graph, owl:TransitiveProperty;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "points to a subgraph, i.e., a graph that is subordinate (to the present graph) by containment and/or instantiation"^^xs:string.

osmo:has_starting_point a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_node;
   rdfs:comment "points to the position, or a position, where the workflow execution begins";
   rdfs:subPropertyOf osmo:has_subgraph, evmpo:has_annotation.
osmo:has_terminal_point a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_node;
   rdfs:comment "points to the position, or a position, where the workflow execution terminates";
   rdfs:subPropertyOf osmo:has_subgraph, evmpo:has_annotation.

osmo:has_cause a osmo:op_workflow_graph, owl:TransitiveProperty;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "[G has_cause F] means that in every execution, F needs to finish before G can begin"^^xs:string.
osmo:has_effect a osmo:op_workflow_graph, owl:TransitiveProperty;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   owl:inverseOf osmo:has_cause.
osmo:is_coupled_with a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "if [F is_coupled_with G] they must occur synchronously when a workflow that contains them is executed"^^xs:string.
osmo:is_direct_cause_of a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:subPropertyOf osmo:is_linked_to, osmo:has_effect;
   rdfs:comment "if [F is direct cause of G] there is an immediate link by which G depends on the conclusion of F"^^xs:string.
osmo:is_direct_effect_of a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   owl:inverseOf osmo:is_direct_cause_of;
   rdfs:subPropertyOf osmo:is_linked_to, osmo:has_cause.
osmo:is_independent_of a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "two workflow graphs F and G are independent if they (and any of their subgraphs) are not coupled or linked to each other"^^xs:string.
osmo:is_linked_to a osmo:op_workflow_graph;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "if [F is linked to G] they cannot be executed concurrently; one side depends on the completion of the other side"^^xs:string.
osmo:is_part_of a osmo:op_workflow_graph, owl:TransitiveProperty;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:concrete_graph;
   owl:inverseOf osmo:contains_graph;
   rdfs:comment "[F is_part_of G] if F is a subgraph of graph G"^^xs:string.

osmo:workflow_node a owl:Class;
   rdfs:subClassOf osmo:workflow_graph, evmpo:annotation;
   rdfs:comment "workflow node, i.e., a workflow graph that contains exactly one workflow resource"^^xs:string.
osmo:op_workflow_node a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_workflow_node a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_workflow_graph rdfs:subClassOf osmo:op_workflow_node.
osmo:dp_workflow_graph rdfs:subClassOf osmo:dp_workflow_node.
evmpo:op_annotation rdfs:subClassOf osmo:op_workflow_node.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_workflow_node.

osmo:R_CONTAINS_ONE_RESOURCE a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:contains;
   owl:cardinality 1.
osmo:workflow_node rdfs:subClassOf osmo:R_CONTAINS_ONE_RESOURCE.

osmo:logical_node a owl:Class;
   rdfs:subClassOf osmo:workflow_node;
   rdfs:comment "logical node, i.e., a workflow node that contains a logical resource"^^xs:string.
osmo:op_logical_node a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_node a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_workflow_node rdfs:subClassOf osmo:op_logical_node.
osmo:dp_workflow_node rdfs:subClassOf osmo:dp_logical_node.

osmo:R_CONTAINS_A_LOGICAL_RESOURCE a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:contains_logical;
   owl:someValuesFrom osmo:logical_resource.
osmo:logical_node rdfs:subClassOf osmo:R_CONTAINS_A_LOGICAL_RESOURCE.

osmo:simulation_workflow a owl:Class;
   rdfs:subClassOf osmo:workflow_graph, osmo:simulation;
   rdfs:comment "simulation workflow, i.e., a (complete) workflow graph of an actually executable simulation workflow"^^xs:string.
osmo:op_simulation_workflow a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_simulation_workflow a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_workflow_graph rdfs:subClassOf osmo:op_simulation_workflow.
osmo:dp_workflow_graph rdfs:subClassOf osmo:dp_simulation_workflow.
osmo:op_simulation rdfs:subClassOf osmo:op_simulation_workflow.
osmo:dp_simulation rdfs:subClassOf osmo:dp_simulation_workflow.

osmo:simulation_workflow owl:intersectionOf (osmo:workflow_graph osmo:simulation).

osmo:has_simulation_outcome a osmo:op_simulation_workflow;
   rdfs:domain osmo:simulation_workflow;
   rdfs:range osmo:logical_node;
   rdfs:comment "points to a node that contains a logical resource with (all or part of) the end outcome of the simulation workflow"^^xs:string;
   rdfs:subPropertyOf osmo:has_subgraph, evmpo:has_simulation_annotation.
osmo:R_HAS_A_SIMULATION_OUTCOME a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_simulation_outcome;
   owl:someValuesFrom osmo:logical_node.
osmo:simulation_workflow rdfs:subClassOf osmo:R_HAS_A_SIMULATION_OUTCOME.

osmo:concrete_graph a owl:Class;
   rdfs:subClassOf osmo:workflow_graph, osmo:R_IS_CONCRETE;
   owl:intersectionOf (osmo:workflow_graph osmo:R_IS_CONCRETE);
   rdfs:comment "concrete graph, i.e., a workflow graph that is a concrete (non-virtual) workflow resource"^^xs:string.
osmo:op_concrete_graph a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_concrete_graph a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_workflow_graph rdfs:subClassOf osmo:op_concrete_graph.
osmo:dp_workflow_graph rdfs:subClassOf osmo:dp_concrete_graph.

osmo:contains a osmo:op_concrete_graph;
   rdfs:domain osmo:concrete_graph;
   rdfs:range osmo:workflow_resource;
   rdfs:comment "[G contains R] if R occurs as a part/component of graph G"^^xs:string.
osmo:contains_logical a osmo:op_concrete_graph;
   rdfs:domain osmo:concrete_graph;
   rdfs:range osmo:logical_resource;
   rdfs:subPropertyOf osmo:contains, evmpo:has_annotation.
osmo:contains_section a osmo:op_concrete_graph;
   rdfs:domain osmo:concrete_graph;
   rdfs:range osmo:section;
   rdfs:subPropertyOf osmo:contains.

osmo:contains_graph a osmo:op_concrete_graph, owl:TransitiveProperty;
   rdfs:domain osmo:concrete_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:subPropertyOf osmo:contains, osmo:has_subgraph;
   owl:inverseOf osmo:is_part_of.
osmo:instantiates a osmo:op_concrete_graph;
   rdfs:domain osmo:concrete_graph;
   rdfs:range osmo:virtual_graph;
   rdfs:comment "use of this relation: a virtual resource can be defined by describing a concrete resource that instantiates it (e.g., a single loop iteration), i.e., which is relates to the corresponding virtual resource by osmo:instantiates"^^xs:string;
   rdfs:subPropertyOf osmo:has_subgraph.

osmo:physical_variable a owl:Class;
   rdfs:subClassOf vivo:material_property, osmo:logical_variable, evmpo:material_property, evmpo:paradigmatic_entity;
   rdfs:comment "physical variable, i.e., a logical variable defined in terms of a material property"^^xs:string.
osmo:op_physical_variable a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_physical_variable a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_variable rdfs:subClassOf osmo:op_physical_variable.
osmo:dp_logical_variable rdfs:subClassOf osmo:dp_physical_variable.
vivo:op_material_property rdfs:subClassOf osmo:op_physical_variable.
vivo:dp_material_property rdfs:subClassOf osmo:dp_physical_variable.
evmpo:op_material_property rdfs:subClassOf osmo:op_physical_variable.
evmpo:dp_material_property rdfs:subClassOf osmo:dp_physical_variable.
evmpo:op_paradigmatic_entity rdfs:subClassOf osmo:op_physical_variable.
evmpo:dp_paradigmatic_entity rdfs:subClassOf osmo:dp_physical_variable.

osmo:is_quantity_kind a osmo:op_physical_variable, owl:FunctionalProperty;
   rdfs:domain osmo:physical_variable;
   rdfs:range qudt:QuantityKind;
   rdfs:subPropertyOf vivo:is_quantity_kind;
   rdfs:comment "points to the physical property characterization following QUDT"^^xs:string.

osmo:unique_physical_constant a owl:Class;
   rdfs:subClassOf osmo:logical_constant, osmo:unique_elementary, osmo:physical_variable;
   rdfs:comment "unique physical constant, i.e., a physical variable with a unique constant value"^^xs:string.
osmo:op_unique_physical_constant a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_unique_physical_constant a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_constant rdfs:subClassOf osmo:op_unique_physical_constant.
osmo:dp_logical_constant rdfs:subClassOf osmo:dp_unique_physical_constant.
osmo:op_unique_elementary rdfs:subClassOf osmo:op_unique_physical_constant.
osmo:dp_unique_elementary rdfs:subClassOf osmo:dp_unique_physical_constant.
osmo:op_physical_variable rdfs:subClassOf osmo:op_unique_physical_constant.
osmo:dp_physical_variable rdfs:subClassOf osmo:dp_unique_physical_constant.

osmo:condition a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "condition, i.e., a statement concerning values of properties and/or parameters and/or their relation to each other"^^xs:string.
osmo:op_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_condition.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_condition.

osmo:contains_variable a osmo:op_condition;
   rdfs:domain osmo:condition;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to a parameter (e.g., variable or input value) occurring in a condition"^^xs:string.
osmo:contains_predetermined_variable a osmo:op_condition;
   rdfs:domain osmo:condition;
   rdfs:range osmo:logical_variable;
   rdfs:subPropertyOf osmo:contains_variable;
   rdfs:comment "points to a parameter that is determined before workflow execution begins, i.e., an input parameter that is not specified by the user"^^xs:string.
osmo:contains_precomputed_variable a osmo:op_condition;
   rdfs:domain osmo:condition;
   rdfs:range osmo:logical_variable;
   rdfs:subPropertyOf osmo:contains_variable;
   rdfs:comment "points to a parameter that is computed within the workflow, but available and fixed at run time (of the element associate with the present condition), i.e., an input parameter that is not specified by the user"^^xs:string.
osmo:contains_runtime_variable a osmo:op_condition;
   rdfs:domain osmo:condition;
   rdfs:range osmo:logical_variable;
   rdfs:subPropertyOf osmo:contains_variable;
   rdfs:comment "points to a parameter that is only determined at run time (of the element associate with the present condition) or that is varied at run time"^^xs:string.
osmo:contains_undetermined_variable a osmo:op_condition;
   rdfs:domain osmo:condition;
   rdfs:range osmo:logical_variable;
   rdfs:subPropertyOf osmo:contains_variable;
   rdfs:comment "points to a parameter for which no value can be obtained (in an executable simulation workflow, undetermined parameters can only be present in conditions associated with elements that are never executed)"^^xs:string.

osmo:condition owl:disjointUnionOf (osmo:qualitative_condition osmo:quantitative_condition).

osmo:qualitative_condition a owl:Class;
   rdfs:subClassOf osmo:condition;
   rdfs:comment "qualitative condition, i.e., a condition (e.g., an inequality) that compares or connects properties and/or parameters without imposing any particular value for any of them, even if the values of all the others are given"^^xs:string.
osmo:op_qualitative_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_qualitative_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_condition rdfs:subClassOf osmo:op_qualitative_condition.
osmo:dp_condition rdfs:subClassOf osmo:dp_qualitative_condition.

osmo:quantitative_condition a owl:Class;
   rdfs:subClassOf osmo:condition;
   rdfs:comment "quantitative condition, i.e., a condition (e.g., an equation) that imposes a particular value for one of the considered quantities assuming that all others are given"^^xs:string.
osmo:op_quantitative_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_quantitative_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_condition rdfs:subClassOf osmo:op_quantitative_condition.
osmo:dp_condition rdfs:subClassOf osmo:dp_quantitative_condition.


osmo:logical_entity a owl:Class;
   rdfs:subClassOf osmo:workflow_entity;
   rdfs:comment "logical entity, i.e., a notational element of a simulation workflow related to data exchange and communication"^^xs:string.
osmo:op_logical_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_workflow_entity rdfs:subClassOf osmo:op_logical_entity.
osmo:dp_workflow_entity rdfs:subClassOf osmo:dp_logical_entity.

osmo:logical_resource a owl:Class;
   rdfs:subClassOf osmo:logical_entity, osmo:workflow_resource, evmpo:annotation, osmo:R_IS_CONCRETE;
   rdfs:comment "logical resource, i.e., a notational element of a simulation workflow to which data (i.e., logical variables) are formally written and/or from where they can be formally read"^^xs:string.
osmo:op_logical_resource a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_resource a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_entity rdfs:subClassOf osmo:op_logical_resource.
osmo:dp_logical_entity rdfs:subClassOf osmo:dp_logical_resource.
osmo:op_workflow_resource rdfs:subClassOf osmo:op_logical_resource.
osmo:dp_workflow_resource rdfs:subClassOf osmo:dp_logical_resource.
evmpo:op_annotation rdfs:subClassOf osmo:op_logical_resource.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_logical_resource.

osmo:has_logical_label a osmo:dp_logical_resource;
   rdfs:domain osmo:logical_resource;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_resource_label.
osmo:has_lrid a osmo:dp_logical_resource;
   rdfs:domain osmo:logical_resource;
   rdfs:range xs:string;
   rdfs:comment "points to the logical resource ID (LRID) associated with the present logical resource; n.b., this ID has technical purposes (for notational and visualization purposes, use osmo:has_logical_label)"^^xs:string.
osmo:is_interactive a osmo:dp_logical_resource;
   rdfs:domain osmo:logical_resource;
   rdfs:range xs:boolean;
   rdfs:comment "states whether an external entity (user interaction, or some process not represented in the workflow) has write access to this resource at execution time"^^xs:string.

osmo:has_stored_variable a osmo:op_logical_resource;
   rdfs:domain osmo:logical_resource;
   rdfs:range osmo:logical_variable.

osmo:logical_term a owl:Class;
   rdfs:subClassOf osmo:logical_entity;
   rdfs:comment "logical term, i.e., a logical variable or a value that can be assigned to a logical variable"^^xs:string.
osmo:op_logical_variable a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_variable a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_entity rdfs:subClassOf osmo:op_logical_term.
osmo:dp_logical_entity rdfs:subClassOf osmo:dp_logical_term.

osmo:has_term_index a osmo:dp_logical_term;
   rdfs:domain osmo:logical_term;
   rdfs:range xs:nonNegativeInteger;
   rdfs:comment "identifies a term by a non-negative integer number; in particular, use for array terms (numbering convention: from 0 to array size minus 1)"^^xs:string.
osmo:has_term_name a osmo:dp_logical_term;
   rdfs:domain osmo:logical_term;
   rdfs:range xs:string.

osmo:logical_term owl:disjointUnionOf (osmo:logical_variable osmo:logical_value).

osmo:logical_term owl:disjointUnionOf (osmo:elementary_term osmo:structure_term).

osmo:elementary_term a owl:Class;
   rdfs:subClassOf osmo:logical_term;
   rdfs:comment "elementary term, i.e., a logical term without any internal structure"^^xs:string.
osmo:op_elementary_term a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_elementary_term a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_term rdfs:subClassOf osmo:op_elementary_term.
osmo:dp_logical_term rdfs:subClassOf osmo:dp_elementary_term.

osmo:has_term_unit a osmo:op_elementary_term, owl:FunctionalProperty;
   rdfs:domain osmo:elementary_term;
   rdfs:range qudt:Unit, vivo:unit;
   rdfs:comment "unit associated with any assigned decimal values"^^xs:string;
   rdfs:subPropertyOf evmpo:has_annotation.

osmo:elementary_term owl:disjointUnionOf (osmo:elementary_logical osmo:elementary_value).

osmo:structure_term a owl:Class;
   rdfs:subClassOf osmo:logical_term;
   rdfs:comment "structure term, i.e., a logical term with an internal structure"^^xs:string.
osmo:op_structure_term a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_structure_term a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_term rdfs:subClassOf osmo:op_structure_term.
osmo:dp_logical_term rdfs:subClassOf osmo:dp_structure_term.

osmo:has_structure_element a osmo:op_structure_term;
   rdfs:domain osmo:structure_term;
   rdfs:range osmo:logical_term;
   rdfs:comment "points to an element of a structure"^^xs:string.

osmo:structure_term owl:disjointUnionOf (osmo:logical_structure osmo:structure_value).

osmo:logical_value a owl:Class;
   rdfs:subClassOf osmo:logical_term, evmpo:annotation;
   rdfs:comment "logical value, i.e., an entity that can be assigned to a logical variable as its value"^^xs:string.
osmo:op_logical_value a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_value a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_term rdfs:subClassOf osmo:op_logical_value.
osmo:dp_logical_term rdfs:subClassOf osmo:dp_logical_value.

osmo:logical_value owl:disjointUnionOf (osmo:elementary_value osmo:structure_value).

osmo:elementary_value a owl:Class;
   rdfs:subClassOf osmo:logical_value, osmo:elementary_term;
   owl:intersectionOf (osmo:logical_value osmo:elementary_term);
   rdfs:comment "elementary value, i.e., an entity that can be assigned to an elementary logical (variable) as its value"^^xs:string.
osmo:op_elementary_value a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_elementary_value a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_value rdfs:subClassOf osmo:op_elementary_value.
osmo:dp_logical_value rdfs:subClassOf osmo:dp_elementary_value.
osmo:op_elementary_term rdfs:subClassOf osmo:op_elementary_value.
osmo:dp_elementary_term rdfs:subClassOf osmo:dp_elementary_value.

osmo:is_decimal a osmo:dp_elementary_value;
   rdfs:domain osmo:elementary_value;
   rdfs:range xs:decimal.
osmo:is_string a osmo:dp_elementary_value;
   rdfs:domain osmo:elementary_value;
   rdfs:range xs:string.

osmo:structure_value a owl:Class;
   rdfs:subClassOf osmo:logical_value, osmo:structure_term;
   owl:intersectionOf (osmo:logical_value osmo:structure_term);
   rdfs:comment "structure value, i.e., an entity that can be assigned to a logical structure as its value"^^xs:string.
osmo:op_structure_value a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_structure_value a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_value rdfs:subClassOf osmo:op_structure_value.
osmo:dp_logical_value rdfs:subClassOf osmo:dp_structure_value.
osmo:op_structure_term rdfs:subClassOf osmo:op_structure_value.
osmo:dp_structure_term rdfs:subClassOf osmo:dp_structure_value.

osmo:logical_variable a owl:Class;
   rdfs:subClassOf osmo:logical_term;
   rdfs:comment "logical variable, i.e., property which can be exchanged between logical resources"^^xs:string.
osmo:op_logical_variable a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_logical_variable a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_logical_term rdfs:subClassOf osmo:op_logical_variable.
osmo:dp_logical_term rdfs:subClassOf osmo:dp_logical_variable.

osmo:has_value a osmo:op_logical_variable;
   rdfs:domain osmo:logical_variable;
   rdfs:range osmo:logical_value;
   rdfs:subPropertyOf evmpo:has_annotation.
osmo:has_current_value a osmo:op_logical_variable;
   rdfs:domain osmo:elementary_logical;
   rdfs:range osmo:logical_value;
   rdfs:subPropertyOf osmo:has_value.
osmo:has_default_value a osmo:op_logical_variable;
   rdfs:domain osmo:elementary_logical;
   rdfs:range osmo:logical_value;
   rdfs:subPropertyOf osmo:has_value.
osmo:has_initial_value a osmo:op_logical_variable;

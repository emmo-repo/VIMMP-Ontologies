otras:doi_citation rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty otras:is_about;
      owl:cardinality 1
   ];
   owl:intersectionOf (otras:citation [a owl:Restriction; owl:onProperty otras:has_doi; owl:someValuesFrom xs:string]).


   owl:inverseOf vico:is_inferior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

vico:academic_title a owl:Class;
   rdfs:subClassOf vico:titular_rank;
   rdfs:comment "academic title, i.e., a titular rank that signifies possession of an academic degree"^^xs:string.
vico:op_academic_title a owl:ObjectProperty.
vico:dp_academic_title a owl:DatatypeProperty.
vico:op_academic_title rdfs:subPropertyOf vico:op_titular_rank.
vico:dp_academic_title rdfs:subPropertyOf vico:dp_titular_rank.

vico:academic_title owl:disjointUnionOf (vico:nondoctoral_title vico:doctoral_title vico:postdoctoral_title).

vico:postdoctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "postdoctoral title, i.e., an academic title that is usually only conferred on people with a doctorate"^^xs:string.
vico:op_postdoctoral_title a owl:ObjectProperty.
vico:dp_postdoctoral_title a owl:DatatypeProperty.
vico:op_postdoctoral_title rdfs:subPropertyOf vico:op_academic_title.
vico:dp_postdoctoral_title rdfs:subPropertyOf vico:dp_academic_title.

vico:doctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "doctoral title, i.e., an academic title that signifies the possession of a doctorate degree"^^xs:string.
vico:op_doctoral_title a owl:ObjectProperty.
vico:dp_doctoral_title a owl:DatatypeProperty.
vico:op_doctoral_title rdfs:subPropertyOf vico:op_academic_title.
vico:dp_doctoral_title rdfs:subPropertyOf vico:dp_academic_title.

vico:nondoctoral_title a owl:Class;
   rdfs:subClassOf vico:academic_title;
   rdfs:comment "nondoctoral title, i.e., an academic title which does not confer a doctorate and also usually does not require a doctorate"^^xs:string.
vico:op_nondoctoral_title a owl:ObjectProperty.
vico:dp_nondoctoral_title a owl:DatatypeProperty.
vico:op_nondoctoral_title rdfs:subPropertyOf vico:op_academic_title.
vico:dp_nondoctoral_title rdfs:subPropertyOf vico:dp_academic_title.

vico:TITLE_FEMALE_GENERIC a vico:personal_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Ms"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_gendered_form_of vico:TITLE_MALE_GENERIC.
vico:TITLE_MALE_GENERIC a vico:personal_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Mr"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_gendered_form_of vico:TITLE_FEMALE_GENERIC.

vico:TITLE_DOCTOR_GENERIC a vico:doctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Dr"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean.

vico:TITLE_PROFESSOR_GENERIC_LONG a vico:postdoctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Professor"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_extended_form_of vico:TITLE_PROFESSOR_GENERIC_SHORT.
vico:TITLE_PROFESSOR_GENERIC_SHORT a vico:postdoctoral_title;
   vico:has_prefix_element "true"^^xs:boolean;
   vico:has_prefix_text "Prof"^^xs:string;
   vico:has_suffix_element "false"^^xs:boolean;
   vico:is_shortened_form_of vico:TITLE_PROFESSOR_GENERIC_LONG.

_:C_PERSONAL_TITLE a owl:AllDifferent;
   owl:members (vico:TITLE_FEMALE_GENERIC vico:TITLE_MALE_GENERIC vico:TITLE_DOCTOR_GENERIC vico:TITLE_PROFESSOR_GENERIC_LONG vico:TITLE_PROFESSOR_GENERIC_SHORT).

# "no nondoctoral title is superior to any postdoctoral title"
#
vico:is_superior_to_any a owl:ObjectProperty;
   rdfs:subPropertyOf vico:op_titular_rank;
   rdfs:domain vico:titular_rank;
   rdfs:range owl:Class;
   owl:propertyChainAxiom (vico:is_superior_to rdf:type).
vico:nondoctoral_title owl:disjointWith [
   a owl:Restriction;
   owl:onProperty vico:is_superior_to_any;
   owl:hasValue vico:postdoctoral_title
].

### VIMMP VALIDATION ONTOLOGY (VIVO) ###

### VERSION 0.4.1 ###

### DATED 27TH MARCH 2020 ###

@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix mmto: <http://www.vimmp.eu/semantics/mmto/mmto.ttl#>.
@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix qudt: <http://qudt.org/schema/qudt/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/vivo/vivo.ttl#> a owl:Ontology.

vivo:validation_entity a owl:Class;
   rdfs:comment "validation entity, i.e., a paradigmatic entity related to validation"^^xs:string;
   rdfs:subClassOf evmpo:paradigmatic_entity.

vivo:validation_entity owl:unionOf (vivo:assertion evmpo:assessment vivo:validation_statement vivo:validation_infrastructure vivo:material_property).

vivo:accuracy_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.

vivo:accuracy_entity owl:disjointUnionOf (vivo:accuracy_assertion vivo:accuracy_assessment);
   owl:disjointWith vivo:requirement_entity, vivo:review_entity.

vivo:requirement_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.

vivo:requirement_entity owl:disjointUnionOf (vivo:requirement_assertion vivo:requirement_assessment);
   owl:disjointWith vivo:accuracy_entity, vivo:review_entity.

vivo:review_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.

vivo:review_entity owl:disjointUnionOf (vivo:review_assertion vivo:review_assessment);
   owl:disjointWith vivo:accuracy_entity, vivo:requirement_entity.


#####################################
# ASSERTION BRANCH OF VIVO: GENERAL #
#####################################

vivo:data_item a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity;
   owl:equivalentClass evmpo:data_item.

vivo:assertion a owl:Class;
   rdfs:subClassOf vivo:data_item.

vivo:asserts_magnitude a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain vivo:assertion;
   rdfs:range xs:decimal.
vivo:has_unit a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain vivo:assertion;
   rdfs:range vivo:unit;
   rdfs:comment "unit in which any values are given"^^xs:string;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.
vivo:refers_to_property a owl:ObjectProperty;
   rdfs:domain vivo:assertion;
   rdfs:range evmpo:property.

vivo:assertion owl:disjointWith evmpo:assessment, vivo:material_property, vivo:validation_infrastructure.

vivo:assertion owl:disjointUnionOf (vivo:accuracy_assertion vivo:requirement_assertion vivo:review_assertion).

vivo:conceptual_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion.

vivo:material_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion.

vivo:refers_to_material a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain vivo:material_assertion;
   rdfs:range evmpo:material.
vivo:refers_to_mp a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain vivo:material_assertion;
   rdfs:range vivo:material_property;
   rdfs:subPropertyOf vivo:refers_to_property.

vivo:material_property_information a owl:Class;
   rdfs:subClassOf vivo:material_assertion;
   rdfs:comment "material property information, i.e., an assertion/datum referring to a (thermodynamic or mechanical) material property"^^xs:string.

vivo:has_error_statement a owl:ObjectProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:accuracy_assertion.
vivo:has_given_property a owl:ObjectProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:material_property;
   rdfs:subPropertyOf vivo:refers_to_mp.
vivo:has_lower_bound a owl:DatatypeProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:has_upper_bound a owl:DatatypeProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:has_value a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:is_given_in a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:unit;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.

vivo:accuracy_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:accuracy_entity;
   owl:intersectionOf (vivo:assertion vivo:accuracy_entity);
   rdfs:comment "accuracy assertion, i.e., statement on an uncertainty, error, or deviation from the most accurate possible value"^^xs:string.

vivo:has_deviation_magnitude a owl:DatatypeProperty, owl:FunctionalProperty;
   rdfs:domain vivo:accuracy_assertion;
   rdfs:range xs:decimal;
   rdfs:subPropertyOf vivo:asserts_magnitude.

vivo:accuracy_assertion owl:disjointUnionOf (vivo:absolute_deviation vivo:relative_deviation).

vivo:absolute_deviation a owl:Class;
   rdfs:subClassOf vivo:accuracy_assertion.

vivo:relative_deviation a owl:Class;
   rdfs:subClassOf vivo:accuracy_assertion.

vivo:requirement_assertion a owl:Class;
   rdfs:subClassOf vivo:conceptual_assertion, vivo:requirement_entity;
   owl:intersectionOf (vivo:assertion vivo:requirement_entity).

vivo:review_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:review_entity;
   owl:intersectionOf (vivo:assertion vivo:review_entity).


######################################
# ASSESSMENT BRANCH OF VIVO: GENERAL #
######################################

evmpo:assessment a owl:Class;
   rdfs:subClassOf vivo:validation_entity.

vivo:evaluates a owl:ObjectProperty;
   rdfs:domain evmpo:assessment;
   rdfs:range evmpo:marketplace_related_entity.
vivo:has_assertion a owl:ObjectProperty;
   rdfs:domain evmpo:assessment;
   rdfs:range vivo:assertion.
vivo:has_text_content a owl:DatatypeProperty;
   rdfs:domain evmpo:assessment;
   rdfs:range xs:string.

evmpo:assessment owl:disjointWith vivo:assertion, vivo:material_property, vivo:validation_infrastructure.

evmpo:assessment owl:disjointUnionOf (vivo:accuracy_assessment vivo:requirement_assessment vivo:review_assessment).

evmpo:assessment owl:disjointUnionOf (vivo:agent_assessment vivo:data_item_assessment vivo:document_assessment vivo:event_assessment vivo:infrastructure_assessment vivo:meta_assessment vivo:model_assessment vivo:project_assessment vivo:service_assessment vivo:workflow_assessment).

vivo:agent_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.

vivo:evaluates_agent a owl:ObjectProperty;
   rdfs:domain vivo:agent_assessment;
   rdfs:range evmpo:agent, vico:agent;
   rdfs:subPropertyOf vivo:evaluates.

vivo:agent_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:data_item_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment, [owl:unionOf (vivo:accuracy_assessment vivo:review_assessment)].

vivo:evaluates_data_item a owl:ObjectProperty;
   rdfs:domain vivo:data_item_assessment;
   rdfs:range vivo:data_item;
   rdfs:subPropertyOf vivo:evaluates.

vivo:data_item_assessment owl:disjointWith vivo:requirement_assessment.

vivo:document_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment, [owl:unionOf (vivo:accuracy_assessment vivo:review_assessment)].

vivo:evaluates_document a owl:ObjectProperty;
   rdfs:domain vivo:document_assessment;
   rdfs:range evmpo:document;
   rdfs:subPropertyOf vivo:evaluates.

vivo:document_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:event_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment, [owl:unionOf (vivo:review_assessment vivo:requirement_assessment)].

vivo:evaluates_event a owl:ObjectProperty;
   rdfs:domain vivo:event_assessment;
   rdfs:range evmpo:calendar_event;
   rdfs:subPropertyOf vivo:evaluates.

vivo:event_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:infrastructure_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment.

vivo:evaluates_infrastructure a owl:ObjectProperty;
   rdfs:domain vivo:infrastructure_assessment;
   rdfs:range evmpo:infrastructure;
   rdfs:subPropertyOf vivo:evaluates.

vivo:meta_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.

vivo:evaluates_assessment a owl:ObjectProperty;
   rdfs:domain vivo:meta_assessment;
   rdfs:range evmpo:assessment;
   rdfs:subPropertyOf vivo:evaluates.

vivo:meta_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:model_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment.

vivo:evaluates_model a owl:ObjectProperty;
   rdfs:domain vivo:model_assessment;
   rdfs:range evmpo:model;
   rdfs:subPropertyOf vivo:evaluates.

vivo:project_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment, [owl:unionOf (vivo:requirement_assessment vivo:review_assessment)].

vivo:evaluates_project a owl:ObjectProperty;
   rdfs:domain vivo:project_assessment;
   rdfs:range mmto:project;
   rdfs:subPropertyOf vivo:evaluates.

vivo:project_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:service_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.

vivo:evaluates_service a owl:ObjectProperty;
   rdfs:domain vivo:service_assessment;
   rdfs:range evmpo:service;
   rdfs:subPropertyOf vivo:evaluates.

vivo:service_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:service_assessment owl:disjointUnionOf (vivo:data_access_assessment vivo:hardware_access_assessment vivo:software_access_assessment vivo:training_assessment vivo:translation_assessment vivo:other_service_assessment).

vivo:data_access_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:hardware_access_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:software_access_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:training_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:translation_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:other_service_assessment a owl:Class;
   rdfs:subClassOf vivo:service_assessment.

vivo:workflow_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment.

vivo:evaluates_workflow a owl:ObjectProperty;
   rdfs:domain vivo:workflow_assessment;
   rdfs:range osmo:workflow_graph;
   rdfs:subPropertyOf vivo:evaluates.

vivo:accuracy_assessment a owl:Class;
   rdfs:subClassOf vivo:accuracy_entity, evmpo:assessment, [owl:unionOf (vivo:data_item_assessment vivo:document_assessment vivo:infrastructure_assessment vivo:model_assessment vivo:workflow_assessment)];
   owl:intersectionOf (vivo:accuracy_entity evmpo:assessment).

vivo:has_accuracy_assertion a owl:ObjectProperty;
   rdfs:domain vivo:accuracy_assessment;
   rdfs:range vivo:accuracy_assertion;
   rdfs:subPropertyOf vivo:has_assertion.

vivo:requirement_assessment a owl:Class;
   rdfs:subClassOf vivo:requirement_entity, evmpo:assessment, [owl:unionOf (vivo:event_assessment vivo:infrastructure_assessment vivo:model_assessment vivo:project_assessment vivo:workflow_assessment)];
   owl:intersectionOf (vivo:requirement_entity evmpo:assessment);
   owl:equivalentClass evmpo:requirement_assessment.

vivo:has_requirement_assertion a owl:ObjectProperty;
   rdfs:domain vivo:requirement_assessment;
   rdfs:range vivo:requirement_assertion;
   rdfs:subPropertyOf vivo:has_assertion.

vivo:review_assessment a owl:Class;
   rdfs:subClassOf vivo:review_entity, evmpo:assessment;
   owl:intersectionOf (vivo:review_entity evmpo:assessment).

vivo:has_review_assertion a owl:ObjectProperty;
   rdfs:domain vivo:review_assessment;
   rdfs:range vivo:review_assertion;
   rdfs:subPropertyOf vivo:has_assertion.
vivo:has_stars a owl:DatatypeProperty;
   rdfs:domain vivo:review_assessment;
   rdfs:range xs:decimal;
   rdfs:comment "By convention, the number of stars will normally be an integer between zero and five; in special circumstances, exceptions (non-integer, or negative, or six stars, etc.) may be justified."^^xs:string.

vivo:endorsement_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment;
   owl:equivalentClass evmpo:endorsement_assessment.


################################
# COMMUNICATION BRANCH OF VIVO #
################################

vivo:validation_statement a owl:Class;
   rdfs:subClassOf evmpo:statement, vico:validation_communication;
   owl:intersectionOf (vico:validation_communication evmpo:statement);
   rdfs:comment "validation statement, i.e., a statement which, as its main feature or purpose, relates to validation"^^xs:string.

vivo:validation_statement owl:disjointWith vivo:material_property, vivo:validation_infrastructure.

vivo:certificate a owl:Class;
   rdfs:subClassOf vivo:validation_statement;
   rdfs:comment "certificate, i.e., a validation statement by which an assessment is stated"^^xs:string.

vivo:has_certifier a owl:ObjectProperty;
   rdfs:domain vivo:certificate;
   rdfs:range vico:certifier;
   owl:inverseOf vico:is_certifier_of;
   rdfs:comment "connects a certificate to the certifier who is responsible for the included assessment (not necessarily identical with the author who communicates the certificate)"^^xs:string.
vivo:states_assessment a owl:ObjectProperty;
   rdfs:domain vivo:certificate;
   rdfs:range evmpo:assessment.

vivo:certificate rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vivo:has_certifier;
      owl:someValuesFrom vico:certifier
   ], [
      a owl:Restriction;
      owl:onProperty vico:states_assessment;
      owl:someValuesFrom evmpo:assessment
   ].


#################################
# INFRASTRUCTURE BRANCH OF VIVO #
#################################

vivo:validation_infrastructure a owl:Class;
   rdfs:subClassOf vivo:validation_entity, evmpo:infrastructure;
   owl:intersectionOf (vivo:validation_entity evmpo:infrastructure);
   rdfs:comment "validation infrastructure, i.e., an infrastructure which, as its main feature or purpose, relates to validation"^^xs:string.

vivo:validation_infrastructure owl:disjointWith vivo:assertion, evmpo:assessment, vivo:material_property, vivo:validation_statement.

vivo:validation_channel a owl:Class;
   rdfs:subClassOf vivo:validation_infrastructure, macro:channel;
   owl:intersectionOf (vivo:validation_entity macro:channel).

vivo:validation_repository a owl:Class;
   rdfs:subClassOf vivo:validation_infrastructure, macro:repository, evmpo:agent, vico:agent;
   owl:intersectionOf (vivo:validation_entity macro:repository).


###########################
# PROPERTY BRANCH OF VIVO #
###########################

vivo:material_property a owl:Class;
   rdfs:subClassOf vivo:validation_entity, evmpo:property, osmo:logical_variable;
   owl:equivalentClass evmpo:material_property;
   rdfs:comment "thermodynamic or mechanical behaviour of a material (referring either to the real material itself or a model thereof)"^^xs:string.

vivo:is_quantity_kind a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:domain vivo:material_property;
   rdfs:range qudt:QuantityKind;
   rdfs:comment "points to the physical property characterization following QUDT"^^xs:string.

vivo:material_property owl:disjointWith vivo:assertion, evmpo:assessment, vivo:validation_statement, vivo:validation_infrastructure.


################################################################
# ANNOTATION: ADDITIONAL UNITS DEFINED IN ACCORDANCE WITH QUDT #
################################################################

vivo:unit a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   owl:equivalentClass qudt:Unit;
   rdfs:comment "this class will be used to define units (individuals of the unit class) that are absent from qudt-unit"^^xs:string.

vivo:UNIT_DEBYE_ANGSTROM a vivo:unit, qudt:ElectricityAndMagnetismUnit;
   qudt:conversionMultiplier 3.33564e-40;
   qudt:conversionOffset 0.0;
   qudt:symbol "DA"^^xs:string;
   qudt:description "Debye Angstrom, i.e., a quadrupole-moment unit, corresponding to 3.33564e-40 C m^2, also referred to as Buckingham"^^xs:string.
qudt:ElectricityAndMagnetismUnit rdfs:subClassOf vivo:unit.
vivo:UNIT_KELVIN_BOLTZMANN a vivo:unit, qudt:EnergyAndWorkUnit;
   qudt:conversionMultiplier 1.38065e-23;
   qudt:conversionOffset 0.0;
   qudt:symbol "K k_B"^^xs:string;
   qudt:description "Kelvin x Boltzmann constant, i.e., an energy unit, corresponding to 1.38065e-23 J"^^xs:string.
qudt:EnergyAndWorkUnit rdfs:subClassOf vivo:unit.
vivo:UNIT_U a vivo:unit, qudt:MassUnit;  # note: this exists within qudt, but there is a bug in the definition
   qudt:conversionMultiplier 1.660539e-27;
   qudt:conversionOffset 0.0;
   qudt:symbol "u"^^xs:string;
   qudt:description "Unit of mass corresponding to 1 g/mol, also referred to as Dalton"^^xs:string.
qudt:MassUnit rdfs:subClassOf vivo:unit.

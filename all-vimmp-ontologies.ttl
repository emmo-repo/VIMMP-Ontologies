evmpo:property a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   rdfs:comment "property, i.e., a property as characterized by the EMMO"^^xs:string.
evmpo:op_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_paradigmatic_entity rdfs:subClassOf evmpo:op_property.
evmpo:dp_paradigmatic_entity rdfs:subClassOf evmpo:dp_property.

### subclasses of the fundamental class ###

evmpo:material_property a owl:Class;
   rdfs:subClassOf evmpo:property;
   rdfs:comment "material property, i.e., a property associated with a material"^^xs:string.
evmpo:op_material_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_material_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_property rdfs:subClassOf evmpo:op_material_property.
evmpo:dp_property rdfs:subClassOf evmpo:dp_material_property.
evmpo:physical_process_property a owl:Class;
   rdfs:subClassOf evmpo:property;
   rdfs:comment "physical process property, i.e., a property associated with a physical process"^^xs:string.
evmpo:op_physical_process_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_physical_process_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_property rdfs:subClassOf evmpo:op_physical_process_property.
evmpo:dp_property rdfs:subClassOf evmpo:dp_physical_process_property.

### annotations to the fundamental class ###

evmpo:has_property_annotation a evmpo:op_property;
   rdfs:domain evmpo:property;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.


###############################################################
###  non-paradigmatic fundamental category (0): annotation  ###
###############################################################

### fundamental class ###

evmpo:annotation a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "annotation, i.e., a non-paradigmatic aspect of any entity covered by EVMPO or a subordinate marketplace-level or subdomain-specific ontology"^^xs:string.
evmpo:op_annotation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_annotation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf evmpo:op_annotation.
evmpo:dp_marketplace_related_entity rdfs:subClassOf evmpo:dp_annotation.

### properties related to the fundamental class and its subclasses ###

evmpo:has_annotation a evmpo:op_marketplace_related_entity;
   rdfs:domain evmpo:marketplace_related_entity;
   rdfs:range evmpo:annotation.

### annotations to the fundamental class ###

evmpo:has_meta_annotation a evmpo:op_annotation;
   rdfs:domain evmpo:annotation;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.


<http://www.vimmp.eu/semantics/macro/macro.ttl#> a owl:Ontology.

macro:computational_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "computational entity, i.e., a computational resource or a pheonomenon or attribute associated with a computational resource"^^xs:string.
macro:op_computational_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_computational_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf macro:op_computational_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf macro:dp_computational_entity.

macro:computational_resource a owl:Class;
   rdfs:subClassOf evmpo:infrastructure, macro:computational_entity;
   rdfs:comment "computational resource, i.e., an infrastructure that is a computational entity"^^xs:string;
   owl:intersectionOf (evmpo:infrastructure macro:computational_entity).
macro:op_computational_resource a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_computational_resource a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_infrastructure rdfs:subClassOf macro:op_computational_resource.
evmpo:dp_infrastructure rdfs:subClassOf macro:dp_computational_resource.
macro:op_computational_entity rdfs:subClassOf macro:op_computational_resource.
macro:dp_computational_entity rdfs:subClassOf macro:dp_computational_resource.

macro:computational_resource owl:unionOf (macro:data_infrastructure macro:hardware_infrastructure macro:software_infrastructure).

macro:data_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:data_infrastructure.
macro:op_data_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_data_infrastructure.
macro:dp_data_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_data_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_data_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_data_infrastructure.

macro:hardware_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:hardware_infrastructure.
macro:op_hardware_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_hardware_infrastructure.
macro:dp_hardware_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_hardware_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_hardware_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_hardware_infrastructure.

macro:software_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:software_infrastructure.
macro:op_software_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_software_infrastructure.
macro:dp_software_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_software_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_software_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_software_infrastructure.

viso:software_tool rdfs:subClassOf macro:software_infrastructure.

macro:repository a owl:Class;
   rdfs:subClassOf macro:data_infrastructure, macro:hardware_infrastructure, vico:agent.
macro:op_repository a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_repository a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_data_infrastructure rdfs:subClassOf macro:op_repository.
macro:dp_data_infrastructure rdfs:subClassOf macro:dp_repository.
macro:op_hardware_infrastructure rdfs:subClassOf macro:op_repository.
macro:dp_hardware_infrastructure rdfs:subClassOf macro:dp_repository.
vico:op_agent rdfs:subClassOf macro:op_repository.
vico:dp_agent rdfs:subClassOf macro:dp_repository.

macro:VIMMP_CERTIFICATE_REGISTRY a macro:repository;
   vico:acts_on macro:VIMMP_MARKETPLACE;
   vico:has_name "VIMMP Marketplace Certificate Registry"^^xs:string.

macro:model_database a owl:Class;
   rdfs:subClassOf macro:repository, vico:model_provider.
macro:op_model_database a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_model_database a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_repository rdfs:subClassOf macro:op_model_database.
macro:dp_repository rdfs:subClassOf macro:dp_model_database.
vico:op_model_provider rdfs:subClassOf macro:op_model_database.
vico:dp_model_provider rdfs:subClassOf macro:dp_model_database.

macro:BOTTLED_SAFT a macro:model_database;
   vico:acts_on macro:VIMMP_MARKETPLACE;
   vico:has_name "Bottled SAFT"^^xs:string.

macro:channel a owl:Class;
   rdfs:subClassOf macro:data_infrastructure.
macro:op_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_data_infrastructure rdfs:subClassOf macro:op_channel.
macro:dp_data_infrastructure rdfs:subClassOf macro:dp_channel.

macro:has_channel_member a macro:op_channel;
   rdfs:domain macro:channel;
   rdfs:range vico:agent;
   owl:inverseOf vico:has_access_to.
macro:has_channel_name a macro:dp_channel;
   rdfs:domain macro:channel;
   rdfs:range xs:string.

macro:channel owl:disjointUnionOf (macro:unidirectional_channel macro:bidirectional_channel).

macro:unidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_unidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_unidirectional_channel.

macro:has_channel_owner a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range vico:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf vico:is_owner_of.
macro:has_channel_subscriber a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range vico:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf vico:is_subscribed_to.

macro:newsletter a owl:Class;
   rdfs:subClassOf macro:unidirectional_channel.
macro:op_newsletter a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_newsletter a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_unidirectional_channel rdfs:subClassOf macro:op_newsletter.
macro:dp_unidirectional_channel rdfs:subClassOf macro:dp_newsletter.

macro:EMMC_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "EMMC Newsletter"^^xs:string.
macro:VIMMP_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "VIMMP Newsletter"^^xs:string.

_:C_NEWSLETTER a owl:AllDifferent;
   owl:members (macro:EMMC_NEWSLETTER macro:VIMMP_NEWSLETTER).

macro:bidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_bidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_bidirectional_channel.

macro:virtual_marketplace a owl:Class;
   rdfs:subClassOf macro:bidirectional_channel.
macro:op_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_bidirectional_channel rdfs:subClassOf macro:op_virtual_marketplace.
macro:dp_bidirectional_channel rdfs:subClassOf macro:dp_virtual_marketplace.

macro:has_stakeholder a macro:op_virtual_marketplace;
   rdfs:domain macro:virtual_marketplace;
   rdfs:range vico:agent;
   owl:inverseOf vico:acts_at;
   rdfs:subPropertyOf macro:has_channel_member.

macro:VIMMP_MARKETPLACE a macro:virtual_marketplace.
macro:MARKETPLACE_MARKETPLACE a macro:virtual_marketplace.
macro:MARKET40_MARKETPLACE a macro:virtual_marketplace.

_:C_VIRTUAL_MARKETPLACE a owl:AllDifferent;
   owl:members (macro:VIMMP_MARKETPLACE macro:MARKETPLACE_MARKETPLACE macro:MARKET40_MARKETPLACE).

macro:io_format a owl:Class;
   rdfs:subClassOf macro:computational_entity, evmpo:annotation;
   rdfs:comment "I/O format, i.e., syntactical convention to which a technical I/O implementation can adhere"^^xs:string.
macro:op_io_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_io_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_computational_entity rdfs:subClassOf macro:op_io_format.
macro:dp_computational_entity rdfs:subClassOf macro:dp_io_format.
evmpo:op_annotation rdfs:subClassOf macro:op_io_format.
evmpo:dp_annotation rdfs:subClassOf macro:dp_io_format.

macro:has_definition_uri a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string.
macro:has_format_abbreviation a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string.
macro:has_format_name a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string.
macro:has_format_version a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string.
macro:is_supported_by a macro:op_io_format;
   rdfs:domain macro:io_format;
   rdfs:range viso:software_tool.

macro:io_format owl:unionOf (macro:file_format macro:stream_format).

macro:file_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   owl:equivalentClass edam:format_1915;  # "Format" in EDAM
   rdfs:comment "file format, i.e., a syntactical standard for files"^^xs:string.
macro:op_file_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_file_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_file_format.
macro:dp_io_format rdfs:subClassOf macro:dp_file_format.

macro:has_file_ending a macro:dp_file_format;
   rdfs:domain macro:file_format;
   rdfs:range xs:string.

macro:file_format owl:disjointUnionOf (macro:binary_file_format macro:textual_file_format).

macro:binary_file_format a owl:Class;
   rdfs:subClassOf macro:file_format;
   owl:equivalentClass edam:format_2333;  # "Binary format" in EDAM
   rdfs:comment "binary file format, i.e., any file format not encoded in ASCII, Unicode, or comparable"^^xs:string.
macro:op_binary_file_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_binary_file_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_file_format rdfs:subClassOf macro:op_binary_file_format.
macro:dp_file_format rdfs:subClassOf macro:dp_binary_file_format.

macro:textual_file_format a owl:Class;
   rdfs:subClassOf macro:file_format;
   owl:equivalentClass edam:format_2330;  # "Textual format" in EDAM
   rdfs:comment "textual file format, i.e., any file format encoded in ASCII, Unicode, or comparable"^^xs:string.
macro:op_textual_file_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_textual_file_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_file_format rdfs:subClassOf macro:op_textual_file_format.
macro:dp_file_format rdfs:subClassOf macro:dp_textual_file_format.

edam:format_2332 a owl:Class;  # "XML" in EDAM
   rdfs:subClassOf macro:file_format.

macro:textual_xml_format a owl:Class;
   rdfs:subClassOf macro:textual_file_format, edam:format_2332;  # "XML" in EDAM
   rdfs:comment "textual XML format, i.e., an XML-based textual file format"^^xs:string.
macro:op_textual_xml_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_textual_xml_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_textual_file_format rdfs:subClassOf macro:op_textual_xml_format.
macro:dp_textual_file_format rdfs:subClassOf macro:dp_textual_xml_format.

macro:stream_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   rdfs:comment "stream format, i.e., a syntactical standard for streams"^^xs:string.
macro:op_stream_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_stream_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_stream_format.
macro:dp_io_format rdfs:subClassOf macro:dp_stream_format.

macro:materials_modelling_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   rdfs:comment "materials modelling format, i.e., an I/O format that represents entities occurring within a model, solver, processor, or workflow in materials modelling"^^xs:string.
macro:op_materials_modelling_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_materials_modelling_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_materials_modelling_format.
macro:dp_io_format rdfs:subClassOf macro:dp_materials_modelling_format.

macro:has_granularity a macro:op_materials_modelling_format;
   rdfs:domain macro:materials_modelling_format;
   rdfs:range osmo:granularity_level;
   rdfs:comment "points to the granularity level to which the entities represented in the file format belong"^^xs:string.

macro:materials_relation_format a owl:Class;
   rdfs:subClassOf macro:materials_modelling_format;
   rdfs:comment "materials relation format, i.e., an I/O format that encodes a materials relation or part of a materials relation"^^xs:string.
macro:op_materials_relation_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_materials_relation_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_materials_modelling_format rdfs:subClassOf macro:op_materials_relation_format.
macro:dp_materials_modelling_format rdfs:subClassOf macro:dp_materials_relation_format.

macro:force_field_format a owl:Class;
   rdfs:subClassOf macro:materials_relation_format;
   rdfs:comment "force field format, i.e., a format representing an atomistic or mesoscopic materials relation (force field)"^^xs:string.
macro:op_force_field_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_force_field_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_materials_relation_format rdfs:subClassOf macro:op_force_field_format.
macro:dp_materials_relation_format rdfs:subClassOf macro:dp_force_field_format.

macro:force_field_format owl:disjointWith [
      a owl:Restriction;
      owl:onProperty macro:has_granularity;
      owl:hasValue osmo:ATOMISTIC
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_granularity;
      owl:hasValue osmo:CONTINUUM
   ].

macro:vocta_data_format a owl:Class;
   rdfs:subClassOf macro:force_field_format, macro:textual_file_format;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty macro:has_file_ending;
      owl:hasValue "data"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_format_abbreviation;
      owl:hasValue "VOCTA"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_format_name;
      owl:hasValue "VOCTA potential/force format"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_granularity;
      owl:hasValue osmo:MESOSCOPIC
   ];
   rdfs:comment "VOCTA potential/force format, used by the VOCTA code"^^xs:string.
macro:op_vocta_data_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_vocta_data_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_force_field_format rdfs:subClassOf macro:op_vocta_data_format.
macro:dp_force_field_format rdfs:subClassOf macro:dp_vocta_data_format.
macro:op_textual_file_format rdfs:subClassOf macro:op_vocta_data_format.
macro:dp_textual_file_format rdfs:subClassOf macro:dp_vocta_data_format.

macro:VOCTA_DATA_FORMAT_1 a macro:vocta_data_format;
   macro:has_format_version "1.0"^^xs:string;
   macro:is_supported_by macro:SOFTWARE_TOOL_VOCTA.

macro:solver_parameter_format a owl:Class;
   rdfs:subClassOf macro:materials_modelling_format;
   rdfs:comment "solver parameter format, i.e., an I/O format that is used to steer or parameterize a solver"^^xs:string.
macro:op_solver_parameter_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_solver_parameter_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_materials_modelling_format rdfs:subClassOf macro:op_solver_parameter_format.
macro:dp_materials_modelling_format rdfs:subClassOf macro:dp_solver_parameter_format.

macro:vocta_xml_format a owl:Class;
   rdfs:subClassOf macro:solver_parameter_format, macro:textual_xml_format;
   rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty macro:has_file_ending;
      owl:hasValue "xml"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_format_abbreviation;
      owl:hasValue "VOCTA"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_format_name;
      owl:hasValue "VOCTA control/settings format"^^xs:string
   ], [
      a owl:Restriction;
      owl:onProperty macro:has_granularity;
      owl:hasValue osmo:MESOSCOPIC
   ];
   rdfs:comment "VOCTA control/settings format, used by the VOCTA code"^^xs:string.
macro:op_vocta_xml_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_vocta_xml_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_solver_parameter_format rdfs:subClassOf macro:op_vocta_xml_format.
macro:dp_solver_parameter_format rdfs:subClassOf macro:dp_vocta_xml_format.
macro:op_textual_xml_format rdfs:subClassOf macro:op_vocta_xml_format.
macro:dp_textual_xml_format rdfs:subClassOf macro:dp_vocta_xml_format.

macro:VOCTA_XML_FORMAT_1 a macro:vocta_xml_format;
   macro:has_format_version "1.0"^^xs:string;
   macro:is_supported_by macro:SOFTWARE_TOOL_VOCTA.

# software tool individuals - details to be sorted out within VISO
#
macro:SOFTWARE_TOOL_VOCTA a viso:software_tool.


   owl:intersectionOf (osmo:physics_equation_type osmo:R_HAS_ATOMISTIC_GRANULARITY_LEVEL);
   owl:disjointUnionOf (
      osmo:pe_type_atomistic_density_functional     # RoMM category 2.1, OSMO PE type A.1
      osmo:pe_type_atomistic_molecular_statics      # RoMM category 2.2, OSMO PE type A.2
      osmo:pe_type_atomistic_molecular_dynamics     # RoMM category 2.3, OSMO PE type A.3
      osmo:pe_type_atomistic_partition_function     # RoMM category 2.4, OSMO PE type A.4

macro:computational_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "computational entity, i.e., a computational resource or a pheonomenon or attribute associated with a computational resource"^^xs:string.
macro:op_computational_entity a owl:ObjectProperty.
macro:dp_computational_entity a owl:DatatypeProperty.
macro:op_computational_entity rdfs:subPropertyOf evmpo:op_marketplace_related_entity.
macro:dp_computational_entity rdfs:subPropertyOf evmpo:dp_marketplace_related_entity.

macro:computational_resource a owl:Class;
   rdfs:subClassOf evmpo:infrastructure, macro:computational_entity;
   rdfs:comment "computational resource, i.e., an infrastructure that is a computational entity"^^xs:string;
   owl:intersectionOf (evmpo:infrastructure macro:computational_entity).
macro:op_computational_resource a owl:ObjectProperty.
macro:dp_computational_resource a owl:DatatypeProperty.
macro:op_computational_resource rdfs:subPropertyOf evmpo:op_infrastructure.
macro:dp_computational_resource rdfs:subPropertyOf evmpo:dp_infrastructure.
macro:op_computational_resource rdfs:subPropertyOf macro:op_computational_entity.
macro:dp_computational_resource rdfs:subPropertyOf macro:dp_computational_entity.

macro:computational_resource owl:unionOf (macro:data_infrastructure macro:hardware_infrastructure macro:software_infrastructure).

macro:data_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:data_infrastructure.
macro:op_data_infrastructure a owl:ObjectProperty;
   owl:equivalentProperty evmpo:op_data_infrastructure.
macro:dp_data_infrastructure a owl:DatatypeProperty;
   owl:equivalentProperty evmpo:dp_data_infrastructure.
macro:op_data_infrastructure rdfs:subPropertyOf macro:op_computational_resource.
macro:dp_data_infrastructure rdfs:subPropertyOf macro:dp_computational_resource.

macro:hardware_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:hardware_infrastructure.
macro:op_hardware_infrastructure a owl:ObjectProperty;

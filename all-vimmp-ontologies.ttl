osmo:op_pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.

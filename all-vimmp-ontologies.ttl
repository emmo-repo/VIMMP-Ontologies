mmto:translation_statement a owl:Class;
   rdfs:subClassOf evmpo:statement, vico:translation_communication;
   owl:intersectionOf (vico:translation_communication evmpo:statement);
   rdfs:comment "translation statement, i.e., a EVMPO/VICO statement which, as its main feature or purpose, relates to translation"^^xs:string.
mmto:op_translation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_translation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_translation_communication rdfs:subClassOf mmto:op_translation_statement.
vico:dp_translation_communication rdfs:subClassOf mmto:dp_translation_statement.
evmpo:op_statement rdfs:subClassOf mmto:op_translation_statement.
evmpo:dp_statement rdfs:subClassOf mmto:dp_translation_statement.

mmto:translation_statement owl:disjointUnionOf (mmto:modelling_statement mmto:negotiation_statement mmto:project_statement).

mmto:modelling_statement a owl:Class;
   rdfs:subClassOf mmto:translation_statement;
   rdfs:comment "modelling statement, i.e., a translation statement that relates to a MODA graph and/or user case (following OSMO)"^^xs:string.
mmto:op_modelling_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_modelling_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_translation_statement rdfs:subClassOf mmto:op_modelling_statement.
mmto:dp_translation_statement rdfs:subClassOf mmto:dp_modelling_statement.

mmto:modelling_statement owl:disjointUnionOf (mmto:problem_statement mmto:solution_statement).

mmto:problem_statement a owl:Class;
   rdfs:subClassOf mmto:modelling_statement;
   rdfs:comment "problem statement, i.e., a MODA statement that discusses a user case from the user-side point of view, in terms of MODA section 1"^^xs:string.
mmto:op_problem_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_problem_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_modelling_statement rdfs:subClassOf mmto:op_problem_statement.
mmto:dp_modelling_statement rdfs:subClassOf mmto:dp_problem_statement.

mmto:solution_statement a owl:Class;
   rdfs:subClassOf mmto:modelling_statement;
   rdfs:comment "solution statement, i.e., a MODA statement by which a modelling and simulation solution is provided in response to a problem statement"^^xs:string.
mmto:op_solution_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_solution_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_modelling_statement rdfs:subClassOf mmto:op_solution_statement.
mmto:dp_modelling_statement rdfs:subClassOf mmto:dp_solution_statement.

mmto:negotiation_statement a owl:Class;
   rdfs:subClassOf mmto:translation_statement, evmpo:interlocution;
   rdfs:comment "negotiation statement, i.e., a translation statement that relates to negotiations on a materials modelling project"^^xs:string.
mmto:op_negotiation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_negotiation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_translation_statement rdfs:subClassOf mmto:op_negotiation_statement.
mmto:dp_translation_statement rdfs:subClassOf mmto:dp_negotiation_statement.
evmpo:op_interlocution rdfs:subClassOf mmto:op_negotiation_statement.
evmpo:dp_interlocution rdfs:subClassOf mmto:dp_negotiation_statement.

mmto:negotiation_statement owl:disjointUnionOf (mmto:interest_statement mmto:decision_statement).

mmto:interest_statement a owl:Class;
   rdfs:subClassOf mmto:negotiation_statement;
   rdfs:comment "interest statement, i.e., a negotiation statement by which a translator or model provider announces that he will propose a solution to a problem statement"^^xs:string.
mmto:op_interest_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_interest_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_negotiation_statement rdfs:subClassOf mmto:op_interest_statement.
mmto:dp_negotiation_statement rdfs:subClassOf mmto:dp_interest_statement.

mmto:decision_statement a owl:Class;
   rdfs:subClassOf mmto:negotiation_statement;
   rdfs:comment "decision statement, i.e., a negotiation statement by which an end user communicates a final decision on a proposed solution statement"^^xs:string.
mmto:op_decision_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_decision_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_negotiation_statement rdfs:subClassOf mmto:op_decision_statement.
mmto:dp_negotiation_statement rdfs:subClassOf mmto:dp_decision_statement.

mmto:decision_statement owl:disjointUnionOf (mmto:acceptance_statement mmto:rejection_statement).

mmto:acceptance_statement a owl:Class;
   rdfs:subClassOf mmto:decision_statement;
   rdfs:comment "acceptance statement, i.e., a decision statement by which a solution statement is accepted, so that a materials modelling project can begin"^^xs:string.
mmto:op_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_decision_statement rdfs:subClassOf mmto:op_acceptance_statement.
mmto:dp_decision_statement rdfs:subClassOf mmto:dp_acceptance_statement.

mmto:rejection_statement a owl:Class;
   rdfs:subClassOf mmto:decision_statement;
   rdfs:comment "acceptance statement, i.e., a decision statement by which a solution statement is rejected (not leading to a materials modelling project)"^^xs:string.
mmto:op_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_decision_statement rdfs:subClassOf mmto:op_acceptance_statement.
mmto:dp_decision_statement rdfs:subClassOf mmto:dp_acceptance_statement.

mmto:project_statement a owl:Class;
   rdfs:subClassOf mmto:translation_statement, evmpo:declaration;
   rdfs:comment "project statement, i.e., a translation statement that relates to an agreed materials modelling project"^^xs:string.
mmto:op_project_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_project_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_translation_statement rdfs:subClassOf mmto:op_project_statement.
mmto:dp_translation_statement rdfs:subClassOf mmto:dp_project_statement.
evmpo:op_declaration rdfs:subClassOf mmto:op_project_statement.
evmpo:dp_declaration rdfs:subClassOf mmto:dp_project_statement.

mmto:project_statement owl:disjointUnionOf (mmto:opening_statement mmto:status_statement mmto:closing_statement).

mmto:opening_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;
   rdfs:comment "opening statement, i.e., a project statement by which a materials modelling project is initialized"^^xs:string.
mmto:op_opening_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_opening_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project_statement rdfs:subClassOf mmto:op_opening_statement.
mmto:dp_project_statement rdfs:subClassOf mmto:dp_opening_statement.

mmto:status_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;
   rdfs:comment "status statement, i.e., a project statement by which the status of an ongoing materials modelling project is communicated"^^xs:string.
mmto:op_status_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_status_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project_statement rdfs:subClassOf mmto:op_status_statement.
mmto:dp_project_statement rdfs:subClassOf mmto:dp_status_statement.

mmto:closing_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;
   rdfs:comment "closing statement, i.e., a project statement by which a materials modelling project is finalized"^^xs:string.
mmto:op_closing_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_closing_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project_statement rdfs:subClassOf mmto:op_closing_statement.
mmto:dp_project_statement rdfs:subClassOf mmto:dp_closing_statement.


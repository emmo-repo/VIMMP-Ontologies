viprs:p_loc_cand_rnoc a owl:ObjectProperty;
   rdfs:domain viprs:possible_entity;
   rdfs:range viprs:non_necessary_entity;
   rdfs:subPropertyOf viprs:conceptual_cooccurrence_possibility, viprs:p_loc_and_rnoc;

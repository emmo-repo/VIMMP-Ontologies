mmto:project a owl:Class;
   rdfs:subClassOf evmpo:process;
   rdfs:comment "project, i.e., an undertaking of one or several stakeholders aiming at reaching a pre-specified objective within limited time"^^xs:string.

mmto:has_project_coordinator a owl:ObjectProperty;
   rdfs:domain mmto:project;
   rdfs:range evmpo:agent;
   rdfs:comment "agent that carries the main responsibility for the project"^^xs:string;
   rdfs:subPropertyOf mmto:has_project_participant.
mmto:has_project_description a owl:DatatypeProperty;
   rdfs:domain mmto:project;
   rdfs:range xs:string;
   rdfs:comment "description of the project in text form"^^xs:string.
mmto:has_project_identifier a owl:DatatypeProperty;
   rdfs:domain mmto:project;
   rdfs:range xs:string;
   rdfs:comment "identifier of the project in text form"^^xs:string.
mmto:has_project_participant a owl:ObjectProperty;
   rdfs:domain mmto:project;
   rdfs:range evmpo:agent;
   rdfs:subPropertyOf viprs:has_proper_part;
   rdfs:comment "agent that participates in the project"^^xs:string.
mmto:has_project_step a owl:ObjectProperty;
   rdfs:domain mmto:project;
   rdfs:range mmto:project_step;
   rdfs:subPropertyOf viprs:has_proper_part;
   rdfs:comment "points to a step that occurs within the project"^^xs:string;
   owl:inverseOf mmto:belongs_to_project.
mmto:has_subproject a owl:ObjectProperty, owl:TransitiveProperty;
   rdfs:domain mmto:project;
   rdfs:range mmto:project;
   rdfs:subPropertyOf viprs:has_proper_part;
   rdfs:comment "points to a part of the project (e.g., a work package) that can also be considered as a (smaller) project by itself"^^xs:string.
mmto:has_objective_description a owl:DatatypeProperty;
   rdfs:domain mmto:project;
   rdfs:range xs:string;
   rdfs:comment "description of a project objective in text form"^^xs:string.

mmto:mm_project a owl:Class;
   rdfs:subClassOf mmto:project;
   rdfs:comment "materials modelling project, i.e., a project that aims at realizing a materials modelling workflow that can be represented by MODA/OSMO"^^xs:string.

mmto:addresses a owl:ObjectProperty;
   rdfs:domain mmto:mm_project;
   rdfs:range osmo:application_case;
   owl:inverseOf osmo:is_addressed_by;
   rdfs:subPropertyOf evmpo:has_process_annotation, viprs:mereosemiotics_hp_ip_hs;
   rdfs:comment "points to an application case that is addressed by the present materials modelling project"^^xs:string.
mmto:has_translation_step a owl:ObjectProperty;
   rdfs:domain mmto:mm_project;
   rdfs:range mmto:project_step;
   rdfs:comment "points to a translation step that occurs within the materials modelling project"^^xs:string;
   rdfs:subPropertyOf mmto:has_project_step.

mmto:business_case a owl:Class;
   rdfs:subClassOf osmo:application_case;
   owl:disjointWith mmto:bc_aspect, mmto:business_decision_support_system;
   rdfs:comment "business case, i.e., an application case with a focus on economic/business aspects"^^xs:string.

mmto:has_bc_aspect a owl:ObjectProperty;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bc_aspect;
   rdfs:subPropertyOf osmo:has_application_case_aspect;
   rdfs:comment "points to a business-case aspect"^^xs:string.
mmto:has_bc_kpi a owl:ObjectProperty;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:key_performance_indicator;
   rdfs:subPropertyOf viprs:mereosemiotics_is_hp_hs;
   owl:inverseOf mmto:occurs_in_bc;
   rdfs:comment "points to a KPI that is relevant for the business case"^^xs:string.
mmto:has_bc_summary a owl:DatatypeProperty;
   rdfs:domain mmto:business_case;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_application_case_summary;
   rdfs:comment "plain-text summary of the business case"^^xs:string.

mmto:has_bca_description a owl:ObjectProperty;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_description;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to the BC aspect containing an abstract or a rough description of the BC in text form"^^xs:string.
mmto:has_bca_industrial_case a owl:ObjectProperty;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_industrial_case;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on an industrial case associated with the present BC"^^xs:string.
mmto:has_bca_red_zone a owl:ObjectProperty;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_red_zone;

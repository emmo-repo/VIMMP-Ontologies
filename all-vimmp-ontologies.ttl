   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_processor_method_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_processor_aspect rdfs:subClassOf osmo:op_moda_processor_method_type.
osmo:dp_moda_processor_aspect rdfs:subClassOf osmo:dp_moda_processor_method_type.
osmo:op_moda_method_type rdfs:subClassOf osmo:op_moda_processor_method_type.
osmo:dp_moda_method_type rdfs:subClassOf osmo:dp_moda_processor_method_type.

osmo:moda_preprocessor_method_type a owl:Class;
   rdfs:subClassOf osmo:moda_processor_method_type;
   rdfs:comment "MODA preprocessor method type, i.e., a method type associated with a preprocessor"^^xs:string.
osmo:op_moda_preprocessor_method_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_preprocessor_method_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_processor_method_type rdfs:subClassOf osmo:op_moda_preprocessor_method_type.
osmo:dp_moda_processor_method_type rdfs:subClassOf osmo:dp_moda_preprocessor_method_type.

osmo:moda_coupled_processor_method_type a owl:Class;
   rdfs:subClassOf osmo:moda_processor_method_type;
   rdfs:comment "MODA coupled processor method type, i.e., a method type associated with a coupled processor"^^xs:string.
osmo:op_moda_coupled_processor_method_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_coupled_processor_method_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_processor_method_type rdfs:subClassOf osmo:op_moda_coupled_processor_method_type.
osmo:dp_moda_processor_method_type rdfs:subClassOf osmo:dp_moda_coupled_processor_method_type.

osmo:moda_postprocessor_method_type a owl:Class;
   rdfs:subClassOf osmo:moda_processor_method_type;
   rdfs:comment "MODA postprocessor method type, i.e., a method type associated with a postprocessor"^^xs:string.
osmo:op_moda_postprocessor_method_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_postprocessor_method_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_processor_method_type rdfs:subClassOf osmo:op_moda_postprocessor_method_type.
osmo:dp_moda_processor_method_type rdfs:subClassOf osmo:dp_moda_postprocessor_method_type.

osmo:TRAJECTORY_ANALYSER a osmo:moda_postprocessor_method_type;
   osmo:has_moda_string_content "trajectory analyser, i.e., a postprocessor that analyses simulation trajectory output"^^xs:string.

osmo:moda_processor_es a owl:Class;
   rdfs:subClassOf osmo:moda_processor_aspect, osmo:moda_error_statement;
   rdfs:comment "MODA processor error statement (MODA subsection 4.3), characterizes/describes the error or uncertainty associated with a processor output"^^xs:string.
osmo:op_moda_processor_es a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_processor_es a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_processor_aspect rdfs:subClassOf osmo:op_moda_processor_es.
osmo:dp_moda_processor_aspect rdfs:subClassOf osmo:dp_moda_processor_es.
osmo:op_moda_error_statement rdfs:subClassOf osmo:op_moda_processor_es.
osmo:dp_moda_error_statement rdfs:subClassOf osmo:dp_moda_processor_es.

osmo:moda_graph a owl:Class;
   rdfs:subClassOf osmo:moda_structure_entity, osmo:simulation_related_entity;
   rdfs:comment "MODA graph, i.e., a model, simulation, or aspects thereof that can be represented in MODA by a combination of MODA sections"^^xs:string.
osmo:op_moda_graph a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_graph a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_structure_entity rdfs:subClassOf osmo:op_moda_graph.
osmo:dp_moda_structure_entity rdfs:subClassOf osmo:dp_moda_graph.
osmo:op_simulation_related_entity rdfs:subClassOf osmo:op_moda_graph.
osmo:dp_simulation_related_entity rdfs:subClassOf osmo:dp_moda_graph.

osmo:contains a osmo:op_moda_graph;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_section;
   rdfs:comment "[G contains S] if the MODA section S occurs as a part/component of the MODA graph G"^^xs:string.
osmo:has_cause a osmo:op_moda_graph, owl:TransitiveProperty;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   rdfs:comment "[G has_cause F] means that in every execution, F needs to finish before G can begin"^^xs:string.
osmo:has_effect a osmo:op_moda_graph, owl:TransitiveProperty;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   owl:inverseOf osmo:has_cause.
osmo:is_coupled_with a osmo:op_moda_graph;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   rdfs:comment "if [F is_coupled_with G] they must occur synchronously when a workflow that contains them is executed"^^xs:string.
osmo:is_direct_cause_of a osmo:op_moda_graph;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   rdfs:subPropertyOf osmo:is_linked_to, osmo:has_effect;
   rdfs:comment "if [F is direct cause of G] there is an immediate link by which G depends on the conclusion of F"^^xs:string.
osmo:is_direct_effect_of a osmo:op_moda_graph;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   owl:inverseOf osmo:is_direct_cause_of;
   rdfs:subPropertyOf osmo:is_linked_to, osmo:has_cause.
osmo:is_linked_to a osmo:op_moda_graph;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   rdfs:comment "if [F is linked to G] they cannot be executed concurrently; one side depends on the completion of the other side"^^xs:string.
osmo:is_part_of a osmo:op_moda_graph, owl:TransitiveProperty;
   rdfs:domain osmo:moda_graph;
   rdfs:range osmo:moda_graph;
   rdfs:comment "[F is_part_of G] if F is a subgraph of graph G"^^xs:string.

osmo:moda_node a owl:Class;
   rdfs:subClassOf osmo:moda_graph;
   rdfs:comment "MODA node, i.e., a MODA graph that contains exactly one MODA section"^^xs:string.
osmo:op_moda_node a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_node a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_graph rdfs:subClassOf osmo:op_moda_node.
osmo:dp_moda_graph rdfs:subClassOf osmo:dp_moda_node.

osmo:R_CONTAINS_ONE_SECTION a owl:Restriction;
   rdfs:subClassOf osmo:constrained_simulation_related_entity;
   owl:onProperty osmo:contains;
   owl:cardinality 1.
osmo:moda_node rdfs:subClassOf osmo:R_CONTAINS_ONE_SECTION.

osmo:moda_workflow a owl:Class;
   rdfs:subClassOf osmo:moda_graph, osmo:simulation;
   rdfs:comment "MODA workflow, i.e., an executable simulation (workflow) that can be represented in MODA"^^xs:string.
osmo:op_moda_workflow a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_workflow a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_graph rdfs:subClassOf osmo:op_moda_workflow.
osmo:dp_moda_graph rdfs:subClassOf osmo:dp_moda_workflow.
osmo:op_simulation rdfs:subClassOf osmo:op_moda_workflow.
osmo:dp_simulation rdfs:subClassOf osmo:dp_moda_workflow.

osmo:moda_workflow owl:intersectionOf (osmo:moda_graph osmo:simulation).

osmo:parameter a owl:Class;
   rdfs:subClassOf osmo:model_annotation, osmo:simulation_annotation;
   rdfs:comment "parameter, i.e., adjustable quantity within a model or a simulation workflow, or a part or component thereof"^^xs:string.
osmo:op_parameter a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_parameter a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_model_annotation rdfs:subClassOf osmo:op_parameter.
osmo:dp_model_annotation rdfs:subClassOf osmo:dp_parameter.
osmo:op_simulation_annotation rdfs:subClassOf osmo:op_parameter.
osmo:dp_simulation_annotation rdfs:subClassOf osmo:dp_parameter.

osmo:condition a owl:Class;
   rdfs:subClassOf osmo:model_annotation, osmo:simulation_annotation;
   rdfs:comment "condition, i.e., a statement concerning values of properties and/or parameters and/or their relation to each other"^^xs:string.
osmo:op_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_model_annotation rdfs:subClassOf osmo:op_condition.
osmo:dp_model_annotation rdfs:subClassOf osmo:dp_condition.
osmo:op_simulation_annotation rdfs:subClassOf osmo:op_condition.
osmo:dp_simulation_annotation rdfs:subClassOf osmo:dp_condition.

osmo:condition owl:disjointUnionOf (osmo:qualitative_condition osmo:quantitative_condition).

osmo:qualitative_condition a owl:Class;
   rdfs:subClassOf osmo:condition;
   rdfs:comment "qualitative condition, i.e., a condition (e.g., an inequality) that compares or connects properties and/or parameters without imposing any particular value for any of them, even if the values of all the others are given"^^xs:string.
osmo:op_qualitative_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_qualitative_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_condition rdfs:subClassOf osmo:op_qualitative_condition.
osmo:dp_condition rdfs:subClassOf osmo:dp_qualitative_condition.

osmo:quantitative_condition a owl:Class;
   rdfs:subClassOf osmo:condition;
   rdfs:comment "quantitative condition, i.e., a condition (e.g., an equation) that imposes a particular value for one of the considered quantities assuming that all others are given"^^xs:string.
osmo:op_quantitative_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_quantitative_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_condition rdfs:subClassOf osmo:op_quantitative_condition.
osmo:dp_condition rdfs:subClassOf osmo:dp_quantitative_condition.

#

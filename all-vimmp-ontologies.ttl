   rdfs:range vivo:assertion.


vivo:qualification a owl:Class;
   rdfs:subClassOf evmpo:annotation.
vivo:op_qualification a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_qualification a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vivo:op_qualification.
evmpo:dp_annotation rdfs:subClassOf vivo:dp_qualification.

vivo:EXPECTATION a vivo:qualification.
vivo:UNSPECIFIED a vivo:qualification.

vivo:quantification a owl:Class;
   rdfs:subClassOf vivo:qualification.
vivo:op_quantification a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_quantification a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_qualification rdfs:subClassOf vivo:op_quantification.
vivo:dp_qualification rdfs:subClassOf vivo:dp_quantification.

vivo:is_relative_rank a vivo:dp_quantification, owl:FunctionalProperty;
   rdfs:domain vivo:quantification;
   rdfs:range xs:decimal.
vivo:MEDIAN_CASE a vivo:quantification;
   vivo:is_relative_rank 0.5.

vivo:scalar_order_quantification a owl:Class;
   rdfs:subClassOf vivo:quantification.
vivo:HIGHEST_CASE a vivo:scalar_order_quantification;
   vivo:is_relative_rank 1.0.
vivo:LOWEST_CASE a vivo:scalar_order_quantification;
   vivo:is_relative_rank 0.0.

vivo:judgement_order_quantification a owl:Class;
   rdfs:subClassOf vivo:quantification.
vivo:BEST_CASE a vivo:judgement_order_quantification;
   vivo:is_relative_rank 1.0.
vivo:WORST_CASE a vivo:judgement_order_quantification;
   vivo:is_relative_rank 0.0.


vivo:confidence a owl:Class;
   rdfs:subClassOf evmpo:annotation.
vivo:op_confidence a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_confidence a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vivo:op_confidence.
evmpo:dp_annotation rdfs:subClassOf vivo:dp_confidence.

vivo:ESTIMATE_FROM_CORRELATION a vivo:confidence.
vivo:ESTIMATE_FROM_EXPERIENCE a vivo:confidence.
vivo:STATEMENT_OF_BELIEF a vivo:confidence.
vivo:STATEMENT_OF_KNOWLEDGE a vivo:confidence.

vivo:data_item a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity;
   owl:equivalentClass evmpo:data_item.
vivo:op_data_item a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_data_item.
vivo:dp_data_item a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_data_item.
evmpo:op_information_content_entity rdfs:subClassOf vivo:op_data_item.
evmpo:dp_information_content_entity rdfs:subClassOf vivo:dp_data_item.

vivo:assertion a owl:Class;
   rdfs:subClassOf vivo:data_item.
vivo:op_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_data_item rdfs:subClassOf vivo:op_assertion.
vivo:dp_data_item rdfs:subClassOf vivo:dp_assertion.

vivo:asserts_magnitude a vivo:dp_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:assertion;
   rdfs:range xs:decimal.


vivo:requirement_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:requirement_entity.
vivo:op_requirement_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_requirement_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_requirement_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_requirement_assertion.
vivo:op_requirement_entity rdfs:subClassOf vivo:op_requirement_assertion.
vivo:dp_requirement_entity rdfs:subClassOf vivo:dp_requirement_assertion.

vivo:has_requirement_type a vivo:op_requirement_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:requirement_assertion;
   rdfs:range vivo:requirement_type;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.
vivo:has_requirement_scope a vivo:op_requirement_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:requirement_assertion;
   rdfs:range vivo:requirement_scope;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.

vivo:requirement_type a owl:Class;
   rdfs:subClassOf vivo:requirement_entity, evmpo:annotation.

vivo:REQUIRED_CORE_HOUR a vivo:requirement_type.
vivo:REQUIRED_IO_CAPACITY_MEGABYTE a vivo:requirement_type.
vivo:REQUIRED_MEMORY_MEGABYTE a vivo:requirement_type.
vivo:REQUIRED_RUNTIME_HOUR a vivo:requirement_type.

vivo:requirement_scope a owl:Class;
   rdfs:subClassOf vivo:requirement_entity, evmpo:annotation.

vivo:REQUIRED_OVERALL a vivo:requirement_scope.
vivo:REQUIRED_PER_DATA_POINT a vivo:requirement_scope.
vivo:REQUIRED_PER_SIMULATION_RUN a vivo:requirement_scope.
vivo:REQUIRED_PER_SIMULATED_NANOSECOND a vivo:requirement_scope.


vivo:physical_property a owl:Class;
   rdfs:subClassOf vivo:property, osmo:logical_variable, evmpo:material_property;
   rdfs:comment "thermodynamic or mechanical behaviour of a material (referring either to the real material itself or a model thereof)"^^xs:string.
vivo:op_physical_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_physical_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_property rdfs:subClassOf vivo:op_physical_property.
vivo:dp_property rdfs:subClassOf vivo:dp_physical_property.
osmo:op_logical_variable rdfs:subClassOf vivo:op_physical_property.
osmo:dp_logical_variable rdfs:subClassOf vivo:dp_physical_property.
evmpo:op_material_property rdfs:subClassOf vivo:op_physical_property.
evmpo:dp_material_property rdfs:subClassOf vivo:dp_physical_property.

vivo:is_quantity_kind a vivo:op_physical_property, owl:FunctionalProperty;
   rdfs:domain vivo:physical_property;
   rdfs:range quq:QuantityKind;
   rdfs:comment "points to the physical property characterization following QUDT"^^xs:string.

vivo:physical_property_information a owl:Class;
   rdfs:subClassOf vivo:data_item;
   rdfs:comment "physical property information, i.e., a condition/datum referring to a physical (thermodynamic or mechanical) property"^^xs:string.
vivo:op_physical_property_information a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_physical_property_information a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_data_item rdfs:subClassOf vivo:op_physical_property_information.
vivo:dp_data_item rdfs:subClassOf vivo:dp_physical_property_information.

vivo:has_error_statement a vivo:op_physical_property_information;
   rdfs:domain vivo:physical_property_information;
   rdfs:range vivo:deviation_assertion.
vivo:has_given_property a vivo:op_physical_property_information;
   rdfs:domain vivo:physical_property_information;
   rdfs:range vivo:physical_property.
vivo:has_lower_bound a vivo:dp_physical_property_information;
   rdfs:domain vivo:physical_property_information;
   rdfs:range xs:decimal.
vivo:has_upper_bound a vivo:dp_physical_property_information;
   rdfs:domain vivo:physical_property_information;
   rdfs:range xs:decimal.
vivo:has_value a vivo:dp_physical_property_information, owl:FunctionalProperty;
   rdfs:domain vivo:physical_property_information;
   rdfs:range xs:decimal.
vivo:is_given_in a vivo:op_physical_property_information, owl:FunctionalProperty;
   rdfs:domain vivo:physical_property_information;
   rdfs:range qudt:DecimalScaledUnit.

vivo:property_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:accuracy_entity.
vivo:op_property_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_property_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_property_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_property_assertion.
vivo:op_accuracy_entity rdfs:subClassOf vivo:op_property_assertion.
vivo:dp_accuracy_entity rdfs:subClassOf vivo:dp_property_assertion.

vivo:refers_to_material a vivo:op_property_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:property_assertion;
   rdfs:range evmpo:material.
vivo:refers_to_property a vivo:op_property_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:property_assertion;
   rdfs:range vivo:physical_property.

vivo:absolute_assertion a owl:Class;
   rdfs:subClassOf vivo:property_assertion.
vivo:op_absolute_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_absolute_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_property_assertion rdfs:subClassOf vivo:op_absolute_assertion.
vivo:dp_property_assertion rdfs:subClassOf vivo:dp_absolute_assertion.

vivo:has_unit a vivo:op_absolute_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:absolute_assertion;
   rdfs:range qudt:DecimalScaledUnit.

vivo:deviation_assertion a owl:Class;
   rdfs:subClassOf vivo:property_assertion.

vivo:magnitude_assertion a owl:Class;
   rdfs:subClassOf vivo:absolute_assertion.

vivo:absolute_deviation_assertion a owl:Class;
   rdfs:subClassOf vivo:absolute_assertion, vivo:deviation_assertion.
vivo:relative_deviation_assertion a owl:Class;
   rdfs:subClassOf vivo:deviation_assertion.


### VIMMP TRANSLATION ONTOLOGY (VTO) ###

### VERSION 0.4.7 ###

### DATED 31ST MAY 2019 ###

@prefix vto: <http://www.vimmp.eu/semantics/vto/vto.ttl#>.

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/vto/vto.ttl#> a owl:Ontology.

vto:translation_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "translation entity, i.e., an entity related to translating materials modelling problems to appropriate solutions"^^xs:string.
vto:op_translation_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_translation_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf vto:op_translation_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf vto:dp_translation_entity.

vto:project a owl:Class;
   rdfs:subClassOf vto:translation_entity, evmpo:process;
   rdfs:comment "project, i.e., an undertaking of one or several stakeholders aiming at reaching a pre-specified objective within limited time"^^xs:string.
vto:op_project a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_project a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_translation_entity rdfs:subClassOf vto:op_project.
vto:dp_translation_entity rdfs:subClassOf vto:dp_project.
evmpo:op_process rdfs:subClassOf vto:op_project.
evmpo:dp_process rdfs:subClassOf vto:dp_project.

vto:mm_project a owl:Class;
   rdfs:subClassOf vto:project, vto:translation_entity;
   rdfs:comment "materials modelling project, i.e., a project specifically on materials modelling, aiming at realizing an agreed workflow that can be represented by MODA/OSMO"^^xs:string.
vto:op_mm_project a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_mm_project a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_project rdfs:subClassOf vto:op_mm_project.
vto:dp_project rdfs:subClassOf vto:dp_mm_project.
vto:op_translation_entity rdfs:subClassOf vto:op_mm_project.
vto:dp_translation_entity rdfs:subClassOf vto:dp_mm_project.

vto:translation_communication a owl:Class;
   rdfs:subClassOf vto:translation_entity, evmpo:communication;
   owl:equivalentClass vico:translation_communication;
   rdfs:comment "translation communication, i.e., a EVMPO/VICO communication which, as its main feature or purpose, relates to translation"^^xs:string.
vto:op_translation_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vico:op_translation_communication.
vto:dp_translation_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vico:dp_translation_communication.
vto:op_translation_entity rdfs:subClassOf vto:op_translation_communication.
vto:dp_translation_entity rdfs:subClassOf vto:dp_translation_communication.
vto:op_communication rdfs:subClassOf vto:op_translation_communication.
vto:dp_communication rdfs:subClassOf vto:dp_translation_communication.

vto:translation_statement a owl:Class;
   rdfs:subClassOf evmpo:statement, vto:translation_communication;
   owl:equivalentClass vico:translation_statement;
   owl:intersection (vto:translation_communication evmpo:statement);
   rdfs:comment "translation statement, i.e., a EVMPO/VICO statement which, as its main feature or purpose, relates to translation"^^xs:string.
vto:op_translation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vico:op_translation_statement.
vico:dp_translation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vico:op_translation_statement.
vico:op_translation_communication rdfs:subClassOf vico:op_translation_statement.
vico:dp_translation_communication rdfs:subClassOf vico:dp_translation_statement.
evmpo:op_statement rdfs:subClassOf vico:op_translation_statement.
evmpo:dp_statement rdfs:subClassOf vico:dp_translation_statement.

vto:translation_statement owl:disjointUnionOf (vto:moda_statement vto:negotiation_statement vto:project_statement).

vto:moda_statement a owl:Class;
   rdfs:subClassOf vto:translation_statement;
   rdfs:comment "MODA statement, i.e., a translation statement that relates to a MODA graph and/or user case (following OSMO)"^^xs:string.
vto:op_moda_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_moda_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_translation_statement rdfs:subClassOf vto:op_moda_statement.
vto:dp_translation_statement rdfs:subClassOf vto:dp_moda_statement.

vto:moda_statement owl:disjointUnionOf (vto:problem_statement vto:solution_statement).

vto:problem_statement a owl:Class;
   rdfs:subClassOf vto:moda_statement;
   rdfs:comment "problem statement, i.e., a MODA statement that discusses a user case from the user-side point of view, in terms of MODA section 1"^^xs:string.
vto:op_problem_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_problem_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_moda_statement rdfs:subClassOf vto:op_problem_statement.
vto:dp_moda_statement rdfs:subClassOf vto:dp_problem_statement.

vto:solution_statement a owl:Class;
   rdfs:subClassOf vto:moda_statement;
   rdfs:comment "solution statement, i.e., a MODA statement by which a modelling and simulation solution is provided in response to a problem statement"^^xs:string.
vto:op_solution_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_solution_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_moda_statement rdfs:subClassOf vto:op_solution_statement.
vto:dp_moda_statement rdfs:subClassOf vto:dp_solution_statement.

vto:negotiation_statement a owl:Class;
   rdfs:subClassOf vto:translation_statement, evmpo:interlocution;
   rdfs:comment "negotiation statement, i.e., a translation statement that relates to negotiations on a materials modelling project"^^xs:string.
vto:op_negotiation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_negotiation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_translation_statement rdfs:subClassOf vto:op_negotiation_statement.
vto:dp_translation_statement rdfs:subClassOf vto:dp_negotiation_statement.
evmpo:op_interlocution rdfs:subClassOf vto:op_negotiation_statement.
evmpo:dp_interlocution rdfs:subClassOf vto:dp_negotiation_statement.

vto:negotiation_statement owl:disjointUnionOf (vto:interest_statement vto:decision_statement).

vto:interest_statement a owl:Class;
   rdfs:subClassOf vto:negotiation_statement;
   rdfs:comment "interest statement, i.e., a negotiation statement by which a translator or model provider announces that he will propose a solution to a problem statement"^^xs:string.
vto:op_interest_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_interest_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_negotiation_statement rdfs:subClassOf vto:op_interest_statement.
vto:dp_negotiation_statement rdfs:subClassOf vto:dp_interest_statement.

vto:decision_statement a owl:Class;
   rdfs:subClassOf vto:negotiation_statement;
   rdfs:comment "decision statement, i.e., a negotiation statement by which an end user communicates a final decision on a proposed solution statement"^^xs:string.
vto:op_decision_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_decision_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_negotiation_statement rdfs:subClassOf vto:op_decision_statement.
vto:dp_negotiation_statement rdfs:subClassOf vto:dp_decision_statement.

vto:decision_statement owl:disjointUnionOf (vto:acceptance_statement vto:rejection_statement).

vto:acceptance_statement a owl:Class;
   rdfs:subClassOf vto:decision_statement;
   rdfs:comment "acceptance statement, i.e., a decision statement by which a solution statement is accepted, so that a materials modelling project can begin"^^xs:string.
vto:op_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_decision_statement rdfs:subClassOf vto:op_acceptance_statement.
vto:dp_decision_statement rdfs:subClassOf vto:dp_acceptance_statement.

vto:rejection_statement a owl:Class;
   rdfs:subClassOf vto:decision_statement;
   rdfs:comment "acceptance statement, i.e., a decision statement by which a solution statement is rejected (not leading to a materials modelling project)"^^xs:string.
vto:op_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_acceptance_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_decision_statement rdfs:subClassOf vto:op_acceptance_statement.
vto:dp_decision_statement rdfs:subClassOf vto:dp_acceptance_statement.

vto:project_statement a owl:Class;
   rdfs:subClassOf vto:translation_statement, evmpo:declaration;
   rdfs:comment "project statement, i.e., a translation statement that relates to an agreed materials modelling project"^^xs:string.
vto:op_project_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_project_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_translation_statement rdfs:subClassOf vto:op_project_statement.
vto:dp_translation_statement rdfs:subClassOf vto:dp_project_statement.
evmpo:op_declaration rdfs:subClassOf vto:op_project_statement.
evmpo:dp_declaration rdfs:subClassOf vto:dp_project_statement.

vto:project_statement owl:disjointUnionOf (vto:opening_statement vto:status_statement vto:closing_statement).

vto:opening_statement a owl:Class;
   rdfs:subClassOf vto:project_statement;
   rdfs:comment "opening statement, i.e., a project statement by which a materials modelling project is initialized"^^xs:string.
vto:op_opening_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_opening_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_project_statement rdfs:subClassOf vto:op_opening_statement.
vto:dp_project_statement rdfs:subClassOf vto:dp_opening_statement.

vto:status_statement a owl:Class;
   rdfs:subClassOf vto:project_statement;
   rdfs:comment "status statement, i.e., a project statement by which the status of an ongoing materials modelling project is communicated"^^xs:string.
vto:op_status_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_status_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_project_statement rdfs:subClassOf vto:op_status_statement.
vto:dp_project_statement rdfs:subClassOf vto:dp_status_statement.

vto:closing_statement a owl:Class;
   rdfs:subClassOf vto:project_statement;
   rdfs:comment "closing statement, i.e., a project statement by which a materials modelling project is finalized"^^xs:string.
vto:op_closing_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vto:dp_closing_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vto:op_project_statement rdfs:subClassOf vto:op_closing_statement.
vto:dp_project_statement rdfs:subClassOf vto:dp_closing_statement.

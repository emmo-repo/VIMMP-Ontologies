osmo:has_material_class a osmo:op_moda_usercase;
   rdfs:domain osmo:moda_usercase;
   rdfs:range osmo:moda_material_class;
   rdfs:subPropertyOf evmpo:has_annotation.
osmo:has_material a osmo:op_moda_usercase;
   rdfs:domain osmo:moda_usercase;
   rdfs:range osmo:moda_material_descriptor.

osmo:moda_material_class a owl:Class;
   rdfs:subClassOf osmo:moda_usercase_entity, evmpo:annotation.

osmo:FLUID a osmo:moda_material_class.
osmo:SOLID a osmo:moda_material_class.

osmo:moda_material_descriptor a owl:Class;
   rdfs:subClassOf osmo:moda_usercase_entity, evmpo:annotation.
osmo:op_moda_material_descriptor a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_material_descriptor a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_usercase_entity rdfs:subClassOf osmo:op_moda_material_descriptor.
osmo:dp_moda_usercase_entity rdfs:subClassOf osmo:dp_moda_material_descriptor.

osmo:has_mole_fraction a osmo:dp_moda_material_descriptor, owl:FunctionalProperty;
   rdfs:domain osmo:moda_material_descriptor;
   rdfs:range xs:decimal.

osmo:moda_cas_number a owl:Class;
   rdfs:subClassOf osmo:moda_material_descriptor.
osmo:op_moda_cas_number a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_cas_number a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_material_descriptor rdfs:subClassOf osmo:op_moda_cas_number.
osmo:dp_moda_material_descriptor rdfs:subClassOf osmo:dp_moda_cas_number.

osmo:is_given_by a osmo:dp_moda_cas_number, owl:FunctionalProperty;
   rdfs:domain osmo:moda_cas_number;
   rdfs:range xs:string.



otras:has_identified_given_name_initials a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_person a otras:op_author_identifier;
   rdfs:domain otras:author_identifier;
   rdfs:range cviii:person.
otras:has_identified_surname a otras:dp_author_identifier, owl:FunctionalProperty;

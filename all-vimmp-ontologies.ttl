   owl:intersection (vico:translation_communication vico:statement);
   rdfs:comment "translation statement, i.e., a statement which, as its main feature or purpose, relates to translation"^^xs:string.
vico:op_translation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vto:op_translation_statement.
vico:dp_translation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vto:op_translation_statement.

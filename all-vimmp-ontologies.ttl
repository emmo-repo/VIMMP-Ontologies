otras:kmk_operator_level rdfs:subClassOf otras:R_IS_EXACTLY_ONE_NUMERICAL_LEVEL.

otras:KMK_LEVEL_I a otras:kmk_operator_level;
   otras:is_numerical_level 1.
otras:KMK_LEVEL_II a otras:kmk_operator_level;
   otras:is_numerical_level 2.
otras:KMK_LEVEL_III a otras:kmk_operator_level;
   otras:is_numerical_level 3.

otras:kmk_operator_specifier a owl:Class;

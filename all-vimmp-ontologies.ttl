   rdfs:range otras:document;
   rdfs:subPropertyOf otras:is_about;
   owl:inverseOf otras:is_cited_as.

otras:R_HAS_ONE_FIRST_AUTHOR_SURNAME a owl:Restriction;  # important to ensure that any set of citations can be sorted by first-author surname
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_first_author_surname;
   owl:cardinality 1.
otras:R_HAS_ONE_PUBLICATION_YEAR a owl:Restriction;  # important to ensure that any set of citations can be sorted by year
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_year;
   owl:cardinality 1.
otras:R_IS_CITATION_ABOUT_A_DOCUMENT a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_citation_about_document;
   owl:someValuesFrom otras:document.
otras:citation rdfs:subClassOf otras:R_HAS_ONE_PUBLICATION_YEAR, otras:R_IS_CITATION_ABOUT_A_DOCUMENT.

otras:doi_citation a owl:Class;
   rdfs:subClassOf otras:citation;
   rdfs:comment "DOI citation, i.e., a citation that has a DOI"^^xs:string.
otras:op_doi_citation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_doi_citation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_citation rdfs:subClassOf otras:op_doi_citation.
evmpo:dp_citation rdfs:subClassOf otras:dp_doi_citation.

otras:R_IS_ABOUT_ONE_ENTITY a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_about;
   owl:cardinality 1.
otras:R_HAS_A_DOI a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_doi;
   owl:someValuesFrom xs:string.
otras:doi_citation
   rdfs:subClassOf otras:R_IS_ABOUT_ONE_ENTITY;
   owl:intersectionOf (otras:citation otras:R_HAS_A_DOI).

otras:journal_article_citation a owl:Class;
   rdfs:subClassOf otras:doi_citation;
   rdfs:comment "journal article citation, i.e., a DOI citation that is about a journal article"^^xs:string.
otras:op_journal_article_citation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_journal_article_citation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_doi_citation rdfs:subClassOf otras:op_journal_article_citation.
evmpo:dp_doi_citation rdfs:subClassOf otras:dp_journal_article_citation.

otras:R_IS_CITATION_ABOUT_A_JOURNAL_ARTICLE a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:is_citation_about_document;
   owl:someValuesFrom otras:journal_article.
otras:journal_article_citation owl:intersectionOf (otras:doi_citation otras:R_IS_CITATION_ABOUT_A_JOURNAL_ARTICLE).

otras:has_publication_journal_identifier a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.
otras:has_publication_journal_name a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_abbreviation a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.
otras:has_publication_journal_issn a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string;
   rdfs:subPropertyOf otras:has_publication_journal_identifier.

otras:has_publication_journal_volume a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_issue a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:positiveInteger.
otras:has_publication_journal_pages a otras:dp_journal_article_citation;
   rdfs:domain otras:journal_article_citation;
   rdfs:range xs:string.

otras:R_HAS_A_PUBLICATION_JOURNAL_IDENTIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_identifier;
   owl:someValuesFrom xs:string.
otras:R_HAS_A_PUBLICATION_JOURNAL_VOLUME a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_volume;
   owl:someValuesFrom xs:positiveInteger.
otras:R_HAS_PUBLICATION_JOURNAL_PAGES a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_publication_journal_pages;
   owl:someValuesFrom xs:string.
otras:journal_article_citation rdfs:subClassOf otras:R_HAS_A_PUBLICATION_JOURNAL_IDENTIFIER, otras:R_HAS_A_PUBLICATION_JOURNAL_VOLUME, otras:R_HAS_PUBLICATION_JOURNAL_PAGES.

otras:author_identification_entity a owl:Class;
   rdfs:subClassOf otras:training_entity, evmpo:annotation;
   rdfs:comment "author identification entity, i.e., an entity related to the identification of authors"^^xs:string.
otras:op_author_identification_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_identification_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_training_entity rdfs:subClassOf otras:op_author_identification_entity.
otras:dp_training_entity rdfs:subClassOf otras:dp_author_identification_entity.
evmpo:op_annotation rdfs:subClassOf otras:op_author_identification_entity.
evmpo:dp_annotation rdfs:subClassOf otras:dp_author_identification_entity.

otras:author_identifier a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author identifier, i.e., a descriptor by which an author can be identified"^^xs:string.
otras:op_author_identifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_identifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_identifier.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_identifier.

otras:has_identified_given_name a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_given_name_initials a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.
otras:has_identified_person a otras:op_author_identifier;
   rdfs:domain otras:author_identifier;
   rdfs:range cviii:person.
otras:has_identified_surname a otras:dp_author_identifier, owl:FunctionalProperty;
   rdfs:domain otras:author_identifier;
   rdfs:range xs:string.

otras:R_HAS_ONE_IDENTIFIED_SURNAME a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_identified_surname;
   owl:cardinality 1.

otras:author_map a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map, i.e., an indexed collection of author identifiers (n.b., not a collection of the persons themselves, but of identifiers)"^^xs:string.
otras:op_author_map a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_map a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_map.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_map.

otras:has_author_map_element a otras:op_author_map;
   rdfs:domain otras:author_map;
   rdfs:range otras:author_map_element;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

otras:author_map_element a owl:Class;
   rdfs:subClassOf otras:author_identification_entity;
   rdfs:comment "author map element, i.e., a numbered entry within an indexed collection of author identifiers"^^xs:string.
otras:op_author_map_element a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_author_map_element a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
otras:op_author_identification_entity rdfs:subClassOf otras:op_author_map_element.
otras:dp_author_identification_entity rdfs:subClassOf otras:dp_author_map_element.

otras:has_ame_identifier a otras:op_author_map_element, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range otras:author_identifier;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
otras:has_ame_index a otras:dp_author_map_element, owl:FunctionalProperty;
   rdfs:domain otras:author_map_element;
   rdfs:range xs:nonNegativeInteger.

otras:R_HAS_ONE_AME_IDENTIFIER a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_ame_identifier;
   owl:cardinality 1.
otras:R_HAS_ONE_AME_INDEX a owl:Restriction;
   rdfs:subClassOf otras:constrained_entity;
   owl:onProperty otras:has_ame_index;
   owl:cardinality 1.
otras:author_map_element rdfs:subClassOf otras:R_HAS_ONE_AME_IDENTIFIER, otras:R_HAS_ONE_AME_INDEX.


otras:instructor a owl:Class;
   rdfs:subClassOf cviii:expert, cviii:training_provider, ccso:Professor;
   rdfs:comment "instructor, i.e., an individual human agent with expertise who can take responsibility for courses, training units, or training services"^^xs:string.
otras:op_instructor a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
otras:dp_instructor a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_expert rdfs:subClassOf otras:op_instructor.
cviii:dp_expert rdfs:subClassOf otras:dp_instructor.
cviii:op_training_provider rdfs:subClassOf otras:op_instructor.
cviii:dp_training_provider rdfs:subClassOf otras:dp_instructor.
cviii:op_human_agent rdfs:subClassOf otras:op_instructor.
cviii:dp_human_agent rdfs:subClassOf otras:dp_instructor.

### VIMMP COMMUNICATION ONTOLOGY (VICO) ###

### VERSION 0.8.5 ###

### DATED 12TH AUGUST 2019 ###

@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.

@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.
@prefix dcterms: <http://purl.org/dc/terms#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix vto: <http://www.vimmp.eu/semantics/vto/vto.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/vico/vico.ttl#> a owl:Ontology.

vico:constrained_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity.

vico:communication a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:communication;
   rdfs:comment "communication, i.e., a statement or collection of statements that is communicated by a stakeholder on a (communication) channel"^^xs:string.
vico:op_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_communication.
vico:dp_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_communication.
evmpo:op_paradigmatic_entity rdfs:subClassOf vico:op_communication.
evmpo:dp_paradigmatic_entity rdfs:subClassOf vico:dp_communication.

vico:contains a vico:op_communication, owl:TransitiveProperty;
   rdfs:domain vico:communication;
   rdfs:range vico:communication;
   rdfs:comment "The expression [A contains B] means that B is a proper part of A, where A and B are both communications"^^xs:string.
vico:has_access_rule a vico:op_communication, owl:FunctionalProperty;
   rdfs:domain vico:communication;
   rdfs:range vico:access_rule;
   rdfs:subPropertyOf evmpo:has_communication_annotation;
   rdfs:comment "regulates (read) access to the communicated content"^^xs:string.
vico:has_author a vico:op_communication, owl:FunctionalProperty;
   rdfs:domain vico:communication;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:is_author_of.
vico:has_expiry_date a vico:op_interlocution, owl:FunctionalProperty;
   rdfs:domain vico:communication;
   rdfs:range dcterms:W3C-DTF.
vico:has_label a vico:op_communication;
   rdfs:domain vico:communication;
   rdfs:range vico:label;
   rdfs:subPropertyOf evmpo:has_communication_annotation.
vico:has_status a vico:op_communication;
   rdfs:domain vico:communication;
   rdfs:range vico:status;
   rdfs:subPropertyOf vico:has_label.
vico:has_submission_date a vico:op_communication, owl:FunctionalProperty;
   rdfs:domain vico:communication;
   rdfs:range dcterms:W3C-DTF.
vico:has_text a vico:dp_communication;
   rdfs:domain vico:communication;
   rdfs:range xs:string.

vico:is_on_channel a vico:op_communication;
   rdfs:domain vico:communication;
   rdfs:range macro:channel.
vico:is_on_marketplace a vico:op_communication;
   rdfs:domain vico:communication;
   rdfs:range macro:virtual_marketplace;
   rdfs:subPropertyOf vico:is_on_channel.

vico:R_HAS_AN_AUTHOR a owl:Restriction;
   rdfs:subClassOf vico:constrained_entity;
   owl:onProperty vico:has_author;
   owl:someValuesFrom cviii:agent.
vico:communication
   owl:disjointUnionOf (vico:message vico:appendix);
   rdfs:subClassOf vico:R_HAS_AN_AUTHOR.

vico:statement a owl:Class;
   rdfs:subClassOf vico:communication;
   owl:equivalentClass evmpo:statement;
   rdfs:comment "statement, i.e., a communication that does not contain any further communication"^^xs:string.
vico:op_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_statement.
vico:dp_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_statement.
vico:op_communication rdfs:subClassOf vico:op_statement.
vico:dp_communication rdfs:subClassOf vico:dp_statement.

vico:R_CONTAINS_NO_COMMUNICATION a owl:Restriction;
   rdfs:subClassOf vico:constrained_entity;
   owl:onProperty vico:contains;
   owl:cardinality 0.
vico:statement owl:intersectionOf (vico:communication vico:R_CONTAINS_NO_COMMUNICATION).

vico:appendix a owl:Class;
   rdfs:subClassOf vico:communication;
   rdfs:comment "appendix, i.e., a communication which cannot stand alone and needs to be combined with a communication body"^^xs:string.
vico:op_appendix a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_appendix a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_communication rdfs:subClassOf vico:op_appendix.
vico:dp_communication rdfs:subClassOf vico:dp_appendix.

vico:R_CONTAINS_NO_MESSAGE a owl:Restriction;
   rdfs:subClassOf vico:constrained_entity;
   owl:onProperty vico:contains;
   owl:cardinality 0.
vico:appendix rdfs:subClassOf vico:R_CONTAINS_NO_MESSAGE.

vico:appendix_statement a owl:Class;
   rdfs:subClassOf vico:appendix, vico:statement;
   owl:intersectionOf (vico:appendix vico:statement).
vico:op_appendix_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_appendix_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_appendix rdfs:subClassOf vico:op_appendix_statement.
vico:dp_appendix rdfs:subClassOf vico:dp_appendix_statement.
vico:op_statement rdfs:subClassOf vico:op_appendix_statement.
vico:dp_statement rdfs:subClassOf vico:dp_appendix_statement.

vico:validation_communication a owl:Class;
   rdfs:subClassOf vico:communication, vivo:validation_entity;
   owl:equivalentClass vivo:validation_communication;
   rdfs:comment "validation communication, i.e., a communication which, as its main feature or purpose, relates to validation"^^xs:string.
vico:op_validation_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vivo:op_validation_communication.
vico:dp_translation_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vivo:dp_validation_communication.
vico:op_communication rdfs:subClassOf vico:op_validation_communication.
vico:dp_communication rdfs:subClassOf vico:dp_validation_communication.
vivo:op_validation_entity rdfs:subClassOf vico:op_validation_communication.
vivo:dp_validation_entity rdfs:subClassOf vico:dp_validation_communication.

vico:validation_statement a owl:Class;
   rdfs:subClassOf vico:statement, vico:validation_communication;
   owl:equivalentClass vivo:validation_statement;
   owl:intersection (vico:validation_communication vico:statement);
   rdfs:comment "validation statement, i.e., a statement which, as its main feature or purpose, relates to validation"^^xs:string.
vico:op_validation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vivo:op_validation_statement.
vico:dp_validation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vivo:op_validation_statement.
vico:op_validation_communication rdfs:subClassOf vico:op_validation_statement.
vico:dp_validation_communication rdfs:subClassOf vico:dp_validation_statement.
vico:op_statement rdfs:subClassOf vico:op_validation_statement.
vico:dp_statement rdfs:subClassOf vico:dp_validation_statement.

vico:certificate a owl:Class;
   rdfs:subClassOf vico:appendix_statement, vico:validation_statement;
   owl:equivalentClass vivo:certificate;
   rdfs:comment "certificate, i.e., an appendix statement by which an assessment is stated"^^xs:string.
vico:op_certificate a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass vivo:op_certificate.
vico:dp_certificate a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass vivo:dp_certificate.
vico:op_appendix_statement rdfs:subClassOf vico:op_certificate.
vico:dp_appendix_statement rdfs:subClassOf vico:dp_certificate.
vico:op_validation_statement rdfs:subClassOf vico:op_certificate.
vico:dp_validation_statement rdfs:subClassOf vico:dp_certificate.

vico:has_certifier a vico:op_certificate;
   rdfs:domain vico:certificate;
   rdfs:range cviii:certifier;
   owl:inverseOf cviii:is_certifier_of;
   rdfs:comment "connects a certificate to the certifier who is responsible for the included assessment (not necessarily identical with the author who communicates the certificate)"^^xs:string.
vico:states_assessment a vico:op_certificate;
   rdfs:domain vico:certificate;
   rdfs:range vivo:assessment;
   owl:equivalentProperty vivo:states_assessment.

vico:R_HAS_A_CERTIFIER a owl:Restriction;
   rdfs:subClassOf vico:constrained_entity;
   owl:onProperty vico:has_certifier;
   owl:someValuesFrom cviii:certifier.
vico:R_STATES_AN_ASSESSMENT a owl:Restriction;
   rdfs:subClassOf vico:constrained_entity;
   owl:onProperty vico:states_assessment;
   owl:someValuesFrom vivo:assessment.
vico:certificate rdfs:subClassOf vico:R_HAS_A_CERTIFIER, vico:R_STATES_AN_ASSESSMENT.

vico:access_rule a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "access rule, i.e., a communication annotation that specifies who is allowed to read the communicated content"^^xs:string.
vico:op_access_rule a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_access_rule a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vico:op_access_rule.
evmpo:dp_annotation rdfs:subClassOf vico:dp_access_rule.

vico:allows a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range cviii:interlocutor;
   rdfs:comment "[A allows B] means that B, who is an interlocutor, must be granted access"^^xs:string.
vico:excludes a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range cviii:interlocutor;
   rdfs:comment "[A excludes B] means that B, who is an interlocutor, must be denied access"^^xs:string.
vico:allows_all a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range cviii:interlocutor_group;
   rdfs:comment "[A allows_all C] means that all interlocutors from group C must be granted access"^^xs:string.
vico:excludes_all a vico:op_access_rule;
   rdfs:domain vico:access_rule;
   rdfs:range cviii:interlocutor_group;
   rdfs:comment "[A excludes_all C] means that all interlocutors from group C must be denied access"^^xs:string.

vico:allows owl:propertyDisjointWith vico:excludes.
vico:allows_all owl:propertyDisjointWith vico:excludes_all.

vico:label a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "label, i.e., a remark or information item that can be carried by (i.e., associated with) a communication"^^xs:string.
vico:op_label a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_label a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vico:op_label.
evmpo:dp_annotation rdfs:subClassOf vico:dp_label.

vico:status a owl:Class;
   rdfs:subClassOf vico:label;
   rdfs:comment "status, i.e., a label that expresses information on the submission, reading, or exchange process of a communication"^^xs:string.
vico:op_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_label rdfs:subClassOf vico:op_status.
evmpo:dp_label rdfs:subClassOf vico:dp_status.

vico:has_status_timestamp a vico:op_status, owl:FunctionalProperty;
   rdfs:domain vico:status;
   rdfs:range dcterms:W3C-DTF.

vico:processed_status a owl:Class;
   rdfs:subClassOf vico:status;
   rdfs:comment "processed status, i.e., a status expressing to what extent a communication and/or its contents or associated tasks have been processed or carried out"^^xs:string.
vico:op_processed_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_processed_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_status rdfs:subClassOf vico:op_processed_status.
evmpo:dp_status rdfs:subClassOf vico:dp_processed_status.

vico:has_fractional_progress a vico:dp_processed_status;
   rdfs:domain vico:processed_status;
   rdfs:range xs:decimal;
   rdfs:comment "fractional extent to which the communication has been processed; minimum: 0 for zero progress, maximum: 1 for completion"^^xs:string.

vico:read_status a owl:Class;
   rdfs:subClassOf vico:status;
   rdfs:comment "read status, i.e., a status expressing that a communication has been read"^^xs:string.
vico:op_read_status a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_read_status a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_status rdfs:subClassOf vico:op_read_status.
evmpo:dp_status rdfs:subClassOf vico:dp_read_status.

vico:sent_status a owl:Class;
   rdfs:subClassOf vico:status;

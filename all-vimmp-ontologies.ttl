@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.
@prefix edam: <http://edamontology.org/>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/macro/macro.ttl#> a owl:Ontology.

macro:infrastructure a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:infrastructure.
macro:op_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_infrastructure.
macro:dp_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_infrastructure.
evmpo:op_paradigmatic_entity rdfs:subClassOf macro:op_infrastructure.
evmpo:dp_paradigmatic_entity rdfs:subClassOf macro:dp_infrastructure.

macro:computational_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass osmo:computational_entity;
   rdfs:comment "computational entity, i.e., a computational resource or a pheonomenon or attribute associated with a computational resource"^^xs:string.
macro:op_computational_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass osmo:op_computational_entity.
macro:dp_computational_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass osmo:dp_computational_entity.
evmpo:op_marketplace_related_entity rdfs:subClassOf macro:op_computational_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf macro:dp_computational_entity.

macro:computational_resource a owl:Class;
   rdfs:subClassOf macro:infrastructure, macro:computational_entity;
   rdfs:comment "computational resource, i.e., an infrastructure that is a computational entity"^^xs:string;
   owl:intersectionOf (macro:infrastructure macro:computational_entity).
macro:op_computational_resource a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_computational_resource a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_infrastructure rdfs:subClassOf macro:op_computational_resource.
macro:dp_infrastructure rdfs:subClassOf macro:dp_computational_resource.
macro:op_computational_entity rdfs:subClassOf macro:op_computational_resource.
macro:dp_computational_entity rdfs:subClassOf macro:dp_computational_resource.

macro:computational_resource owl:unionOf (macro:data_infrastructure macro:hardware_infrastructure macro:software_infrastructure).

macro:data_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:data_infrastructure.
macro:op_data_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_data_infrastructure.
macro:dp_data_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_data_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_data_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_data_infrastructure.

macro:hardware_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:hardware_infrastructure.
macro:op_hardware_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_hardware_infrastructure.
macro:dp_hardware_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_hardware_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_hardware_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_hardware_infrastructure.

macro:software_infrastructure a owl:Class;
   rdfs:subClassOf macro:computational_resource;
   owl:equivalentClass evmpo:software_infrastructure.
macro:op_software_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_software_infrastructure.
macro:dp_software_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_software_infrastructure.
macro:op_computational_resource rdfs:subClassOf macro:op_software_infrastructure.
macro:dp_computational_resource rdfs:subClassOf macro:dp_software_infrastructure.

macro:repository a owl:Class;
   rdfs:subClassOf macro:data_infrastructure, macro:hardware_infrastructure, cviii:agent.
macro:op_repository a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_repository a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_data_infrastructure rdfs:subClassOf macro:op_repository.
macro:dp_data_infrastructure rdfs:subClassOf macro:dp_repository.
macro:op_hardware_infrastructure rdfs:subClassOf macro:op_repository.
macro:dp_hardware_infrastructure rdfs:subClassOf macro:dp_repository.
cviii:op_agent rdfs:subClassOf macro:op_repository.
cviii:dp_agent rdfs:subClassOf macro:dp_repository.

macro:VIMMP_CERTIFICATE_REGISTRY a macro:repository;
   cviii:acts_on macro:VIMMP_MARKETPLACE;
   cviii:has_name "VIMMP Marketplace Certificate Registry"^^xs:string.

macro:model_database a owl:Class;
   rdfs:subClassOf macro:repository, cviii:model_provider.
macro:op_model_database a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_model_database a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_repository rdfs:subClassOf macro:op_model_database.
macro:dp_repository rdfs:subClassOf macro:dp_model_database.
cviii:op_model_provider rdfs:subClassOf macro:op_model_database.
cviii:dp_model_provider rdfs:subClassOf macro:dp_model_database.

macro:BOTTLED_SAFT a macro:model_database;
   cviii:acts_on macro:VIMMP_MARKETPLACE;
   cviii:has_name "Bottled SAFT"^^xs:string.

macro:channel a owl:Class;
   rdfs:subClassOf macro:data_infrastructure.
macro:op_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_data_infrastructure rdfs:subClassOf macro:op_channel.
macro:dp_data_infrastructure rdfs:subClassOf macro:dp_channel.

macro:has_channel_member a macro:op_channel;
   rdfs:domain macro:channel;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:has_access_to.
macro:has_channel_name a macro:dp_channel;
   rdfs:domain macro:channel;
   rdfs:range xs:string.

macro:channel owl:disjointUnionOf (macro:unidirectional_channel macro:bidirectional_channel).

macro:unidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_unidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_unidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_unidirectional_channel.

macro:has_channel_owner a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range cviii:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf cviii:is_owner_of.
macro:has_channel_subscriber a macro:op_unidirectional_channel;
   rdfs:domain macro:unidirectional_channel;
   rdfs:range cviii:agent;
   rdfs:subPropertyOf macro:has_channel_member;
   owl:inverseOf cviii:is_subscribed_to.

macro:newsletter a owl:Class;
   rdfs:subClassOf macro:unidirectional_channel.
macro:op_newsletter a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_newsletter a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_unidirectional_channel rdfs:subClassOf macro:op_newsletter.
macro:dp_unidirectional_channel rdfs:subClassOf macro:dp_newsletter.

macro:EMMC_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "EMMC Newsletter"^^xs:string.
macro:VIMMP_NEWSLETTER a macro:newsletter;
   macro:has_channel_name "VIMMP Newsletter"^^xs:string.

macro:bidirectional_channel a owl:Class;
   rdfs:subClassOf macro:channel.
macro:op_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_bidirectional_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_channel rdfs:subClassOf macro:op_bidirectional_channel.
macro:dp_channel rdfs:subClassOf macro:dp_bidirectional_channel.

macro:virtual_marketplace a owl:Class;
   rdfs:subClassOf macro:bidirectional_channel.
macro:op_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_virtual_marketplace a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_bidirectional_channel rdfs:subClassOf macro:op_virtual_marketplace.
macro:dp_bidirectional_channel rdfs:subClassOf macro:dp_virtual_marketplace.

macro:has_stakeholder a macro:op_virtual_marketplace;
   rdfs:domain macro:virtual_marketplace;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:acts_at;
   rdfs:subPropertyOf macro:has_channel_member.

macro:VIMMP_MARKETPLACE a macro:virtual_marketplace.
macro:MARKETPLACE_MARKETPLACE a macro:virtual_marketplace.
macro:MARKET40_MARKETPLACE a macro:virtual_marketplace.

macro:io_format a owl:Class;
   rdfs:subClassOf macro:computational_entity, evmpo:annotation, edam:format_1915;  # Format in EDAM
   owl:equivalentClass osmo:io_format;
   rdfs:comment "I/O format, i.e., syntactical convention to which a technical I/O implementation can adhere"^^xs:string.
macro:op_io_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass osmo:op_io_format.
macro:dp_io_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass osmo:dp_io_format.
macro:op_computational_entity rdfs:subClassOf macro:op_io_format.
macro:dp_computational_entity rdfs:subClassOf macro:dp_io_format.
evmpo:op_annotation rdfs:subClassOf macro:op_io_format.
evmpo:dp_annotation rdfs:subClassOf macro:dp_io_format.

macro:has_definition_uri a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string;
   owl:equivalentProperty osmo:has_definition_uri.
macro:has_format_abbreviation a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string;
   owl:equivalentProperty osmo:has_format_abbreviation.
macro:has_format_name a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string;
   owl:equivalentProperty osmo:has_format_name.
macro:has_format_version a macro:dp_io_format;
   rdfs:domain macro:io_format;
   rdfs:range xs:string;
   owl:equivalentProperty osmo:has_format_version.

macro:io_format owl:unionOf (macro:file_format macro:memory_format macro:stream_format).

macro:file_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   rdfs:comment "file format, i.e., a syntactical standard for files"^^xs:string.
macro:op_file_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_file_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_file_format.
macro:dp_io_format rdfs:subClassOf macro:op_file_format.

macro:memory_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   rdfs:comment "memory format, i.e., an agreed way of representing information technically in memory"^^xs:string.
macro:op_memory_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_memory_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_memory_format.
macro:dp_io_format rdfs:subClassOf macro:op_memory_format.

macro:stream_format a owl:Class;
   rdfs:subClassOf macro:io_format;
   rdfs:comment "stream format, i.e., a syntactical standard for streams"^^xs:string.
macro:op_stream_format a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
macro:dp_stream_format a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
macro:op_io_format rdfs:subClassOf macro:op_stream_format.
macro:dp_io_format rdfs:subClassOf macro:op_stream_format.

### ONTOLOGY FOR SIMULATION, MODELLING, AND OPTIMIZATION (OSMO) ###

### VERSION 1.1.4 ###

### DATED 4TH OCTOBER 2019 ###

@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.

@prefix edam: <http://edamontology.org/>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix qudt: <http://qudt.org/schema/qudt/>.
@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix viso: <http://www.vimmp.eu/semantics/viso/viso-general.ttl#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix vto: <http://www.vimmp.eu/semantics/vto/vto.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/osmo/osmo.ttl#> a owl:Ontology.

osmo:constrained_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity.

osmo:model a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:model;
   rdfs:comment "model, i.e., a model as characterized by the EMMO"^^xs:string.
osmo:op_model a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model.
osmo:dp_model a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model.
evmpo:op_paradigmatic_entity rdfs:subClassOf osmo:op_model.
evmpo:dp_paradigmatic_entity rdfs:subClassOf osmo:dp_model.

osmo:simulation a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:simulation;
   rdfs:comment "simulation, i.e., a simulation workflow (including a single simulation as special case)"^^xs:string.
osmo:op_simulation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_simulation.
osmo:dp_simulation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_simulation.
evmpo:op_paradigmatic_entity rdfs:subClassOf osmo:op_simulation.
evmpo:dp_paradigmatic_entity rdfs:subClassOf osmo:dp_simulation.

osmo:model owl:disjointWith osmo:simulation.


osmo:materials_model_classifier a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "materials model classifier, i.e., a symbol for categories of materials-model related entities, following RoMM (note: these are descriptors, not identical with the described entities)"^^xs:string.
osmo:op_materials_model_classifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_materials_model_classifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf osmo:op_materials_model_classifier.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_materials_model_classifier.

osmo:granularity_level a owl:Class;
   rdfs:subClassOf osmo:materials_model_classifier;
   owl:oneOf (osmo:ELECTRONIC osmo:ATOMISTIC osmo:MESOSCOPIC osmo:CONTINUUM);
   rdfs:comment "granularity level (from RoMM), i.e., electronic, atomistic, mesoscopic, or continuum level according to RoMM"^^xs:string.
osmo:op_granularity_level a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_granularity_level a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_materials_model_classifier rdfs:subClassOf osmo:op_granularity_level.
osmo:dp_materials_model_classifier rdfs:subClassOf osmo:dp_granularity_level.

osmo:ELECTRONIC a osmo:granularity_level;
   rdfs:comment "granularity level label: electronic materials modelling"^^xs:string.
osmo:ATOMISTIC a osmo:granularity_level;
   rdfs:comment "granularity level label: atomistic materials modelling"^^xs:string.
osmo:MESOSCOPIC a osmo:granularity_level;
   rdfs:comment "granularity level label: mesoscopic materials modelling"^^xs:string.
osmo:CONTINUUM a osmo:granularity_level;
   rdfs:comment "granularity level label: continuum materials modelling"^^xs:string.

osmo:physical_equation_type a owl:Class;
   rdfs:subClassOf osmo:materials_model_classifier;
   rdfs:comment "physical equation type, i.e., a symbol referring to the physical equation underlying a model, mostly following RoMM (n.b., these entities are not models, they are descriptors of models)"^^xs:string.
osmo:op_physical_equation_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_physical_equation_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_materials_model_classifier rdfs:subClassOf osmo:op_physical_equation_type.
osmo:dp_materials_model_classifier rdfs:subClassOf osmo:dp_physical_equation_type.

osmo:has_granularity_level a osmo:op_physical_equation_type;
   rdfs:domain osmo:physical_equation_type;
   rdfs:range osmo:granularity_level;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
osmo:has_pe_type_descriptor a osmo:dp_physical_equation_type;
   rdfs:domain osmo:physical_equation_type;
   rdfs:range xs:string.
osmo:has_pe_type_id a osmo:dp_physical_equation_type;
   rdfs:domain osmo:physical_equation_type;
   rdfs:range xs:string.

osmo:R_HAS_ONE_GRANULARITY_LEVEL a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_granularity_level;
   owl:cardinality 1.
osmo:R_HAS_A_PE_TYPE_DESCRIPTOR a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_pe_type_descriptor;
   owl:someValuesFrom xs:string.
osmo:R_HAS_ONE_PE_TYPE_ID a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_pe_type_id;
   owl:cardinality 1.
osmo:physical_equation_type rdfs:subClassOf osmo:R_HAS_ONE_GRANULARITY_LEVEL, osmo:R_HAS_A_PE_TYPE_DESCRIPTOR, osmo:R_HAS_ONE_PE_TYPE_ID.

osmo:physical_equation_type owl:disjointUnionOf (osmo:pe_type_electronic osmo:pe_type_atomistic osmo:pe_type_mesoscopic osmo:pe_type_continuum).

osmo:pe_type_electronic a owl:Class;
   rdfs:subClassOf osmo:physical_equation_type;
   rdfs:comment "electronic physical equation type, i.e., a PE type at the electronic granularity level"^^xs:string.
osmo:op_pe_type_electronic a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_physical_equation_type rdfs:subClassOf osmo:op_pe_type_electronic.
osmo:dp_physical_equation_type rdfs:subClassOf osmo:dp_pe_type_electronic.

osmo:R_HAS_ELECTRONIC_GRANULARITY_LEVEL a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_granularity_level;
   owl:hasValue osmo:ELECTRONIC.
osmo:pe_type_electronic
   owl:intersectionOf (osmo:physical_equation_type osmo:R_HAS_ELECTRONIC_GRANULARITY_LEVEL);
   owl:disjointUnionOf (
      osmo:pe_type_electronic_qm_abinitio         # RoMM category 1.1, OSMO PE type identifier EL.1
      osmo:pe_type_electronic_manybody_effective  # RoMM category 1.2, OSMO PE type identifier EL.2
      osmo:pe_type_electronic_time_dependent      # RoMM category 1.3, OSMO PE type identifier EL.3
      osmo:pe_type_electronic_charge_transport    # RoMM category 1.4, OSMO PE type identifier EL.4
      osmo:pe_type_electronic_spin_transport      # RoMM category 1.5, OSMO PE type identifier EL.5
   ).

osmo:pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type EL.1: Ab-initio quantum mechanical (or first principle) model"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "EL.1"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.1, OSMO PE type EL.1"^^xs:string.
osmo:op_pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_qm_abinitio.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_qm_abinitio.

osmo:pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type EL.2: Electronic many-body and/or effective Hamiltonian model"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "EL.2"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.2, OSMO PE type EL.2"^^xs:string.
osmo:op_pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_manybody_effective.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_manybody_effective.

osmo:pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type EL.3: QM model of the response to time-dependent fields"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "EL.3"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.3, OSMO PE type EL.3"^^xs:string.
osmo:op_pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_time_dependent.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_time_dependent.

osmo:pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;


### VIMMP VALIDATION ONTOLOGY (VIVO) ###

### VERSION 0.3.8 ###

### DATED 7TH NOVEMBER 2019 ###

@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.

@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix iao: <http://purl.obolibrary.org/obo/iao.owl#>.
@prefix ical: <https://www.w3.org/2002/12/cal/icaltzd#>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix mmto: <http://www.vimmp.eu/semantics/mmto/mmto.ttl#>.
@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix qudt: <http://qudt.org/schema/qudt/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/vivo/vivo.ttl#> a owl:Ontology.

vivo:validation_entity a owl:Class;
   rdfs:comment "validation entity, i.e., a paradigmatic entity related to validation"^^xs:string;
   rdfs:subClassOf evmpo:paradigmatic_entity.
vivo:op_validation_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_validation_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_paradigmatic_entity rdfs:subClassOf vivo:op_validation_entity.
evmpo:dp_paradigmatic_entity rdfs:subClassOf vivo:dp_validation_entity.

vivo:validation_entity owl:unionOf (vivo:assertion vivo:assessment vivo:validation_statement vivo:validation_infrastructure vivo:property).

vivo:accuracy_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.
vivo:op_accuracy_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_accuracy_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_entity rdfs:subClassOf vivo:op_accuracy_entity.
vivo:dp_validation_entity rdfs:subClassOf vivo:dp_accuracy_entity.

vivo:accuracy_entity owl:disjointUnionOf (vivo:accuracy_assertion vivo:accuracy_assessment);
   owl:disjointWith vivo:requirement_entity, vivo:review_entity.

vivo:requirement_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.
vivo:op_requirement_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_requirement_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_entity rdfs:subClassOf vivo:op_requirement_entity.
vivo:dp_validation_entity rdfs:subClassOf vivo:dp_requirement_entity.

vivo:requirement_entity owl:disjointUnionOf (vivo:requirement_assertion vivo:requirement_assessment);
   owl:disjointWith vivo:accuracy_entity, vivo:review_entity.

vivo:review_entity a owl:Class;
   rdfs:subClassOf vivo:validation_entity.
vivo:op_review_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_review_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_entity rdfs:subClassOf vivo:op_review_entity.
vivo:dp_validation_entity rdfs:subClassOf vivo:dp_review_entity.

vivo:review_entity owl:disjointUnionOf (vivo:review_assertion vivo:review_assessment);
   owl:disjointWith vivo:accuracy_entity, vivo:requirement_entity.


############################
# ASSERTION BRANCH OF VIVO #
############################

vivo:data_item a owl:Class;
   rdfs:subClassOf evmpo:information_content_entity;
   owl:equivalentClass evmpo:data_item, iao:IAO_0000027.  # iao label "data item"
vivo:op_data_item a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_data_item.
vivo:dp_data_item a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_data_item.
evmpo:op_information_content_entity rdfs:subClassOf vivo:op_data_item.
evmpo:dp_information_content_entity rdfs:subClassOf vivo:dp_data_item.

vivo:assertion a owl:Class;
   rdfs:subClassOf vivo:data_item.
vivo:op_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_data_item rdfs:subClassOf vivo:op_assertion.
vivo:dp_data_item rdfs:subClassOf vivo:dp_assertion.

vivo:asserts_magnitude a vivo:dp_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:assertion;
   rdfs:range xs:decimal.
vivo:has_unit a vivo:op_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:assertion;
   rdfs:range vivo:unit;
   rdfs:comment "unit in which any values are given"^^xs:string;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.
vivo:refers_to_property a vivo:op_assertion;
   rdfs:domain vivo:assertion;
   rdfs:range vivo:property.

vivo:assertion owl:disjointWith vivo:assessment, vivo:property, vivo:validation_infrastructure.

vivo:assertion owl:disjointUnionOf (vivo:accuracy_assertion vivo:requirement_assertion vivo:review_assertion).

vivo:conceptual_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion.
vivo:op_conceptual_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_conceptual_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_conceptual_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_conceptual_assertion.

vivo:material_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion.
vivo:op_material_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_material_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_material_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_material_assertion.

vivo:refers_to_material a vivo:op_material_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:material_assertion;
   rdfs:range evmpo:material.
vivo:refers_to_mp a vivo:op_material_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:material_assertion;
   rdfs:range vivo:material_property;
   rdfs:subPropertyOf vivo:refers_to_property.

vivo:material_property_information a owl:Class;
   rdfs:subClassOf vivo:material_assertion;
   rdfs:comment "material property information, i.e., an assertion/datum referring to a (thermodynamic or mechanical) material property"^^xs:string.
vivo:op_material_property_information a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_material_property_information a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_material_assertion rdfs:subClassOf vivo:op_material_property_information.
vivo:dp_material_assertion rdfs:subClassOf vivo:dp_material_property_information.

vivo:has_error_statement a vivo:op_material_property_information;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:accuracy_assertion.
vivo:has_given_property a vivo:op_material_property_information;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:material_property;
   rdfs:subPropertyOf vivo:refers_to_mp.
vivo:has_lower_bound a vivo:dp_material_property_information;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:has_upper_bound a vivo:dp_material_property_information;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:has_value a vivo:dp_material_property_information, owl:FunctionalProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range xs:decimal.
vivo:is_given_in a vivo:op_material_property_information, owl:FunctionalProperty;
   rdfs:domain vivo:material_property_information;
   rdfs:range vivo:unit;
   rdfs:subPropertyOf evmpo:has_information_content_entity_annotation.

vivo:accuracy_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:accuracy_entity;
   owl:intersectionOf (vivo:assertion vivo:accuracy_entity);
   rdfs:comment "accuracy assertion, i.e., statement on an uncertainty, error, or deviation from the most accurate possible value"^^xs:string.
vivo:op_accuracy_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_accuracy_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_accuracy_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_accuracy_assertion.
vivo:op_accuracy_entity rdfs:subClassOf vivo:op_accuracy_assertion.
vivo:dp_accuracy_entity rdfs:subClassOf vivo:dp_accuracy_assertion.

vivo:has_deviation_magnitude a vivo:dp_accuracy_assertion, owl:FunctionalProperty;
   rdfs:domain vivo:accuracy_assertion;
   rdfs:range xs:decimal;
   rdfs:subPropertyOf vivo:asserts_magnitude.

vivo:accuracy_assertion owl:disjointUnionOf (vivo:absolute_deviation vivo:relative_deviation).

vivo:absolute_deviation a owl:Class;
   rdfs:subClassOf vivo:accuracy_assertion.
vivo:op_absolute_deviation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_absolute_deviation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_accuracy_assertion rdfs:subClassOf vivo:op_absolute_deviation.
vivo:dp_accuracy_assertion rdfs:subClassOf vivo:dp_absolute_deviation.

vivo:relative_deviation a owl:Class;
   rdfs:subClassOf vivo:accuracy_assertion.
vivo:op_relative_deviation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_relative_deviation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_accuracy_assertion rdfs:subClassOf vivo:op_relative_deviation.
vivo:dp_accuracy_assertion rdfs:subClassOf vivo:dp_relative_deviation.

vivo:requirement_assertion a owl:Class;
   rdfs:subClassOf vivo:conceptual_assertion, vivo:requirement_entity;
   owl:intersectionOf (vivo:assertion vivo:requirement_entity).
vivo:op_requirement_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_requirement_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_conceptual_assertion rdfs:subClassOf vivo:op_requirement_assertion.
vivo:dp_conceptual_assertion rdfs:subClassOf vivo:dp_requirement_assertion.
vivo:op_requirement_entity rdfs:subClassOf vivo:op_requirement_assertion.
vivo:dp_requirement_entity rdfs:subClassOf vivo:dp_requirement_assertion.

vivo:review_assertion a owl:Class;
   rdfs:subClassOf vivo:assertion, vivo:review_entity;
   owl:intersectionOf (vivo:assertion vivo:review_entity).
vivo:op_review_assertion a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_review_assertion a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assertion rdfs:subClassOf vivo:op_review_assertion.
vivo:dp_assertion rdfs:subClassOf vivo:dp_review_assertion.
vivo:op_review_entity rdfs:subClassOf vivo:op_review_assertion.
vivo:dp_review_entity rdfs:subClassOf vivo:dp_review_assertion.


#############################
# ASSESSMENT BRANCH OF VIVO #
#############################

vivo:assessment a owl:Class;
   rdfs:subClassOf vivo:validation_entity;
   owl:equivalentClass evmpo:assessment.
vivo:op_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_assessment.
vivo:dp_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_assessment.
vivo:op_validation_entity rdfs:subClassOf vivo:op_assessment.
vivo:dp_validation_entity rdfs:subClassOf vivo:dp_assessment.

vivo:evaluates a vivo:op_assessment;
   rdfs:domain vivo:assessment;
   rdfs:range evmpo:marketplace_related_entity.
vivo:has_assertion a vivo:op_assessment;
   rdfs:domain vivo:assessment;
   rdfs:range vivo:assertion.
vivo:has_text_content a vivo:dp_assessment;
   rdfs:domain vivo:assessment;
   rdfs:range xs:string.

vivo:assessment owl:disjointWith vivo:assertion, vivo:property, vivo:validation_infrastructure.

vivo:assessment owl:disjointUnionOf (vivo:accuracy_assessment vivo:requirement_assessment vivo:review_assessment).

vivo:assessment owl:disjointUnionOf (vivo:agent_assessment vivo:data_item_assessment vivo:document_assessment vivo:event_assessment vivo:infrastructure_assessment vivo:meta_assessment vivo:model_assessment vivo:project_assessment vivo:service_assessment vivo:workflow_assessment).

vivo:agent_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.
vivo:op_agent_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_agent_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_review_assessment rdfs:subClassOf vivo:op_agent_assessment.
vivo:dp_review_assessment rdfs:subClassOf vivo:dp_agent_assessment.

vivo:evaluates_agent a vivo:op_agent_assessment;
   rdfs:domain vivo:agent_assessment;
   rdfs:range evmpo:agent, cviii:agent;
   rdfs:subPropertyOf vivo:evaluates.

vivo:agent_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:data_item_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment, [owl:unionOf (vivo:accuracy_assessment vivo:review_assessment)].
vivo:op_data_item_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_data_item_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_data_item_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_data_item_assessment.

vivo:evaluates_data_item a vivo:op_data_item_assessment;
   rdfs:domain vivo:data_item_assessment;
   rdfs:range vivo:data_item;
   rdfs:subPropertyOf vivo:evaluates.

vivo:data_item_assessment owl:disjointWith vivo:requirement_assessment.

vivo:document_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment, [owl:unionOf (vivo:accuracy_assessment vivo:review_assessment)].
vivo:op_document_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_document_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_document_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_document_assessment.

vivo:evaluates_document a vivo:op_document_assessment;
   rdfs:domain vivo:document_assessment;
   rdfs:range evmpo:document, iao:IAO_0000310;  # iao label "document"
   rdfs:subPropertyOf vivo:evaluates.

vivo:document_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:event_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment, [owl:unionOf (vivo:review_assessment vivo:requirement_assessment)].
vivo:op_event_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_event_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_event_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_event_assessment.

vivo:evaluates_event a vivo:op_event_assessment;
   rdfs:domain vivo:event_assessment;
   rdfs:range evmpo:calendar_event, ical:Vevent;
   rdfs:subPropertyOf vivo:evaluates.

vivo:event_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:infrastructure_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment.
vivo:op_infrastructure_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_infrastructure_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_infrastructure_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_infrastructure_assessment.

vivo:evaluates_infrastructure a vivo:op_infrastructure_assessment;
   rdfs:domain vivo:infrastructure_assessment;
   rdfs:range evmpo:infrastructure, macro:infrastructure;
   rdfs:subPropertyOf vivo:evaluates.

vivo:meta_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.
vivo:op_meta_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_meta_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_review_assessment rdfs:subClassOf vivo:op_meta_assessment.
vivo:dp_review_assessment rdfs:subClassOf vivo:dp_meta_assessment.

vivo:evaluates_assessment a vivo:op_meta_assessment;
   rdfs:domain vivo:meta_assessment;
   rdfs:range vivo:assessment;
   rdfs:subPropertyOf vivo:evaluates.

vivo:meta_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:model_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment.
vivo:op_model_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_model_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_model_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_model_assessment.

vivo:evaluates_model a vivo:op_model_assessment;
   rdfs:domain vivo:model_assessment;
   rdfs:range evmpo:model;
   rdfs:subPropertyOf vivo:evaluates.

vivo:project_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment, [owl:unionOf (vivo:requirement_assessment vivo:review_assessment)].
vivo:op_project_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_project_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_project_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_project_assessment.

vivo:evaluates_project a vivo:op_model_assessment;
   rdfs:domain vivo:project_assessment;
   rdfs:range mmto:project;
   rdfs:subPropertyOf vivo:evaluates.

vivo:project_assessment owl:disjointWith vivo:accuracy_assessment.

vivo:service_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment.
vivo:op_service_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_service_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_review_assessment rdfs:subClassOf vivo:op_service_assessment.
vivo:dp_review_assessment rdfs:subClassOf vivo:dp_service_assessment.

vivo:evaluates_service a vivo:op_service_assessment;
   rdfs:domain vivo:service_assessment;
   rdfs:range evmpo:service;
   rdfs:subPropertyOf vivo:evaluates.

vivo:service_assessment owl:disjointWith vivo:accuracy_assessment, vivo:requirement_assessment.

vivo:workflow_assessment a owl:Class;
   rdfs:subClassOf vivo:assessment.
vivo:op_workflow_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_workflow_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_assessment rdfs:subClassOf vivo:op_workflow_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_workflow_assessment.

vivo:evaluates_workflow a vivo:op_workflow_assessment;
   rdfs:domain vivo:workflow_assessment;
   rdfs:range osmo:workflow_graph;
   rdfs:subPropertyOf vivo:evaluates.

vivo:accuracy_assessment a owl:Class;
   rdfs:subClassOf vivo:accuracy_entity, vivo:assessment, [owl:unionOf (vivo:data_item_assessment vivo:document_assessment vivo:infrastructure_assessment vivo:model_assessment vivo:workflow_assessment)];
   owl:intersectionOf (vivo:accuracy_entity vivo:assessment);
   owl:equivalentClass evmpo:accuracy_assessment.
vivo:op_accuracy_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_accuracy_assessment.
vivo:dp_accuracy_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_accuracy_assessment.
vivo:op_accuracy_entity rdfs:subClassOf vivo:op_accuracy_assessment.
vivo:dp_accuracy_entity rdfs:subClassOf vivo:dp_accuracy_assessment.
vivo:op_assessment rdfs:subClassOf vivo:op_accuracy_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_accuracy_assessment.

vivo:has_accuracy_assertion a vivo:op_accuracy_assessment;
   rdfs:domain vivo:accuracy_assessment;
   rdfs:range vivo:accuracy_assertion;
   rdfs:subPropertyOf vivo:has_assertion.

vivo:requirement_assessment a owl:Class;
   rdfs:subClassOf vivo:requirement_entity, vivo:assessment, [owl:unionOf (vivo:event_assessment vivo:infrastructure_assessment vivo:model_assessment vivo:project_assessment vivo:workflow_assessment)];
   owl:intersectionOf (vivo:requirement_entity vivo:assessment);
   owl:equivalentClass evmpo:requirement_assessment.
vivo:op_requirement_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_requirement_assessment.
vivo:dp_requirement_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_requirement_assessment.
vivo:op_requirement_entity rdfs:subClassOf vivo:op_requirement_assessment.
vivo:dp_requirement_entity rdfs:subClassOf vivo:dp_requirement_assessment.
vivo:op_assessment rdfs:subClassOf vivo:op_requirement_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_requirement_assessment.

vivo:has_requirement_assertion a vivo:op_requirement_assessment;
   rdfs:domain vivo:requirement_assessment;
   rdfs:range vivo:requirement_assertion;
   rdfs:subPropertyOf vivo:has_assertion.

vivo:review_assessment a owl:Class;
   rdfs:subClassOf vivo:review_entity, vivo:assessment;
   owl:intersectionOf (vivo:review_entity vivo:assessment).
vivo:op_review_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_review_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_review_entity rdfs:subClassOf vivo:op_review_assessment.
vivo:dp_review_entity rdfs:subClassOf vivo:dp_review_assessment.
vivo:op_assessment rdfs:subClassOf vivo:op_review_assessment.
vivo:dp_assessment rdfs:subClassOf vivo:dp_review_assessment.

vivo:has_review_assertion a vivo:op_review_assessment;
   rdfs:domain vivo:review_assessment;
   rdfs:range vivo:review_assertion;
   rdfs:subPropertyOf vivo:has_assertion.
vivo:has_stars a vivo:dp_review_assessment;
   rdfs:domain vivo:review_assessment;
   rdfs:range xs:decimal;
   rdfs:comment "By convention, the number of stars will normally be an integer between zero and five; in special circumstances, exceptions (non-integer, or negative, or six stars, etc.) may be justified."^^xs:string.

vivo:endorsement_assessment a owl:Class;
   rdfs:subClassOf vivo:review_assessment;
   owl:equivalentClass evmpo:endorsement_assessment.
vivo:op_endorsement_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_endorsement_assessment.
vivo:dp_endorsement_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_endorsement_assessment.
vivo:op_review_assessment rdfs:subClassOf vivo:op_endorsement_assessment.
vivo:dp_review_assessment rdfs:subClassOf vivo:dp_endorsement_assessment.


################################
# COMMUNICATION BRANCH OF VIVO #
################################

vivo:validation_statement a owl:Class;
   rdfs:subClassOf vico:statement, vico:validation_communication;
   owl:intersectionOf (vico:validation_communication vico:statement);
   rdfs:comment "validation statement, i.e., a statement which, as its main feature or purpose, relates to validation"^^xs:string.
vivo:op_validation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_validation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vico:op_validation_communication rdfs:subClassOf vivo:op_validation_statement.
vico:dp_validation_communication rdfs:subClassOf vivo:dp_validation_statement.
vico:op_statement rdfs:subClassOf vivo:op_validation_statement.
vico:dp_statement rdfs:subClassOf vivo:dp_validation_statement.

vivo:validation_statement owl:disjointWith vivo:property, vivo:validation_infrastructure.

vivo:certificate a owl:Class;
   rdfs:subClassOf vivo:validation_statement;
   rdfs:comment "certificate, i.e., a validation statement by which an assessment is stated"^^xs:string.
vivo:op_certificate a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_certificate a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_statement rdfs:subClassOf vivo:op_certificate.
vivo:dp_validation_statement rdfs:subClassOf vivo:dp_certificate.

vivo:has_certifier a vivo:op_certificate;
   rdfs:domain vivo:certificate;
   rdfs:range cviii:certifier;
   owl:inverseOf cviii:is_certifier_of;
   rdfs:comment "connects a certificate to the certifier who is responsible for the included assessment (not necessarily identical with the author who communicates the certificate)"^^xs:string.
vivo:states_assessment a vivo:op_certificate;
   rdfs:domain vivo:certificate;
   rdfs:range vivo:assessment.

vivo:certificate rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty vivo:has_certifier;
      owl:someValuesFrom cviii:certifier
   ], [
      a owl:Restriction;
      owl:onProperty vico:states_assessment;
      owl:someValuesFrom vivo:assessment
   ].


#################################
# INFRASTRUCTURE BRANCH OF VIVO #
#################################

vivo:validation_infrastructure a owl:Class;
   rdfs:subClassOf vivo:validation_entity, evmpo:infrastructure, macro:infrastructure;
   owl:intersectionOf (vivo:validation_entity evmpo:infrastructure);
   rdfs:comment "validation infrastructure, i.e., an infrastructure which, as its main feature or purpose, relates to validation"^^xs:string.
vivo:op_validation_infrastructure a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_validation_infrastructure a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_entity rdfs:subClassOf vivo:op_validation_infrastructure.
vivo:dp_validation_entity rdfs:subClassOf vivo:dp_validation_infrastructure.
evmpo:op_infrastructure rdfs:subClassOf vivo:op_validation_infrastructure.
evmpo:dp_infrastructure rdfs:subClassOf vivo:dp_validation_infrastructure.
macro:op_infrastructure rdfs:subClassOf vivo:op_validation_infrastructure.
macro:dp_infrastructure rdfs:subClassOf vivo:dp_validation_infrastructure.

vivo:validation_infrastructure owl:disjointWith vivo:assertion, vivo:assessment, vivo:property, vivo:validation_statement.

vivo:validation_channel a owl:Class;
   rdfs:subClassOf vivo:validation_infrastructure, macro:channel;
   owl:intersectionOf (vivo:validation_entity macro:channel).
vivo:op_validation_channel a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_validation_channel a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_infrastructure rdfs:subClassOf vivo:op_validation_channel.
vivo:dp_validation_infrastructure rdfs:subClassOf vivo:dp_validation_channel.
macro:op_channel rdfs:subClassOf vivo:op_validation_channel.
macro:dp_channel rdfs:subClassOf vivo:dp_validation_channel.

vivo:validation_repository a owl:Class;
   rdfs:subClassOf vivo:validation_infrastructure, macro:repository, evmpo:agent, cviii:agent;
   owl:intersectionOf (vivo:validation_entity macro:repository).
vivo:op_validation_repository a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_validation_repository a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_validation_infrastructure rdfs:subClassOf vivo:op_validation_repository.
vivo:dp_validation_infrastructure rdfs:subClassOf vivo:dp_validation_repository.
macro:op_repository rdfs:subClassOf vivo:op_validation_repository.
macro:dp_repository rdfs:subClassOf vivo:dp_validation_repository.
evmpo:op_agent rdfs:subClassOf vivo:op_validation_repository.
evmpo:dp_agent rdfs:subClassOf vivo:dp_validation_repository.
cviii:op_agent rdfs:subClassOf vivo:op_validation_repository.
cviii:dp_agent rdfs:subClassOf vivo:dp_validation_repository.


###########################
# PROPERTY BRANCH OF VIVO #
###########################

vivo:property a owl:Class;
   rdfs:subClassOf vivo:validation_entity;
   owl:equivalentClass evmpo:property;  # EVMPO/EMMO property;
   rdfs:comment "property as defined by the EVMPO and the EMMO"^^xs:string.
vivo:op_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_property.
vivo:dp_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_property.
evmpo:op_paradigmatic_entity rdfs:subClassOf vivo:op_property.
evmpo:dp_paradigmatic_entity rdfs:subClassOf vivo:dp_property.

vivo:property owl:disjointWith vivo:assertion, vivo:assessment, vivo:validation_statement, vivo:validation_infrastructure.

vivo:material_property a owl:Class;
   rdfs:subClassOf vivo:property, osmo:logical_variable;
   owl:equivalentClass evmpo:material_property;
   rdfs:comment "thermodynamic or mechanical behaviour of a material (referring either to the real material itself or a model thereof)"^^xs:string.
vivo:op_material_property a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_material_property a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
vivo:op_property rdfs:subClassOf vivo:op_material_property.
vivo:dp_property rdfs:subClassOf vivo:dp_material_property.
osmo:op_logical_variable rdfs:subClassOf vivo:op_material_property.
osmo:dp_logical_variable rdfs:subClassOf vivo:dp_material_property.
evmpo:op_material_property owl:equivalentClass vivo:op_material_property.
evmpo:dp_material_property owl:equivalentClass vivo:dp_material_property.

vivo:is_quantity_kind a vivo:op_material_property, owl:FunctionalProperty;
   rdfs:domain vivo:material_property;
   rdfs:range qudt:QuantityKind;
   rdfs:comment "points to the physical property characterization following QUDT"^^xs:string.


################################################################
# ANNOTATION: ADDITIONAL UNITS DEFINED IN ACCORDANCE WITH QUDT #
################################################################

vivo:unit a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   owl:equivalentClass qudt:Unit;
   rdfs:comment "this class will be used to define units (individuals of the unit class) that are absent from qudt-unit"^^xs:string.
vivo:op_unit a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vivo:dp_unit a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf vivo:op_unit.
evmpo:dp_annotation rdfs:subClassOf vivo:dp_unit.

vivo:UNIT_DEBYE_ANGSTROM a vivo:unit, qudt:ElectricityAndMagnetismUnit;
   qudt:conversionMultiplier 3.33564e-40;
   qudt:conversionOffset 0.0;
   qudt:symbol "DA"^^xs:string;
   qudt:description "Debye Angstrom, i.e., a quadrupole-moment unit, corresponding to 3.33564e-40 C m^2, also referred to as Buckingham"^^xs:string.
qudt:ElectricityAndMagnetismUnit rdfs:subClassOf vivo:unit.
vivo:UNIT_KELVIN_BOLTZMANN a vivo:unit, qudt:EnergyAndWorkUnit;
   qudt:conversionMultiplier 1.38065e-23;
   qudt:conversionOffset 0.0;
   qudt:symbol "K k_B"^^xs:string;
   qudt:description "Kelvin x Boltzmann constant, i.e., an energy unit, corresponding to 1.38065e-23 J"^^xs:string.
qudt:EnergyAndWorkUnit rdfs:subClassOf vivo:unit.
vivo:UNIT_U a vivo:unit, qudt:MassUnit;  # note: this exists within qudt, but there is a bug in the definition
   qudt:conversionMultiplier 1.660539e-27;
   qudt:conversionOffset 0.0;
   qudt:symbol "u"^^xs:string;
   qudt:description "Unit of mass corresponding to 1 g/mol, also referred to as Dalton"^^xs:string.
qudt:MassUnit rdfs:subClassOf vivo:unit.



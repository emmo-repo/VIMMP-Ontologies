
### ONTOLOGY FOR SIMULATION, MODELLING, AND OPTIMIZATION (OSMO) ###

### VERSION 0.2.3 ###

### DATED 12TH MARCH 2019 ###

@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix qudt: <http://qudt.org/schema/qudt#>.
@prefix quq: <http://purl.oclc.org/NET/ssnx/qu/quantity#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/osmo/osmo.ttl#> a owl:Ontology.

osmo:model_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass evmpo:model_related_entity;
   rdfs:comment "model or non-paradigmatic aspect of a model"^^xs:string.
osmo:op_model_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model_related_entity.
osmo:dp_model_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model_related_entity.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_model_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_model_related_entity.

osmo:model_related_entity owl:disjointUnionOf (osmo:model osmo:model_annotation).

osmo:constrained_model_related_entity a owl:Class;
   rdfs:subClassOf osmo:model_related_entity.

osmo:model a owl:Class;
   rdfs:subClassOf osmo:model_related_entity, evmpo:model_related_entity;
   owl:equivalentClass evmpo:model;
   rdfs:comment "model, i.e., a model as characterized by the EMMO"^^xs:string.
osmo:op_model a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model.
osmo:dp_model a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model.
evmpo:op_model_related_entity rdfs:subClassOf osmo:op_model.
evmpo:dp_model_related_entity rdfs:subClassOf osmo:dp_model.
osmo:op_model_related_entity rdfs:subClassOf osmo:op_model.
osmo:dp_model_related_entity rdfs:subClassOf osmo:dp_model.

osmo:model_annotation a owl:Class;
   rdfs:subClassOf osmo:model_related_entity, evmpo:model_related_entity;
   owl:equivalentClass evmpo:model_annotation;
   rdfs:comment "model annotation, i.e., a non-paradigmatic aspect of a model"^^xs:string.
osmo:op_model_annotation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_model_annotation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_model_related_entity rdfs:subClassOf osmo:op_model_annotation.
evmpo:dp_model_related_entity rdfs:subClassOf osmo:dp_model_annotation.
osmo:op_model_related_entity rdfs:subClassOf osmo:op_model_annotation.
osmo:dp_model_related_entity rdfs:subClassOf osmo:dp_model_annotation.

osmo:simulation_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass evmpo:simulation_related_entity;
   rdfs:comment "simulation or non-paradigmatic aspect of a simulation"^^xs:string.
osmo:op_simulation_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_simulation_related_entity.
osmo:dp_simulation_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_simulation_related_entity.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_simulation_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_simulation_related_entity.

osmo:simulation_related_entity owl:disjointUnionOf (osmo:simulation osmo:simulation_annotation).

osmo:constrained_simulation_related_entity a owl:Class;
   rdfs:subClassOf osmo:simulation_related_entity.

osmo:simulation a owl:Class;
   rdfs:subClassOf osmo:simulation_related_entity, evmpo:simulation_related_entity;
   owl:equivalentClass evmpo:simulation;
   rdfs:comment "simulation, i.e., a simulation workflow (including a single simulation as special case)"^^xs:string.
osmo:op_simulation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_simulation.
osmo:dp_simulation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_simulation.
evmpo:op_simulation_related_entity rdfs:subClassOf osmo:op_simulation.
evmpo:dp_simulation_related_entity rdfs:subClassOf osmo:dp_simulation.
osmo:op_simulation_related_entity rdfs:subClassOf osmo:op_simulation.
osmo:dp_simulation_related_entity rdfs:subClassOf osmo:dp_simulation.

osmo:simulation_annotation a owl:Class;
   rdfs:subClassOf osmo:simulation_related_entity, evmpo:simulation_related_entity;
   owl:equivalentClass evmpo:simulation_annotation;
   rdfs:comment "simulation annotation, i.e., a non-paradigmatic aspect of a simulation"^^xs:string.
osmo:op_simulation_annotation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_simulation_annotation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_simulation_related_entity rdfs:subClassOf osmo:op_simulation_annotation.
evmpo:dp_simulation_related_entity rdfs:subClassOf osmo:dp_simulation_annotation.
osmo:op_simulation_related_entity rdfs:subClassOf osmo:op_simulation_annotation.
osmo:dp_simulation_related_entity rdfs:subClassOf osmo:dp_simulation_annotation.

osmo:material_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass evmpo:material_related_entity;
   rdfs:comment "EMMO material or non-paradigmatic aspect of an EMMO material"^^xs:string.
osmo:op_material_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_material_related_entity.
osmo:dp_material_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_material_related_entity.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_material_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_material_related_entity.

osmo:material_related_entity owl:disjointUnionOf (evmpo:material osmo:material_annotation).

osmo:material_annotation a owl:Class;
   rdfs:subClassOf osmo:material_related_entity, evmpo:material_related_entity;
   owl:equivalentClass evmpo:material_annotation;
   rdfs:comment "non-paradigmatic aspect of a material as defined by the EMMO"^^xs:string.
osmo:op_material_annotation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_material_annotation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_material_related_entity rdfs:subClassOf osmo:op_material_annotation.
evmpo:dp_material_related_entity rdfs:subClassOf osmo:dp_material_annotation.
osmo:op_material_related_entity rdfs:subClassOf osmo:op_material_annotation.
osmo:dp_material_related_entity rdfs:subClassOf osmo:dp_material_annotation.


osmo:moda_structure_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "moda structure entity, i.e., a model, simulation, or aspects thereof that can be represented by a section or a combination of sections in MODA"^^xs:string.
osmo:op_moda_structure_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_structure_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_moda_structure_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_moda_structure_entity.

osmo:moda_structure_entity owl:disjointUnionOf (osmo:moda_section osmo:moda_graph).

osmo:moda_section owl:intersectionOf (osmo:moda_structure_entity osmo:model_related_entity).
osmo:moda_graph owl:intersectionOf (osmo:moda_structure_entity osmo:simulation_related_entity).

osmo:moda_section_entity a owl:Class;
   rdfs:subClassOf osmo:model_related_entity;
   rdfs:comment "MODA section entity, i.e., a MODA section or an aspect thereof"^^xs:string.
osmo:op_moda_section_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_section_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_model_related_entity rdfs:subClassOf osmo:op_moda_section_entity.
osmo:dp_model_related_entity rdfs:subClassOf osmo:dp_moda_section_entity.

osmo:moda_section_entity owl:disjointUnionOf (osmo:moda_section osmo:moda_section_aspect).

osmo:moda_section a owl:Class;
   rdfs:subClassOf osmo:moda_structure_entity, osmo:moda_section_entity;
   rdfs:comment "MODA section, i.e., a user case, model, solver, or processor that can be represented by a section in MODA"^^xs:string.
osmo:op_moda_section a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_section a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_structure_entity rdfs:subClassOf osmo:op_moda_section.
osmo:dp_moda_structure_entity rdfs:subClassOf osmo:dp_moda_section.
osmo:op_moda_section_entity rdfs:subClassOf osmo:op_moda_section.
osmo:dp_moda_section_entity rdfs:subClassOf osmo:dp_moda_section.

osmo:moda_section owl:disjointUnionOf (osmo:moda_usercase osmo:moda_model osmo:moda_solver osmo:moda_processor).

osmo:has_moda_aspect a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_section_aspect;
   rdfs:subPropertyOf evmpo:has_annotation;
   rdfs:comment "points to an aspect associated with the respective MODA section"^^xs:string.
osmo:has_result_property a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range vivo:physical_property;
   rdfs:comment "points to desired physical (thermodynamic or mechanical) information, which is not given and needs to be determined"^^xs:string.

osmo:has_boundary_condition a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_boundary_condition;
   rdfs:subPropertyOf osmo:has_moda_aspect;
   rdfs:comment "points to any given information that needs to be taken into account"^^xs:string.
osmo:has_geometry_information a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_geometry_information;
   rdfs:subPropertyOf osmo:has_boundary_condition.
osmo:has_material_information a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_material_information;
   rdfs:subPropertyOf osmo:has_moda_aspect.
osmo:has_moda_section_description a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_section_description;
   rdfs:subPropertyOf osmo:has_moda_aspect.
osmo:has_section_literature a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_section_literature;
   rdfs:subPropertyOf osmo:has_moda_aspect.
osmo:has_timespan a osmo:op_moda_section;
   rdfs:domain osmo:moda_section;
   rdfs:range osmo:moda_timespan;
   rdfs:subPropertyOf osmo:has_moda_aspect.

osmo:moda_section_aspect a owl:Class;
   rdfs:subClassOf osmo:moda_section_entity, evmpo:annotation;
   rdfs:comment "MODA section aspect, i.e., an aspect of a user case, model, solver, or processor that can be represented by a section in MODA"^^xs:string.
osmo:op_moda_section_aspect a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_section_aspect a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_entity rdfs:subClassOf osmo:op_moda_section_aspect.
osmo:dp_moda_section_entity rdfs:subClassOf osmo:dp_moda_section_aspect.
evmpo:op_annotation rdfs:subClassOf osmo:op_moda_section_aspect.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_moda_section_aspect.

osmo:has_moda_string_content a osmo:dp_moda_section_aspect;
   rdfs:domain osmo:moda_section_aspect;
   rdfs:range xs:string;
   rdfs:comment "plain-text description of an aspect/subsection according to MODA"^^xs:string.
osmo:has_moda_decimal_content a osmo:dp_moda_section_aspect;
   rdfs:domain osmo:moda_section_aspect;
   rdfs:range xs:decimal;
   rdfs:comment "numerical value associated with an aspect/subsection according to MODA"^^xs:string.

osmo:moda_boundary_condition a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect;
   rdfs:comment "MODA boundary condition, i.e., any given information that needs to be taken into account"^^xs:string.
osmo:op_moda_boundary_condition a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_boundary_condition a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_boundary_condition.
osmo:dp_moda_section_aspect rdfs:subClassOf osmo:dp_moda_boundary_condition.

osmo:moda_geometry_information a owl:Class;
   rdfs:subClassOf osmo:moda_boundary_condition;
   rdfs:comment "MODA geometry information, i.e., information on the geometry considered by a MODA section (note: this is not the geometry, but information about the geometry)"^^xs:string.
osmo:op_moda_geometry_information a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_geometry_information a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_boundary_condition rdfs:subClassOf osmo:op_moda_geometry_information.
osmo:dp_moda_boundary_condition rdfs:subClassOf osmo:dp_moda_geometry_information.

osmo:PERIODIC_BOUNDARY_CONDITION a osmo:moda_geometry_information.

osmo:moda_material_information a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect;
   rdfs:comment "MODA material information, i.e., information on the material considered by a MODA section (note: this is not the material, but information about the material)"^^xs:string.
osmo:op_moda_material_information a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_material_information a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_material_information.
osmo:dp_moda_section_aspect rdfs:subClassOf osmo:dp_moda_material_information.

osmo:moda_section_description a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect;
   rdfs:comment "MODA section description, i.e., any descriptive-type information concerning a MODA section"^^xs:string.
osmo:op_moda_section_description a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_section_description a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_section_description.
osmo:dp_moda_section_aspect rdfs:subClassOf osmo:dp_moda_section_description.

osmo:moda_section_literature a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect;
   rdfs:comment "MODA section literature, i.e., a literature reference associated with a MODA section"^^xs:string.
osmo:op_moda_section_literature a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_section_literature a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_section_literature.
osmo:dp_moda_section_aspect rdfs:subClassOf osmo:dp_moda_section_literature.

osmo:moda_timespan a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect;
   rdfs:comment "MODA time span, i.e., the time interval considered, modelled, or represented by a MODA section"^^xs:string.
osmo:op_moda_timespan a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_timespan a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_timespan rdfs:subClassOf osmo:op_moda_section_description.
osmo:dp_moda_timespan rdfs:subClassOf osmo:dp_moda_section_description.

osmo:moda_usercase_entity a owl:Class;
   rdfs:subClassOf osmo:moda_section_entity;
   rdfs:comment "MODA user case entity, i.e., a user case that can be described by MODA section 1, or an aspect thereof"^^xs:string.
osmo:op_moda_usercase_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_usercase_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_entity rdfs:subClassOf osmo:op_moda_usercase_entity.
osmo:dp_moda_section_entity rdfs:subClassOf osmo:dp_moda_usercase_entity.

osmo:moda_usercase_entity owl:disjointUnionOf (osmo:moda_usercase osmo:moda_usercase_aspect).

osmo:moda_usercase a owl:Class;
   rdfs:subClassOf osmo:moda_section, osmo:moda_usercase_entity, osmo:model_annotation;
   rdfs:comment "MODA user case, i.e., a user case that can be described by MODA section 1"^^xs:string.
osmo:op_moda_usercase a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_usercase a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section rdfs:subClassOf osmo:op_moda_usercase.
osmo:dp_moda_section rdfs:subClassOf osmo:dp_moda_usercase.
osmo:op_moda_usercase_entity rdfs:subClassOf osmo:op_moda_usercase.
osmo:dp_moda_usercase_entity rdfs:subClassOf osmo:dp_moda_usercase.
osmo:op_model_annotation rdfs:subClassOf osmo:op_moda_usercase.
osmo:dp_model_annotation rdfs:subClassOf osmo:dp_moda_usercase.

osmo:has_moda_usercase_aspect a osmo:op_moda_usercase;
   rdfs:domain osmo:moda_usercase;
   rdfs:range osmo:moda_usercase_aspect;
   rdfs:subPropertyOf osmo:has_moda_aspect;
   rdfs:comment "points to an aspect associated with MODA section 1"^^xs:string.

osmo:moda_usercase_aspect a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect, osmo:moda_usercase_entity;
   rdfs:comment "MODA user case aspect, i.e., an aspect of a user case that can be described by MODA section 1"^^xs:string.
osmo:op_moda_usercase_aspect a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_usercase_aspect a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_usercase_aspect.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:dp_moda_usercase_aspect.
osmo:op_moda_usercase_entity rdfs:subClassOf osmo:op_moda_usercase_aspect.
osmo:op_moda_usercase_entity rdfs:subClassOf osmo:dp_moda_usercase_aspect.

osmo:moda_usercase_aspect owl:intersectionOf (osmo:moda_section_aspect osmo:moda_usercase_entity).

osmo:moda_model_entity a owl:Class;
   rdfs:subClassOf osmo:moda_section_entity;
   rdfs:comment "MODA model entity, i.e., a model that can be described by MODA section 2, or an aspect thereof"^^xs:string.
osmo:op_moda_model_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_model_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_entity rdfs:subClassOf osmo:op_moda_model_entity.
osmo:dp_moda_section_entity rdfs:subClassOf osmo:dp_moda_model_entity.

osmo:moda_model_entity owl:disjointUnionOf (osmo:moda_model osmo:moda_model_aspect).

osmo:moda_model a owl:Class;
   rdfs:subClassOf osmo:moda_section, osmo:model, osmo:moda_model_entity;
   rdfs:comment "MODA model, i.e., a model that can be described by MODA section 2"^^xs:string.
osmo:op_moda_model a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_model a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section rdfs:subClassOf osmo:op_moda_model.
osmo:dp_moda_section rdfs:subClassOf osmo:dp_moda_model.
osmo:op_model rdfs:subClassOf osmo:op_moda_model.
osmo:dp_model rdfs:subClassOf osmo:dp_moda_model.
osmo:op_moda_model_entity rdfs:subClassOf osmo:op_moda_model.
osmo:dp_moda_model_entity rdfs:subClassOf osmo:dp_moda_model.

osmo:has_moda_model_aspect a osmo:op_moda_model;
   rdfs:domain osmo:moda_model;
   rdfs:range osmo:moda_model_aspect;
   rdfs:subPropertyOf osmo:has_moda_aspect;
   rdfs:comment "points to an aspect associated with MODA section 2"^^xs:string.

osmo:moda_model_aspect a owl:Class;
   rdfs:subClassOf osmo:moda_section_aspect, osmo:moda_model_entity, osmo:model_annotation;
   rdfs:comment "MODA model aspect, i.e., an aspect of a model that can be described by MODA section 2"^^xs:string.
osmo:op_moda_model_aspect a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_model_aspect a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:op_moda_model_aspect.
osmo:op_moda_section_aspect rdfs:subClassOf osmo:dp_moda_model_aspect.
osmo:op_moda_model_entity rdfs:subClassOf osmo:op_moda_model_aspect.
osmo:op_moda_model_entity rdfs:subClassOf osmo:dp_moda_model_aspect.
osmo:op_model_annotation rdfs:subClassOf osmo:op_moda_model_aspect.
osmo:op_model_annotation rdfs:subClassOf osmo:dp_moda_model_aspect.

osmo:moda_model_aspect owl:intersectionOf (osmo:moda_section_aspect osmo:moda_model_entity).

osmo:moda_solver_entity a owl:Class;
   rdfs:subClassOf osmo:moda_section_entity;
   rdfs:comment "MODA solver entity, i.e., a solver that can be described by MODA section 3, or an aspect thereof"^^xs:string.
osmo:op_moda_solver_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_solver_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section_entity rdfs:subClassOf osmo:op_moda_solver_entity.
osmo:dp_moda_section_entity rdfs:subClassOf osmo:dp_moda_solver_entity.



### ONTOLOGY FOR SIMULATION, MODELLING, AND OPTIMIZATION (OSMO) ###

### VERSION 0.4.1 ###

### DATED 11TH JUNE 2019 ###

@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix qudt: <http://qudt.org/schema/qudt#>.
@prefix quq: <http://purl.oclc.org/NET/ssnx/qu/quantity#>.
@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/osmo/osmo.ttl#> a owl:Ontology.

osmo:constrained_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity.

osmo:model a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:model;
   rdfs:comment "model, i.e., a model as characterized by the EMMO"^^xs:string.
osmo:op_model a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model.
osmo:dp_model a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model.
evmpo:op_paradigmatic_entity rdfs:subClassOf osmo:op_model.
evmpo:dp_paradigmatic_entity rdfs:subClassOf osmo:dp_model.

osmo:simulation a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:simulation;
   rdfs:comment "simulation, i.e., a simulation workflow (including a single simulation as special case)"^^xs:string.
osmo:op_simulation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_simulation.
osmo:dp_simulation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_simulation.
evmpo:op_paradigmatic_entity rdfs:subClassOf osmo:op_simulation.
evmpo:dp_paradigmatic_entity rdfs:subClassOf osmo:dp_simulation.

osmo:model owl:disjointWith osmo:simulation.


osmo:materials_model_classifier a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "materials model classifier, i.e., a symbol for categories of materials-model related entities, following RoMM (note: these are descriptors, not identical with the described entities)"^^xs:string.
osmo:op_materials_model_classifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_materials_model_classifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf osmo:op_materials_model_classifier.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_materials_model_classifier.

osmo:granularity_level a owl:Class;
   rdfs:subClassOf osmo:materials_model_classifier;
   owl:oneOf (osmo:ELECTRONIC osmo:ATOMISTIC osmo:MESOSCOPIC osmo:CONTINUUM);
   rdfs:comment "granularity level (from RoMM), i.e., electronic, atomistic, mesoscopic, or continuum level according to RoMM"^^xs:string.
osmo:op_granularity_level a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_granularity_level a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_materials_model_classifier rdfs:subClassOf osmo:op_granularity_level.
osmo:dp_materials_model_classifier rdfs:subClassOf osmo:dp_granularity_level.

osmo:ELECTRONIC a osmo:granularity_level;
   rdfs:comment "granularity level label: electronic materials modelling"^^xs:string.
osmo:ATOMISTIC a osmo:granularity_level;
   rdfs:comment "granularity level label: atomistic materials modelling"^^xs:string.
osmo:MESOSCOPIC a osmo:granularity_level;
   rdfs:comment "granularity level label: mesoscopic materials modelling"^^xs:string.
osmo:CONTINUUM a osmo:granularity_level;
   rdfs:comment "granularity level label: continuum materials modelling"^^xs:string.

osmo:physics_equation_type a owl:Class;
   rdfs:subClassOf osmo:materials_model_classifier;
   rdfs:comment "physics equation type, i.e., a symbol referring to the physical equation underlying a model, mostly following RoMM (n.b., these entities are not models, they are descriptors of models)"^^xs:string.
osmo:op_physics_equation_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_physics_equation_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_materials_model_classifier rdfs:subClassOf osmo:op_physics_equation_type.
osmo:dp_materials_model_classifier rdfs:subClassOf osmo:dp_physics_equation_type.

osmo:has_granularity_level a osmo:op_physics_equation_type;
   rdfs:domain osmo:physics_equation_type;
   rdfs:range osmo:granularity_level;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
osmo:has_pe_type_descriptor a osmo:dp_physics_equation_type;
   rdfs:domain osmo:physics_equation_type;
   rdfs:range xs:string.

osmo:R_HAS_ONE_GRANULARITY_LEVEL a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_granularity_level;
   owl:cardinality 1.
osmo:R_HAS_A_PE_TYPE_DESCRIPTOR a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_pe_type_descriptor;
   owl:someValuesFrom xs:string.
osmo:physics_equation_type rdfs:subClassOf osmo:R_HAS_ONE_GRANULARITY_LEVEL, osmo:R_HAS_A_PE_TYPE_DESCRIPTOR.

osmo:physics_equation_type owl:disjointUnionOf (osmo:pe_type_electronic osmo:pe_type_atomistic osmo:pe_type_mesoscopic osmo:pe_type_continuum).

osmo:pe_type_electronic a owl:Class;
   rdfs:subClassOf osmo:physics_equation_type;
   rdfs:comment "electronic physics equation type, i.e., a PE type at the electronic granularity level"^^xs:string.
osmo:op_pe_type_electronic a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_physics_equation_type rdfs:subClassOf osmo:op_pe_type_electronic.
osmo:dp_physics_equation_type rdfs:subClassOf osmo:dp_pe_type_electronic.

osmo:R_HAS_ELECTRONIC_GRANULARITY_LEVEL a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_granularity_level;
   owl:hasValue osmo:ELECTRONIC.
osmo:pe_type_electronic
   owl:intersectionOf (osmo:physics_equation_type osmo:R_HAS_ELECTRONIC_GRANULARITY_LEVEL);
   owl:disjointUnionOf (
      osmo:pe_type_electronic_qm_abinitio         # RoMM category 1.1, OSMO PE type E.1
      osmo:pe_type_electronic_manybody_effective  # RoMM category 1.2, OSMO PE type E.2
      osmo:pe_type_electronic_time_dependent      # RoMM category 1.3, OSMO PE type E.3
      osmo:pe_type_electronic_charge_transport    # RoMM category 1.4, OSMO PE type E.4
      osmo:pe_type_electronic_spin_transport      # RoMM category 1.5, OSMO PE type E.5
   ).

osmo:pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type E.1: Ab-initio quantum mechanical (or first principle) model"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.1, OSMO PE type E.1"^^xs:string.
osmo:op_pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_qm_abinitio a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_qm_abinitio.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_qm_abinitio.

osmo:pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type E.2: Electronic many-body and/or effective Hamiltonian model"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.2, OSMO PE type E.2"^^xs:string.
osmo:op_pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_manybody_effective a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_manybody_effective.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_manybody_effective.

osmo:pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type E.3: QM model of the response to time-dependent fields"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.3, OSMO PE type E.3"^^xs:string.
osmo:op_pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_time_dependent a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_time_dependent.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_time_dependent.

osmo:pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type E.4: Statistical charge transport model"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.4, OSMO PE type E.4"^^xs:string.
osmo:op_pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_charge_transport a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_charge_transport.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_charge_transport.

osmo:pe_type_electronic_spin_transport a owl:Class;
   rdfs:subClassOf osmo:pe_type_electronic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type E.5: Statistical electron spin transport model"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 1.5, OSMO PE type E.5"^^xs:string.
osmo:op_pe_type_electronic_spin_transport a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_electronic_spin_transport a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_electronic rdfs:subClassOf osmo:op_pe_type_electronic_spin_transport.
osmo:dp_pe_type_electronic rdfs:subClassOf osmo:dp_pe_type_electronic_spin_transport.

osmo:pe_type_atomistic a owl:Class;
   rdfs:subClassOf osmo:physics_equation_type;
   rdfs:comment "atomistic physics equation type, i.e., a PE type that at the atomistic granularity level"^^xs:string.
osmo:op_pe_type_atomistic a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_atomistic a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_physics_equation_type rdfs:subClassOf osmo:op_pe_type_atomistic.
osmo:dp_physics_equation_type rdfs:subClassOf osmo:dp_pe_type_atomistic.

osmo:R_HAS_ATOMISTIC_GRANULARITY_LEVEL a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_granularity_level;
   owl:hasValue osmo:ATOMISTIC.
osmo:pe_type_atomistic
   owl:intersectionOf (osmo:physics_equation_type osmo:R_HAS_ATOMISTIC_GRANULARITY_LEVEL);
   owl:disjointUnionOf (
      osmo:pe_type_atomistic_density_functional     # RoMM category 2.1, OSMO PE type A.1
      osmo:pe_type_atomistic_molecular_statics      # RoMM category 2.2, OSMO PE type A.2
      osmo:pe_type_atomistic_molecular_dynamics     # RoMM category 2.3, OSMO PE type A.3
      osmo:pe_type_atomistic_partition_function     # RoMM category 2.4, OSMO PE type A.4
      osmo:pe_type_atomistic_spin_model             # RoMM category 2.5, OSMO PE type A.5
      osmo:pe_type_atomistic_statistical_transport  # RoMM categories 2.6 and 2.7, OSMO PE type A.6
   ).

osmo:pe_type_atomistic_density_functional a owl:Class;
   rdfs:subClassOf osmo:pe_type_atomistic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type A.1: Classical-mechanical density functional theory (atomistic)"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 2.1, OSMO PE type A.1"^^xs:string.
osmo:op_pe_type_atomistic_density_functional a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_atomistic_density_functional a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_atomistic rdfs:subClassOf osmo:op_pe_type_atomistic_density_functional.
osmo:dp_pe_type_atomistic rdfs:subClassOf osmo:dp_pe_type_atomistic_density_functional.

osmo:pe_type_atomistic_molecular_statics a owl:Class;
   rdfs:subClassOf osmo:pe_type_atomistic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type A.2: Molecular statics (atomistic), energy minimization"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 2.2, OSMO PE type A.2"^^xs:string.
osmo:op_pe_type_atomistic_molecular_statics a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_atomistic_molecular_statics a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_atomistic rdfs:subClassOf osmo:op_pe_type_atomistic_molecular_statics.
osmo:dp_pe_type_atomistic rdfs:subClassOf osmo:dp_pe_type_atomistic_molecular_statics.


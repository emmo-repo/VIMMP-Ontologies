<http://www.vimmp.eu/semantics/osmo/osmo.ttl#> a owl:Ontology.

osmo:model_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass evmpo:model_related_entity;
   rdfs:comment "model or non-paradigmatic aspect of a model"^^xs:string.
osmo:op_model_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model_related_entity.
osmo:dp_model_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model_related_entity.
evmpo:op_marketplace_related_entity rdfs:subClassOf osmo:op_model_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf osmo:dp_model_related_entity.

osmo:model_related_entity owl:disjointUnionOf (osmo:model osmo:model_annotation).

osmo:constrained_model_related_entity a owl:Class;
   rdfs:subClassOf osmo:model_related_entity.

osmo:model a owl:Class;
   rdfs:subClassOf osmo:model_related_entity, evmpo:model_related_entity;
   owl:equivalentClass evmpo:model;
   rdfs:comment "model, i.e., a model as characterized by the EMMO"^^xs:string.
osmo:op_model a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model.
osmo:dp_model a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model.
evmpo:op_model_related_entity rdfs:subClassOf osmo:op_model.
evmpo:dp_model_related_entity rdfs:subClassOf osmo:dp_model.
osmo:op_model_related_entity rdfs:subClassOf osmo:op_model.
osmo:dp_model_related_entity rdfs:subClassOf osmo:dp_model.

osmo:model_annotation a owl:Class;
   rdfs:subClassOf osmo:model_related_entity, evmpo:model_related_entity;
   owl:equivalentClass evmpo:model_annotation;
   rdfs:comment "model annotation, i.e., a non-paradigmatic aspect of a model"^^xs:string.
osmo:op_model_annotation a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_model_annotation a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_model_related_entity rdfs:subClassOf osmo:op_model_annotation.
evmpo:dp_model_related_entity rdfs:subClassOf osmo:dp_model_annotation.
osmo:op_model_related_entity rdfs:subClassOf osmo:op_model_annotation.
osmo:dp_model_related_entity rdfs:subClassOf osmo:dp_model_annotation.

osmo:simulation_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   owl:equivalentClass evmpo:simulation_related_entity;

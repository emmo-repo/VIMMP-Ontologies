osmo:moda_solver a owl:Class;
   rdfs:subClassOf osmo:moda_section.
osmo:op_moda_solver a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_solver a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_section rdfs:subClassOf osmo:op_moda_solver.
osmo:dp_moda_section rdfs:subClassOf osmo:dp_moda_solver.

osmo:has_solver_type a osmo:op_moda_solver;
   rdfs:domain osmo:moda_solver;
   rdfs:range osmo:moda_solver_type;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
osmo:is_tool a osmo:dp_moda_solver;
   rdfs:domain osmo:moda_solver;
   rdfs:range xs:string.
osmo:has_computational_representation a osmo:dp_moda_solver;
   rdfs:domain osmo:moda_solver;
   rdfs:range xs:string.
osmo:has_parameter_descriptor a osmo:dp_moda_solver;
   rdfs:domain osmo:moda_solver;
   rdfs:range xs:string.

osmo:moda_solver_entity a owl:Class;
   rdfs:subClassOf osmo:model_related_entity, osmo:model_annotation.

osmo:moda_solver_type a owl:Class;
   rdfs:subClassOf osmo:moda_solver_entity, evmpo:meta_annotation.

osmo:EOS_SINGLE_PHASE_SOLVER a osmo:moda_solver_type.
osmo:EOS_PHASE_EQUILIBRIUM_SOLVER a osmo:moda_solver_type.
osmo:MOLECULAR_DYNAMICS_SOLVER a osmo:moda_solver_type.
osmo:MONTE_CARLO_SOLVER a osmo:moda_solver_type.

osmo:PERIODIC_BOUNDARY_CONDITION a osmo:moda_boundary_condition.


osmo:moda_processor a owl:Class;
   rdfs:subClassOf osmo:moda_section.

osmo:moda_trajectory_analyser a owl:Class;
   rdfs:subClassOf osmo:moda_processor.

osmo:moda_property_selector a owl:Class;
   rdfs:subClassOf osmo:moda_processor.


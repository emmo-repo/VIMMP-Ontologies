osmo:pe_type_continuum a owl:Class;
   rdfs:subClassOf osmo:physical_equation_type;
   rdfs:comment "continuum physical equation type, i.e., a PE type that operates at the continuum granularity level"^^xs:string.
osmo:op_pe_type_continuum a owl:ObjectProperty.
osmo:dp_pe_type_continuum a owl:DatatypeProperty.
osmo:op_pe_type_continuum rdfs:subPropertyOf osmo:op_physical_equation_type.
osmo:dp_pe_type_continuum rdfs:subPropertyOf osmo:dp_physical_equation_type.

osmo:pe_type_continuum
   owl:intersectionOf (osmo:physical_equation_type [a owl:Restriction; owl:onProperty osmo:has_granularity_level; owl:hasValue osmo:CONTINUUM]);
   owl:disjointUnionOf (
      osmo:pe_type_continuum_solid_mechanics    # RoMM category 4.1, OSMO PE type identifier CO.1
      osmo:pe_type_continuum_fluid_mechanics    # RoMM category 4.2, OSMO PE type identifier CO.2
      osmo:pe_type_continuum_heat_transfer      # RoMM category 4.3, OSMO PE type identifier CO.3
      osmo:pe_type_continuum_phase_field        # RoMM category 4.4.2, OSMO PE type identifier CO.4
      osmo:pe_type_continuum_thermodynamics     # RoMM category 4.4.1, OSMO PE type identifier CO.5
      osmo:pe_type_continuum_reaction_kinetics  # RoMM category 4.5, OSMO PE type identifier CO.6
      osmo:pe_type_continuum_electromagnetism   # RoMM category 4.6, OSMO PE type identifier CO.7
      osmo:pe_type_continuum_process_model      # RoMM category 4.7, OSMO PE type identifier CO.8
   ).

osmo:pe_type_continuum_solid_mechanics a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.1: Continuum solid mechanics"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.1"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.1, OSMO PE type CO.1"^^xs:string.
osmo:op_pe_type_continuum_solid_mechanics a owl:ObjectProperty.
osmo:dp_pe_type_continuum_solid_mechanics a owl:DatatypeProperty.
osmo:op_pe_type_continuum_solid_mechanics rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_solid_mechanics rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_fluid_mechanics a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.2: Continuum fluid mechanics"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.2"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.2, OSMO PE type CO.2"^^xs:string.
osmo:op_pe_type_continuum_fluid_mechanics a owl:ObjectProperty.
osmo:dp_pe_type_continuum_fluid_mechanics a owl:DatatypeProperty.
osmo:op_pe_type_continuum_fluid_mechanics rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_fluid_mechanics rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_heat_transfer a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.3: Continuum modelling of heat transfer and thermomechanics"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.3"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.3, OSMO PE type CO.3"^^xs:string.
osmo:op_pe_type_continuum_heat_transfer a owl:ObjectProperty.
osmo:dp_pe_type_continuum_heat_transfer a owl:DatatypeProperty.
osmo:op_pe_type_continuum_heat_transfer rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_heat_transfer rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_phase_field a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.4: Phase field model or continuum density gradient theory"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.4"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.4.2, OSMO PE type CO.4"^^xs:string.
osmo:op_pe_type_continuum_phase_field a owl:ObjectProperty.
osmo:dp_pe_type_continuum_phase_field a owl:DatatypeProperty.
osmo:op_pe_type_continuum_phase_field rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_phase_field rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_thermodynamics a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.5: Continuum thermodynamics"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.5"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.4.1, OSMO PE type CO.5"^^xs:string.
osmo:op_pe_type_continuum_thermodynamics a owl:ObjectProperty.
osmo:dp_pe_type_continuum_thermodynamics a owl:DatatypeProperty.
osmo:op_pe_type_continuum_thermodynamics rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_thermodynamics rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_reaction_kinetics a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "OSMO PE type CO.6: Continuum modelling of chemical reaction kinetics"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.6"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.5, OSMO PE type CO.6"^^xs:string.
osmo:op_pe_type_continuum_reaction_kinetics a owl:ObjectProperty.
osmo:dp_pe_type_continuum_reaction_kinetics a owl:DatatypeProperty.
osmo:op_pe_type_continuum_reaction_kinetics rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_reaction_kinetics rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_electromagnetism a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.7: Continuum electromagnetism model (including optics, magnetism, etc.)"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.7"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.6, OSMO PE type CO.7"^^xs:string.
osmo:op_pe_type_continuum_electromagnetism a owl:ObjectProperty.
osmo:dp_pe_type_continuum_electromagnetism a owl:DatatypeProperty.
osmo:op_pe_type_continuum_electromagnetism rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_electromagnetism rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:pe_type_continuum_process_model a owl:Class;
   rdfs:subClassOf osmo:pe_type_continuum;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type CO.8: Continuum process model (including devices and components of chemical/mechanical processes, flow-chart models, etc.)"^^xs:string];
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_id;
      owl:hasValue "CO.8"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 4.7, OSMO PE type CO.8"^^xs:string.
osmo:op_pe_type_continuum_process_model a owl:ObjectProperty.
osmo:dp_pe_type_continuum_process_model a owl:DatatypeProperty.
osmo:op_pe_type_continuum_process_model rdfs:subPropertyOf osmo:op_pe_type_continuum.
osmo:dp_pe_type_continuum_process_model rdfs:subPropertyOf osmo:dp_pe_type_continuum.

osmo:workflow_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "workflow entity, i.e., an element or aspect that can occur in a simulation workflow structure"^^xs:string.
osmo:op_workflow_entity a owl:ObjectProperty.
osmo:dp_workflow_entity a owl:DatatypeProperty.
osmo:op_workflow_entity rdfs:subPropertyOf evmpo:op_marketplace_related_entity.
osmo:dp_workflow_entity rdfs:subPropertyOf evmpo:dp_marketplace_related_entity.

osmo:workflow_entity owl:disjointUnionOf (osmo:logical_entity osmo:section_entity osmo:workflow_graph).

osmo:workflow_resource a owl:Class;
   rdfs:subClassOf osmo:workflow_entity;
   rdfs:comment "workflow resource, i.e., a workflow entity that can be contained as a node-like element or as a subgraph by a workflow graph"^^xs:string.
osmo:op_workflow_resource a owl:ObjectProperty.
osmo:dp_workflow_resource a owl:DatatypeProperty.
osmo:op_workflow_resource rdfs:subPropertyOf osmo:op_workflow_entity.
osmo:dp_workflow_resource rdfs:subPropertyOf osmo:dp_workflow_entity.

osmo:has_resource_label a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_workflow_resource;
   rdfs:domain osmo:workflow_entity;
   rdfs:range xs:string;
   rdfs:comment "label to be used, e.g., in graph representations and for other visualization purposes"^^xs:string.

osmo:workflow_resource owl:disjointUnionOf (osmo:logical_resource osmo:section osmo:workflow_graph).

osmo:virtual_graph a owl:Class;
   rdfs:subClassOf osmo:workflow_graph, [
      a owl:Restriction; owl:onProperty osmo:is_concrete; owl:hasValue "false"^^xs:boolean
   ];
   owl:intersectionOf (osmo:workflow_graph [a owl:Restriction; owl:onProperty osmo:is_virtual; owl:hasValue "true"^^xs:boolean]);
   rdfs:comment "virtual graph, i.e., a workflow graph that is variably instantiated based on workflow-dependent quantities; e.g., a virtual graph might be instantiated multiple times in parallel or sequentially, or the way in which it is instantiated might qualitatively depend on input parameters in a significant way, going beyond the conventional role of an input parameter (n.b., virtual graphs do not contain anything; instead, they are instantiated by concrete graphs that contain further elements)"^^xs:string.
osmo:op_virtual_graph a owl:ObjectProperty.
osmo:dp_virtual_graph a owl:DatatypeProperty.
osmo:op_virtual_graph rdfs:subPropertyOf osmo:op_workflow_graph.
osmo:dp_virtual_graph rdfs:subPropertyOf osmo:dp_workflow_graph.

osmo:contingent_graph a owl:Class;
   rdfs:subClassOf osmo:virtual_graph;
   rdfs:comment "contingent graph, i.e., a virtual graph that is only present/active if one or multiple conditions are fulfilled"^^xs:string.
osmo:op_contingent_graph a owl:ObjectProperty.
osmo:dp_contingent_graph a owl:DatatypeProperty.
osmo:op_contingent_graph rdfs:subPropertyOf osmo:op_virtual_graph.
osmo:dp_contingent_graph rdfs:subPropertyOf osmo:dp_virtual_graph.

osmo:is_contingent_on a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_contingent_graph;
   rdfs:domain osmo:contingent_graph;
   rdfs:range osmo:condition;
   rdfs:comment "whenever one or several conditions are not fulfilled, a resource that is contingent on them is absent/inactive"^^xs:string.

osmo:contingent_graph rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:is_contingent_on;
      owl:someValuesFrom osmo:condition
   ].

osmo:indexed_graph a owl:Class;
   rdfs:subClassOf osmo:virtual_graph;
   rdfs:comment "indexed graph, i.e., a virtual graph that is instantiated multiple times, with an index"^^xs:string.
osmo:op_indexed_graph a owl:ObjectProperty.
osmo:dp_indexed_graph a owl:DatatypeProperty.
osmo:op_indexed_graph rdfs:subPropertyOf osmo:op_virtual_graph.
osmo:dp_indexed_graph rdfs:subPropertyOf osmo:dp_virtual_graph.

osmo:has_coupled_instances a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_indexed_graph;
   rdfs:domain osmo:indexed_graph;
   rdfs:range xs:boolean;
   rdfs:comment "indicates whether any of the multiple instantiations of this resource are coupled (synchronized) with each other"^^xs:string.
osmo:has_linked_instances a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_indexed_graph;
   rdfs:domain osmo:indexed_graph;
   rdfs:range xs:boolean;
   rdfs:comment "indicates whether any of the multiple instantiations of this resource are sequentially linked to each other"^^xs:string.

osmo:has_index a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_indexed_graph;
   rdfs:domain osmo:indexed_graph;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to the logical variable/index to which multiple values are assigned"^^xs:string.
osmo:has_index_unit a owl:ObjectProperty, owl:FunctionalProperty;
   rdfs:subPropertyOf osmo:op_indexed_graph;
   rdfs:domain osmo:indexed_graph;
   rdfs:range qudt:Unit, vivo:unit;
   rdfs:comment "unit in which the index values are given (not required; in many typical cases, the index is dimensionless)"^^xs:string;
   rdfs:subPropertyOf evmpo:has_annotation.
osmo:has_index_value a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_indexed_graph;
   rdfs:domain osmo:indexed_graph;
   rdfs:range xs:decimal;
   rdfs:comment "points to each of the multiple values that are given to the index"^^xs:string.

osmo:indexed_graph rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_index;
      owl:cardinality 1
   ], [
      a owl:Restriction;
      owl:onProperty osmo:has_index_value;
      owl:minCardinality 2
   ].

osmo:section_entity a owl:Class;
   rdfs:subClassOf osmo:workflow_entity;
   rdfs:comment "section entity, i.e., an OSMO/MODA section or an aspect thereof"^^xs:string.
osmo:op_section_entity a owl:ObjectProperty.
osmo:dp_section_entity a owl:DatatypeProperty.
osmo:op_section_entity rdfs:subPropertyOf osmo:op_workflow_entity.
osmo:dp_section_entity rdfs:subPropertyOf osmo:dp_workflow_entity.

osmo:section_entity owl:disjointUnionOf (osmo:application_case_entity osmo:model_entity osmo:solver_entity osmo:processor_entity).

osmo:section a owl:Class;
   rdfs:subClassOf osmo:workflow_resource, osmo:section_entity;
   rdfs:comment "section, i.e., a use case, model, solver, or processor (as in MODA)"^^xs:string.
osmo:op_section a owl:ObjectProperty.
osmo:dp_section a owl:DatatypeProperty.
osmo:op_section rdfs:subPropertyOf osmo:op_workflow_resource.
osmo:dp_section rdfs:subPropertyOf osmo:dp_workflow_resource.
osmo:op_section rdfs:subPropertyOf osmo:op_section_entity.
osmo:dp_section rdfs:subPropertyOf osmo:dp_section_entity.

osmo:section owl:disjointUnionOf (osmo:application_case osmo:materials_model osmo:solver osmo:processor).

osmo:has_internal_lv a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to information that plays a role within the respective OSMO/MODA section"^^xs:string.
osmo:has_input_format a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range macro:io_format;
   rdfs:subPropertyOf osmo:has_io_format;
   owl:propertyChainAxiom (osmo:has_logical_input osmo:is_encoded_by).
osmo:has_io_format a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range macro:io_format;
   rdfs:comment "points to a format employed for I/O in the technical implementation of the respective OSMO/MODA section"^^xs:string;
   rdfs:subPropertyOf evmpo:has_annotation;
   owl:propertyChainAxiom (osmo:has_logical_io osmo:is_encoded_by).
osmo:has_logical_input a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to information that needs to be provided"^^xs:string;
   rdfs:subPropertyOf osmo:has_logical_io.
osmo:has_logical_io a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to information required or produced by a OSMO/MODA section"^^xs:string.
osmo:has_logical_output a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:logical_variable;
   rdfs:comment "points to information that is obtained as a result from the respective OSMO/MODA section"^^xs:string;
   rdfs:subPropertyOf osmo:has_logical_io.
osmo:has_output_format a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range macro:io_format;
   rdfs:subPropertyOf osmo:has_io_format;
   owl:propertyChainAxiom (osmo:has_logical_output osmo:is_encoded_by).
osmo:has_section_label a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_section;
   rdfs:domain osmo:section;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_resource_label.

osmo:has_physical_input a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range vivo:material_property, osmo:logical_variable, evmpo:material_property;
   rdfs:subPropertyOf osmo:has_logical_input;
   rdfs:comment "points to required physical (thermodynamic or mechanical) information, which needs to be provided, e.g., from the output of another OSMO/MODA section"^^xs:string.
osmo:has_internal_mp a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range vivo:material_property, osmo:logical_variable, evmpo:material_property;
   rdfs:subPropertyOf osmo:has_internal_lv;
   rdfs:comment "points to physical information that plays a role within the respective OSMO/MODA section"^^xs:string.
osmo:has_physical_output a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range vivo:material_property, osmo:logical_variable, evmpo:material_property;
   rdfs:subPropertyOf osmo:has_logical_output;
   rdfs:comment "points to desired physical (thermodynamic or mechanical) information which is obtained as a result from the respective OSMO/MODA section"^^xs:string.

osmo:has_aspect a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:section_aspect;
   rdfs:comment "points to an aspect associated with the respective OSMO/MODA section"^^xs:string;
   rdfs:subPropertyOf evmpo:has_annotation.

osmo:has_boundary_condition a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:boundary_condition;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_employed_software a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:software;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_error_statement a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:error_statement.
osmo:has_geometry_information a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:geometry_information;
   rdfs:subPropertyOf osmo:has_boundary_condition.
osmo:has_material_information a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:material_information;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_method_type a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:method_type;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_section_description a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:section_description;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_section_literature a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:section_literature;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_section_parameter a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:parameter_aspect;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_temporal_discretization a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:temporal_discretization;
   rdfs:subPropertyOf osmo:has_aspect.
osmo:has_timespan a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section;
   rdfs:range osmo:timespan_information;
   rdfs:subPropertyOf osmo:has_aspect.

osmo:applies_to a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section;
   rdfs:domain osmo:section, [a owl:Class; owl:unionOf (osmo:use_case osmo:materials_model)];
   rdfs:range osmo:workflow_graph;
   rdfs:comment "points to the workflow (sub)graph to which a particular use case or materials model applies"^^xs:string;
   owl:inverseOf osmo:considers.

osmo:section_aspect a owl:Class;
   rdfs:subClassOf osmo:section_entity;
   rdfs:comment "section aspect, i.e., an aspect of a use case, model, solver, or processor that can be represented by a section"^^xs:string.
osmo:op_section_aspect a owl:ObjectProperty.
osmo:dp_section_aspect a owl:DatatypeProperty.
osmo:op_section_aspect rdfs:subPropertyOf osmo:op_section_entity.
osmo:dp_section_aspect rdfs:subPropertyOf osmo:dp_section_entity.

osmo:has_aspect_text_content a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_section_aspect;
   rdfs:domain osmo:section_aspect;
   rdfs:range xs:string;
   rdfs:comment "plain-text description of an aspect/subsection (as in MODA)"^^xs:string.
osmo:has_aspect_scalar_content a owl:DatatypeProperty;
   rdfs:subPropertyOf osmo:dp_section_aspect;
   rdfs:domain osmo:section_aspect;
   rdfs:range xs:decimal;
   rdfs:comment "numerical value associated with an aspect/subsection (as in MODA)"^^xs:string.

osmo:has_aspect_object_content a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section_aspect;
   rdfs:domain osmo:section_aspect;
   rdfs:range evmpo:marketplace_related_entity;
   rdfs:comment "points to an object associated with an aspect/subsection"^^xs:string;
   owl:inverseOf osmo:is_aspect_content_of.
osmo:has_aspect_annotation_content a owl:ObjectProperty;
   rdfs:subPropertyOf osmo:op_section_aspect;
   rdfs:domain osmo:section_aspect;

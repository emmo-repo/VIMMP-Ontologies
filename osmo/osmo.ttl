osmo:has_cause a osmo:op_workflow_graph, owl:TransitiveProperty;
   rdfs:domain osmo:workflow_graph;
   rdfs:range osmo:workflow_graph;
   rdfs:comment "[G has_cause F] means that in every execution, F needs to finish before G can begin"^^xs:string.

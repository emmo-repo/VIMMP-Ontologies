osmo:op_moda_node a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_moda_node a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_moda_graph rdfs:subClassOf osmo:op_moda_node.
osmo:dp_moda_graph rdfs:subClassOf osmo:dp_moda_node.

osmo:R_CONTAINS_ONE_SECTION a owl:Restriction;

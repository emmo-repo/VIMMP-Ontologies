   rdfs:subClassOf osmo:pe_type_mesoscopic;
   rdfs:subClassOf [a owl:Restriction;
      owl:onProperty osmo:has_pe_type_descriptor;
      owl:hasValue "PE type M.5: Micromagnetism model"^^xs:string];
   rdfs:comment "RoMM model vocabulary category 3.4, OSMO PE type M.5"^^xs:string.
osmo:op_pe_type_mesoscopic_micromagnetism a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_mesoscopic_micromagnetism a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_pe_type_mesoscopic rdfs:subClassOf osmo:op_pe_type_mesoscopic_micromagnetism.
osmo:dp_pe_type_mesoscopic rdfs:subClassOf osmo:dp_pe_type_mesoscopic_micromagnetism.

osmo:pe_type_mesoscopic_statistical_transport a owl:Class;

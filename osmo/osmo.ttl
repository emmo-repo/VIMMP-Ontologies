osmo:unit_function a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "Unit function, i.e., purpose or impact of one out of multiple coexisting units of the same or similar type"^^xs:string.
osmo:op_unit_function a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_unit_function a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf osmo:op_unit_function.
evmpo:dp_annotation rdfs:subClassOf osmo:dp_unit_function.

osmo:has_uf_descriptor a osmo:dp_unit_function;
   rdfs:domain osmo:unit_function;
   rdfs:range xs:string.
osmo:refers_to_unit a osmo:op_unit_function;
   rdfs:domain osmo:unit_function;
   rdfs:range evmpo:marketplace_related_entity;
   owl:inverseOf osmo:has_unit_function.

osmo:R_HAS_A_UF_DESCRIPTOR a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:has_uf_descriptor;
   owl:someValuesFrom xs:string.
osmo:R_REFERS_TO_A_UNIT a owl:Restriction;
   rdfs:subClassOf osmo:constrained_entity;
   owl:onProperty osmo:refers_to_unit;
   owl:someValuesFrom evmpo:marketplace_related_entity.
osmo:unit_function rdfs:subClassOf osmo:R_HAS_A_UF_DESCRIPTOR, osmo:R_REFERS_TO_A_UNIT.

osmo:has_unit_function a evmpo:op_marketplace_related_entity, owl:ObjectProperty;
   rdfs:domain evmpo:marketplace_related_entity;
   rdfs:range osmo:unit_function;
   rdfs:subPropertyOf evmpo:has_annotation;
   owl:inverseOf osmo:refers_to_unit.

osmo:material_information_uf a owl:Class;
   rdfs:subClassOf osmo:unit_function;
   rdfs:comment "Unit function of an MI"^^xs:string.
osmo:op_material_information_uf a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_material_information_uf a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_unit_function rdfs:subClassOf osmo:op_material_information_uf.
osmo:dp_unit_function rdfs:subClassOf osmo:dp_material_information_uf.


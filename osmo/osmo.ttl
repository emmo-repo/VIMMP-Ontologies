   rdfs:comment "RoMM model vocabulary category 3.2, OSMO PE type M.3"^^xs:string.
osmo:op_pe_type_mesoscopic_molecular_dynamics a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
osmo:dp_pe_type_mesoscopic_molecular_dynamics a owl:Class;

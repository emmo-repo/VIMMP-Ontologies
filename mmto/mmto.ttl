   ].

mmto:translation_statement a owl:Class;
   rdfs:subClassOf evmpo:statement, vico:translation_communication;
   owl:intersectionOf (vico:translation_communication evmpo:statement);
   rdfs:comment "translation statement, i.e., a EVMPO/VICO statement which, as its main feature or purpose, relates to translation"^^xs:string.
mmto:op_translation_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
vico:dp_translation_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.

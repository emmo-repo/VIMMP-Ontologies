
### MATERIALS MODELLING TRANSLATION ONTOLOGY (MMTO) ###

### VERSION 0.6.5 ###

### DATED 12TH DECEMBER 2019 ###

@prefix mmto: <http://www.vimmp.eu/semantics/mmto/mmto.ttl#>.

@prefix cao: <http://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/>.
@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix osmo: <http://www.vimmp.eu/semantics/osmo/osmo.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.
@prefix vivo: <http://www.vimmp.eu/semantics/vivo/vivo.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/mmto/mmto.ttl#> a owl:Ontology.

mmto:translation_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "translation entity, i.e., a paradigmatic entity related to translating materials modelling problems to appropriate solutions"^^xs:string.
mmto:op_translation_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_translation_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf mmto:op_translation_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf mmto:dp_translation_entity.

mmto:project a owl:Class;
   rdfs:subClassOf mmto:translation_entity, evmpo:process;
   rdfs:comment "project, i.e., an undertaking of one or several stakeholders aiming at reaching a pre-specified objective within limited time"^^xs:string.
mmto:op_project a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_project a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_translation_entity rdfs:subClassOf mmto:op_project.
mmto:dp_translation_entity rdfs:subClassOf mmto:dp_project.
evmpo:op_process rdfs:subClassOf mmto:op_project.
evmpo:dp_process rdfs:subClassOf mmto:dp_project.

mmto:mm_project a owl:Class;
   rdfs:subClassOf mmto:project;
   rdfs:comment "materials modelling project, i.e., a project that aims at realizing a materials modelling workflow that can be represented by MODA/OSMO"^^xs:string.
mmto:op_mm_project a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_mm_project a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project rdfs:subClassOf mmto:op_mm_project.
mmto:dp_project rdfs:subClassOf mmto:dp_mm_project.

mmto:addresses a mmto:op_mm_project;
   rdfs:domain mmto:mm_project;
   rdfs:range osmo:application_case;
   owl:inverseOf osmo:is_addressed_by;
   rdfs:subPropertyOf evmpo:has_process_annotation;
   rdfs:comment "points to an application case that is addressed by the present materials modelling project"^^xs:string.

mmto:bc_entity a owl:Class;
   rdfs:subClassOf osmo:application_case_entity, mmto:translation_entity;
   rdfs:comment "business case entity, i.e., a business case or an aspect thereof"^^xs:string.
mmto:op_bc_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bc_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case_entity rdfs:subClassOf mmto:op_bc_entity.
osmo:dp_application_case_entity rdfs:subClassOf mmto:dp_bc_entity.
mmto:op_translation_entity rdfs:subClassOf mmto:op_bc_entity.
mmto:dp_translation_entity rdfs:subClassOf mmto:dp_bc_entity.

mmto:bc_entity owl:disjointUnionOf (mmto:business_case mmto:bc_aspect).

mmto:business_case a owl:Class;
   rdfs:subClassOf osmo:application_case, mmto:bc_entity;
   rdfs:comment "business case, i.e., an application case with a focus on economic/business aspects"^^xs:string.
mmto:op_business_case a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_business_case a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case rdfs:subClassOf mmto:op_business_case.
osmo:dp_application_case rdfs:subClassOf mmto:dp_business_case.
mmto:op_bc_entity rdfs:subClassOf mmto:op_business_case.
mmto:dp_bc_entity rdfs:subClassOf mmto:dp_business_case.

mmto:has_bc_summary a mmto:dp_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_application_case_summary;
   owl:propertyChainAxiom (mmto:has_bca_description osmo:has_aspect_text_content);
   rdfs:comment "plain-text summary of the business case"^^xs:string.
mmto:has_bc_aspect a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bc_aspect;
   rdfs:subPropertyOf osmo:has_application_case_aspect;
   rdfs:comment "points to a business-case aspect"^^xs:string.

mmto:has_bca_description a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_description;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to the BC aspect containing an abstract or a rough description of the BC in text form"^^xs:string.
mmto:has_bca_industrial_case a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_industrial_case;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on an industrial case associated with the present BC"^^xs:string.
mmto:has_bca_red_zone a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_red_zone;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on a red zone, i.e., operational or social constraint (contained in text form)"^^xs:string.
mmto:has_bca_context a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_context;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on the context of the BC (in text form); e.g., on revenue streams, market competitors, distribution channels, market shares, maximum time to solution, risk management, open/closed innovation, etc."^^xs:string.
mmto:has_bca_currency a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_currency;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on the budgeting currency"^^xs:string.
mmto:has_bca_contribution_to_cost a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_contribution_to_cost;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on a contribution to end-user cost (text plus value)"^^xs:string.
mmto:has_bca_total_cost a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_total_cost;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on the total end-user cost"^^xs:string.
mmto:has_bca_contribution_to_benefit a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_contribution_to_benefit;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on a contribution to end-user benefit (text plus value)"^^xs:string.
mmto:has_bca_total_benefit a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_total_benefit;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on the total end-user benefit"^^xs:string.
mmto:has_bca_return_on_investment a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_return_on_investment;
   rdfs:subPropertyOf mmto:has_bc_aspect;
   rdfs:comment "points to information on the return on investment for the end user"^^xs:string.
mmto:has_bca_decision_support a mmto:op_business_case;
   rdfs:domain mmto:business_case;
   rdfs:range mmto:bca_decision_support;
   rdfs:subPropertyOf mmto:has_bc_aspect, osmo:has_decision_support;
   rdfs:comment "points to information on the business decision support system"^^xs:string.

mmto:bc_aspect a owl:Class;
   rdfs:subClassOf osmo:application_case_aspect, mmto:bc_entity;
   rdfs:comment "business-case aspect"^^xs:string.
mmto:op_bc_aspect a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bc_aspect a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case_aspect rdfs:subClassOf mmto:op_bc_aspect.
osmo:dp_application_case_aspect rdfs:subClassOf mmto:dp_bc_aspect.
mmto:op_bc_entity rdfs:subClassOf mmto:op_bc_aspect.
mmto:dp_bc_entity rdfs:subClassOf mmto:dp_bc_aspect.

mmto:bc_aspect owl:intersectionOf (osmo:application_case_aspect mmto:bc_entity).

mmto:bca_description a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "aspect containing an abstract or a rough description of the BC in text form"^^xs:string.
mmto:op_bca_description a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_description a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_description.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_description.

mmto:bca_description rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_text_content;
      owl:someValuesFrom xs:string
   ].

mmto:bca_industrial_case a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on an industrial case associated with the present BC"^^xs:string.
mmto:op_bca_industrial_case a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_industrial_case a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_industrial_case.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_industrial_case.

mmto:bca_industrial_case rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_annotation_content;
      owl:someValuesFrom mmto:industrial_case
   ].

mmto:bca_red_zone a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on a red zone, i.e., operational or social constraint (contained in text form)"^^xs:string.
mmto:op_bca_red_zone a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_red_zone a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_red_zone.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_red_zone.

mmto:bca_red_zone rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_text_content;
      owl:someValuesFrom xs:string
   ].

mmto:bca_context a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on the context of the BC (in text form); e.g., on revenue streams, market competitors, distribution channels, market shares, maximum time to solution, risk management, open/closed innovation, etc."^^xs:string.
mmto:op_bca_context a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_context a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_context.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_context.

mmto:bca_context rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_text_content;
      owl:someValuesFrom xs:string
   ].

mmto:bca_currency a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on the budgeting currency"^^xs:string.
mmto:op_bca_currency a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_currency a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_currency.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_currency.

mmto:bca_currency rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_object_content;
      owl:someValuesFrom cao:Currency
   ].

mmto:bca_contribution_to_cost a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on a contribution to end-user cost (text explanation to be provided as text content, magnitude in budgeting currency to be provided as scalar content)"^^xs:string.
mmto:op_bca_contribution_to_cost a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_contribution_to_cost a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_contribution_to_cost.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_contribution_to_cost.

mmto:bca_contribution_to_cost rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_text_content;
      owl:someValuesFrom xs:string
   ], [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_scalar_content;
      owl:someValuesFrom xs:decimal
   ].

mmto:bca_total_cost a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on the total end-user cost (to be provided as a xs:decimal value content)"^^xs:string.
mmto:op_bca_total_cost a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_total_cost a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_total_cost.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_total_cost.

mmto:bca_total_cost rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_scalar_content;
      owl:someValuesFrom xs:decimal
   ].

mmto:bca_contribution_to_benefit a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on a contribution to end-user benefit (text explanation to be provided as text content, magnitude in budgeting currency to be provided as scalar content)"^^xs:string.
mmto:op_bca_contribution_to_benefit a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_contribution_to_benefit a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_contribution_to_benefit.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_contribution_to_benefit.

mmto:bca_contribution_to_benefit rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_text_content;
      owl:someValuesFrom xs:string
   ], [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_scalar_content;
      owl:someValuesFrom xs:decimal
   ].

mmto:bca_total_benefit a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on the total end-user benefit (to be provided as a xs:decimal value content)"^^xs:string.
mmto:op_bca_total_benefit a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_total_benefit a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_total_benefit.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_total_benefit.

mmto:bca_total_benefit rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_scalar_content;
      owl:someValuesFrom xs:decimal
   ].

mmto:bca_return_on_investment a owl:Class;
   rdfs:subClassOf mmto:bc_aspect;
   rdfs:comment "information on the return on investment for the end user (to be provided as a xs:decimal value content)"^^xs:string.
mmto:op_bca_return_on_investment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_return_on_investment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_return_on_investment.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_return_on_investment.

mmto:bca_return_on_investment rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_scalar_content;
      owl:someValuesFrom xs:decimal
   ].

mmto:bca_decision_support a owl:Class;
   rdfs:subClassOf mmto:bc_aspect, osmo:application_case_decision_support;
   rdfs:comment "information on the business decision support system (BDSS)"^^xs:string.
mmto:op_bca_decision_support a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_bca_decision_support a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_bc_aspect rdfs:subClassOf mmto:op_bca_decision_support.
mmto:dp_bc_aspect rdfs:subClassOf mmto:dp_bca_decision_support.
osmo:op_application_case_decision_support rdfs:subClassOf mmto:op_bca_decision_support.
osmo:dp_application_case_decision_support rdfs:subClassOf mmto:dp_bca_decision_support.

mmto:bca_decision_support rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_paradigmatic_content;
      owl:someValuesFrom mmto:business_decision_support_system
   ].

mmto:ic_entity a owl:Class;
   rdfs:subClassOf osmo:application_case_entity, mmto:translation_entity;
   rdfs:comment "industrial case entity, i.e., an industrial case or an aspect thereof"^^xs:string.
mmto:op_ic_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_ic_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case_entity rdfs:subClassOf mmto:op_ic_entity.
osmo:dp_application_case_entity rdfs:subClassOf mmto:dp_ic_entity.
mmto:op_translation_entity rdfs:subClassOf mmto:op_ic_entity.
mmto:dp_translation_entity rdfs:subClassOf mmto:dp_ic_entity.

mmto:ic_entity owl:disjointUnionOf (mmto:industrial_case mmto:ic_aspect).

mmto:industrial_case a owl:Class;
   rdfs:subClassOf osmo:application_case, mmto:ic_entity;
   rdfs:comment "industrial case, i.e., an application case with a focus on industrial/manufacturing aspects"^^xs:string.
mmto:op_industrial_case a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_industrial_case a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case rdfs:subClassOf mmto:op_industrial_case.
osmo:dp_application_case rdfs:subClassOf mmto:dp_industrial_case.
mmto:op_ic_entity rdfs:subClassOf mmto:op_industrial_case.
mmto:dp_ic_entity rdfs:subClassOf mmto:dp_industrial_case.

mmto:has_ic_summary a mmto:dp_industrial_case;
   rdfs:domain mmto:industrial_case;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_application_case_summary;
   rdfs:comment "plain-text summary of the industrial case"^^xs:string.
mmto:has_ic_aspect a mmto:op_industrial_case;
   rdfs:domain mmto:industrial_case;
   rdfs:range mmto:ic_aspect;
   rdfs:subPropertyOf osmo:has_application_case_aspect;
   rdfs:comment "points to an industrial-case aspect"^^xs:string.

mmto:has_ica_decision_support a mmto:op_industrial_case;
   rdfs:domain mmto:industrial_case;
   rdfs:range mmto:ica_decision_support;
   rdfs:subPropertyOf mmto:has_ic_aspect, osmo:has_decision_support;
   rdfs:comment "points to information on the industrial decision support system"^^xs:string.

mmto:ic_aspect a owl:Class;
   rdfs:subClassOf osmo:application_case_aspect, mmto:ic_entity;
   rdfs:comment "industrial-case aspect"^^xs:string.
mmto:op_ic_aspect a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_ic_aspect a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case_aspect rdfs:subClassOf mmto:op_ic_aspect.
osmo:dp_application_case_aspect rdfs:subClassOf mmto:dp_ic_aspect.
mmto:op_ic_entity rdfs:subClassOf mmto:op_ic_aspect.
mmto:dp_ic_entity rdfs:subClassOf mmto:dp_ic_aspect.

mmto:ic_aspect owl:intersectionOf (osmo:application_case_aspect mmto:ic_entity).

mmto:ica_decision_support a owl:Class;
   rdfs:subClassOf mmto:ic_aspect, osmo:application_case_decision_support;
   rdfs:comment "information on the industrial decision support system (IDSS)"^^xs:string.
mmto:op_ica_decision_support a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_ica_decision_support a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_ic_aspect rdfs:subClassOf mmto:op_ica_decision_support.
mmto:dp_ic_aspect rdfs:subClassOf mmto:dp_ica_decision_support.
osmo:op_application_case_decision_support rdfs:subClassOf mmto:op_ica_decision_support.
osmo:dp_application_case_decision_support rdfs:subClassOf mmto:dp_ica_decision_support.

mmto:ica_decision_support rdfs:subClassOf [
      a owl:Restriction;
      owl:onProperty osmo:has_aspect_paradigmatic_content;
      owl:someValuesFrom mmto:industrial_decision_support_system
   ].

mmto:tc_entity a owl:Class;
   rdfs:subClassOf osmo:application_case_entity, mmto:translation_entity;
   rdfs:comment "translation case entity, i.e., a translation case or an aspect thereof"^^xs:string.
mmto:op_tc_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_tc_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case_entity rdfs:subClassOf mmto:op_tc_entity.
osmo:dp_application_case_entity rdfs:subClassOf mmto:dp_tc_entity.
mmto:op_translation_entity rdfs:subClassOf mmto:op_tc_entity.
mmto:dp_translation_entity rdfs:subClassOf mmto:dp_tc_entity.

mmto:tc_entity owl:disjointUnionOf (mmto:translation_case mmto:tc_aspect).

mmto:translation_case a owl:Class;
   rdfs:subClassOf osmo:application_case, mmto:tc_entity;
   rdfs:comment "translation case, i.e., an application case that can be described as specified by the EMMC Translation Case Template"^^xs:string.
mmto:op_translation_case a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_translation_case a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
osmo:op_application_case rdfs:subClassOf mmto:op_translation_case.
osmo:dp_application_case rdfs:subClassOf mmto:dp_translation_case.
mmto:op_tc_entity rdfs:subClassOf mmto:op_translation_case.
mmto:dp_tc_entity rdfs:subClassOf mmto:dp_translation_case.

mmto:has_tc_summary a mmto:dp_translation_case;
   rdfs:domain mmto:translation_case;
   rdfs:range xs:string;
   rdfs:subPropertyOf osmo:has_application_case_summary;

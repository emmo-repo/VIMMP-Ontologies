mmto:opening_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;
   rdfs:comment "opening statement, i.e., a project statement by which a materials modelling project is initialized"^^xs:string.
mmto:op_opening_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_opening_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project_statement rdfs:subClassOf mmto:op_opening_statement.
mmto:dp_project_statement rdfs:subClassOf mmto:dp_opening_statement.

mmto:status_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;
   rdfs:comment "status statement, i.e., a project statement by which the status of an ongoing materials modelling project is communicated"^^xs:string.
mmto:op_status_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
mmto:dp_status_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
mmto:op_project_statement rdfs:subClassOf mmto:op_status_statement.
mmto:dp_project_statement rdfs:subClassOf mmto:dp_status_statement.

mmto:closing_statement a owl:Class;
   rdfs:subClassOf mmto:project_statement;

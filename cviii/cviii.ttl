
### CATEGORIZATION OF VIMMP INSTITUTIONS AND INDIVIDUALS (CVIII) ###

### VERSION 0.9.4 ###

### DATED 31ST OCTOBER 2019 ###

@prefix cviii: <http://www.vimmp.eu/semantics/cviii/cviii.ttl#>.

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.
@prefix lcc-cr: <https://www.omg.org/spec/LCC/Countries/CountryRepresentation/>.
@prefix macro: <http://www.vimmp.eu/semantics/macro/macro.ttl#>.
@prefix otras: <http://www.vimmp.eu/semantics/otras/otras.ttl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix vico: <http://www.vimmp.eu/semantics/vico/vico.ttl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://www.vimmp.eu/semantics/cviii/cviii.ttl#> a owl:Ontology.

cviii:role a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   owl:equivalentClass evmpo:role;
   rdfs:comment "role, i.e., a role as characterized by the EMMO"^^xs:string.
cviii:op_role a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_role.
cviii:dp_role a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_role.
evmpo:op_paradigmatic_entity rdfs:subClassOf cviii:op_role.
evmpo:dp_paradigmatic_entity rdfs:subClassOf cviii:dp_role.

cviii:agent a owl:Class;
   rdfs:subClassOf evmpo:role, cviii:role;
   rdfs:comment "agent, i.e., an entity that has (in principle) the potential of acting at a virtual marketplace, whether that capability is exercised or not"^^xs:string;
   owl:equivalentClass evmpo:agent.
cviii:op_agent a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_agent.
cviii:dp_agent a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_agent.
evmpo:op_role rdfs:subClassOf cviii:op_agent.
evmpo:dp_role rdfs:subClassOf cviii:dp_agent.
cviii:op_role rdfs:subClassOf cviii:op_agent.
cviii:dp_role rdfs:subClassOf cviii:dp_agent.

cviii:acts_at a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range macro:virtual_marketplace;
   owl:inverseOf macro:has_stakeholder;
   rdfs:subPropertyOf cviii:has_access_to.
cviii:has_access_to a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range macro:channel;
   owl:inverseOf macro:has_channel_member.
cviii:has_address a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range vico:address;
   rdfs:subPropertyOf evmpo:has_role_annotation.
cviii:has_agentid a cviii:dp_agent;
   rdfs:domain cviii:agent;
   rdfs:range xs:nonNegativeInteger.
cviii:has_interest a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range otras:topic;
   rdfs:subPropertyOf evmpo:has_role_annotation.
cviii:has_name a cviii:dp_agent;
   rdfs:domain cviii:agent;
   rdfs:range xs:string.
cviii:is_assignee_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range vico:message;
   owl:inverseOf vico:has_assignee.
cviii:is_author_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range vico:communication;
   owl:inverseOf vico:has_author.
cviii:is_listener_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range vico:message;
   owl:inverseOf vico:has_listener.
cviii:is_member_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range cviii:agent_assembly;
   owl:inverseOf cviii:has_member.
cviii:is_recipient_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range vico:interlocution;
   owl:inverseOf vico:has_recipient.
cviii:is_subscribed_to a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range macro:unidirectional_channel;
   owl:inverseOf macro:has_channel_subscriber;
   rdfs:subPropertyOf cviii:has_access_to.
cviii:is_owner_of a cviii:op_agent;
   rdfs:domain cviii:agent;
   rdfs:range macro:unidirectional_channel;
   owl:inverseOf macro:has_channel_owner;
   rdfs:subPropertyOf cviii:has_access_to.

cviii:agent owl:disjointUnionOf (cviii:agent_group cviii:institution cviii:person).

cviii:agent_assembly a owl:Class;
   rdfs:subClassOf cviii:agent;
   rdfs:comment "agent assembly, i.e., an agent entity that can have other agents as temporary or permanent constituent members"^^xs:string.
cviii:op_agent_assembly a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_agent_assembly a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_agent rdfs:subClassOf cviii:op_agent_assembly.
cviii:dp_agent rdfs:subClassOf cviii:dp_agent_assembly.

cviii:has_member a cviii:op_agent_assembly;
   rdfs:domain cviii:agent_assembly;
   rdfs:range cviii:agent;
   owl:inverseOf cviii:is_member_of.

cviii:agent_group a owl:Class;
   rdfs:subClassOf cviii:agent_assembly;
   rdfs:comment "agent group, i.e., a non-institutionalized agent assembly"^^xs:string;
   owl:equivalentClass evmpo:agent_group.
cviii:op_agent_group a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_agent_group.
cviii:dp_agent_group a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_agent_group.
cviii:op_agent_assembly rdfs:subClassOf cviii:op_agent_group.
cviii:dp_agent_assembly rdfs:subClassOf cviii:dp_agent_group.

cviii:institution a owl:Class;
   rdfs:subClassOf cviii:agent_assembly;
   rdfs:comment "institution, i.e., any permanent association, organization, office, or comparable entity that has the capability of acting at a virtual marketplace"^^xs:string;
   owl:equivalentClass evmpo:institution.
cviii:op_institution a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_institution.
cviii:dp_institution a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_institution.
cviii:op_agent_assembly rdfs:subClassOf cviii:op_institution.
cviii:dp_agent_assembly rdfs:subClassOf cviii:dp_institution.

cviii:has_institutional_address a cviii:op_institution;
   rdfs:domain cviii:institution;
   rdfs:range vico:address;
   rdfs:subPropertyOf cviii:has_address.

cviii:person a owl:Class;
   rdfs:subClassOf cviii:agent;
   rdfs:comment "person, i.e., a stand-alone agent that does not have multiple constituent parts or components that could by themselves act at a virtual marketplace"^^xs:string.
cviii:op_person a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_person a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_agent rdfs:subClassOf cviii:op_person.
cviii:dp_agent rdfs:subClassOf cviii:dp_person.

cviii:person owl:disjointUnionOf (cviii:human_agent cviii:virtual_agent).

cviii:has_affiliation a cviii:op_person;
   rdfs:domain cviii:person;
   rdfs:range cviii:institution;
   rdfs:subPropertyOf cviii:is_member_of.
cviii:has_business_address a cviii:op_person;
   rdfs:domain cviii:person;
   rdfs:range vico:address;
   rdfs:subPropertyOf cviii:has_personal_address.
cviii:has_personal_address a cviii:op_person;
   rdfs:domain cviii:person;
   rdfs:range vico:address;
   rdfs:subPropertyOf cviii:has_address.
cviii:has_profile_image a cviii:dp_person;
   rdfs:domain cviii:person;
   rdfs:range xs:anyURI.
cviii:has_profile_text a cviii:dp_person;
   rdfs:domain cviii:person;
   rdfs:range xs:string.
cviii:has_role_label a cviii:dp_person;
   rdfs:domain cviii:person;
   rdfs:range xs:string.

cviii:human_agent a owl:Class;
   rdfs:subClassOf cviii:person;
   rdfs:comment "human agent, i.e., a single human being whose identity is disclosed and who has the potential/ability to act at a virtual marketplace"^^xs:string;
   owl:equivalentClass evmpo:human_agent.
cviii:op_human_agent a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_human_agent.
cviii:dp_human_agent a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_human_agent.
cviii:op_person rdfs:subClassOf cviii:op_human_agent.
cviii:dp_person rdfs:subClassOf cviii:dp_human_agent.

cviii:has_academic_title a cviii:op_human_agent;
   rdfs:domain cviii:human_agent;
   rdfs:range cviii:academic_title;
   rdfs:subPropertyOf cviii:has_personal_title.
cviii:has_first_name a cviii:dp_human_agent;
   rdfs:domain cviii:human_agent;
   rdfs:range xs:string.
cviii:has_last_name a cviii:dp_human_agent;
   rdfs:domain cviii:human_agent;
   rdfs:range xs:string;
   rdfs:subPropertyOf cviii:has_name.
cviii:has_private_address a cviii:op_human_agent;
   rdfs:domain cviii:human_agent;
   rdfs:range vico:address;
   rdfs:subPropertyOf cviii:has_personal_address.
cviii:has_personal_title a cviii:op_human_agent;
   rdfs:domain cviii:human_agent;
   rdfs:range cviii:personal_title;
   rdfs:subPropertyOf evmpo:has_role_annotation.

cviii:virtual_agent a owl:Class;
   rdfs:subClassOf cviii:person;
   rdfs:comment "virtual agent, i.e., any person that is not a human agent (this includes bots/engines, agent pseudonyms, etc.)"^^xs:string.
cviii:op_virtual_agent a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_virtual_agent a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_person rdfs:subClassOf cviii:op_virtual_agent.
cviii:dp_person rdfs:subClassOf cviii:dp_virtual_agent.

cviii:stakeholder a owl:Class;
   rdfs:subClassOf cviii:agent;
   rdfs:comment "stakeholder, i.e., an agent that acts at one or multiple virtual marketplaces"^^xs:string;
   owl:equivalentClass evmpo:stakeholder.
cviii:op_stakeholder a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_stakeholder.
cviii:dp_stakeholder a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_stakeholder.
cviii:op_agent rdfs:subClassOf cviii:op_stakeholder.
cviii:dp_agent rdfs:subClassOf cviii:dp_stakeholder.

cviii:stakeholder owl:intersectionOf (cviii:agent [a owl:Restriction; owl:onProperty cviii:acts_at; owl:someValuesFrom macro:virtual_marketplace]).

cviii:expert a owl:Class;
   rdfs:subClassOf cviii:human_agent;
   owl:equivalentClass evmpo:expert;
   rdfs:comment "expert, i.e., a human agent who can provide competencies and expertise"^^xs:string.
cviii:op_expert a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_expert.
cviii:dp_expert a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_expert.
cviii:op_human_agent rdfs:subClassOf cviii:op_expert.
cviii:dp_human_agent rdfs:subClassOf cviii:dp_expert.

cviii:has_competency a cviii:op_expert;
   rdfs:domain cviii:expert;
   rdfs:range otras:expert_competency;
   rdfs:subPropertyOf evmpo:has_role_annotation.
cviii:has_expertise_in a cviii:op_expert;
   rdfs:domain cviii:expert;
   rdfs:range otras:topic;
   rdfs:subPropertyOf cviii:has_interest.

cviii:certifier a owl:Class;
   rdfs:subClassOf cviii:agent;
   rdfs:comment "certifier, i.e., an agent who can issue certificates (communications hat contain assessments)"^^xs:string.
cviii:op_certifier a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_certifier a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_agent rdfs:subClassOf cviii:op_certifier.
cviii:dp_agent rdfs:subClassOf cviii:dp_certifier.

cviii:is_certifier_of a cviii:op_certifier;
   rdfs:domain cviii:certifier;
   rdfs:range vico:certificate;
   owl:inverseOf vico:has_certifier.

cviii:certifier owl:disjointUnionOf (cviii:certifying_expert cviii:certifying_resource).

cviii:certifying_expert a owl:Class;
   rdfs:subClassOf cviii:certifier, cviii:expert;
   owl:intersectionOf (cviii:certifier cviii:expert);
   rdfs:comment "certifying expert, i.e., a certifier who is also an expert"^^xs:string.
cviii:op_certifying_expert a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_certifying_expert a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_certifier rdfs:subClassOf cviii:op_certifying_expert.
cviii:dp_certifier rdfs:subClassOf cviii:dp_certifying_expert.
cviii:op_expert rdfs:subClassOf cviii:op_certifying_expert.
cviii:dp_expert rdfs:subClassOf cviii:dp_certifying_expert.

cviii:certifying_resource a owl:Class;
   rdfs:subClassOf cviii:certifier;
   owl:intersectionOf (cviii:certifier [owl:complementOf cviii:human_agent]);
   rdfs:comment "certifying resource, i.e., a certifier who is not a human agent"^^xs:string.
cviii:op_certifying_resource a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_certifying_resource a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_certifier rdfs:subClassOf cviii:op_certifying_resource.
cviii:dp_certifier rdfs:subClassOf cviii:dp_certifying_resource.

cviii:is_at_uri a cviii:dp_certifying_resource;
   rdfs:domain cviii:certifying_resource;
   rdfs:range xs:anyURI.

cviii:certifying_resource owl:disjointWith cviii:human_agent.

cviii:certifying_journal_article a owl:Class;
   rdfs:subClassOf cviii:certifying_resource.
cviii:op_certifying_journal_article a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_certifying_journal_article a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_certifying_resource rdfs:subClassOf cviii:op_certifying_journal_article.
cviii:dp_certifying_resource rdfs:subClassOf cviii:dp_certifying_journal_article.

cviii:certifying_database a owl:Class;
   rdfs:subClassOf cviii:certifying_resource, macro:repository.
cviii:op_certifying_database a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_certifying_database a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_certifying_resource rdfs:subClassOf cviii:op_certifying_database.
cviii:dp_certifying_resource rdfs:subClassOf cviii:dp_certifying_database.
macro:op_repository rdfs:subClassOf cviii:op_certifying_database.
macro:dp_repository rdfs:subClassOf cviii:dp_certifying_database.

cviii:interlocutor a owl:Class;
   rdfs:subClassOf cviii:agent;
   rdfs:comment "interlocutor, i.e., an entity that can (potentially) send and receive communications at a virtual marketplace"^^xs:string.
cviii:op_interlocutor a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_interlocutor a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_agent rdfs:subClassOf cviii:op_interlocutor.
cviii:dp_agent rdfs:subClassOf cviii:dp_interlocutor.

cviii:marketplace_subject a owl:Class;
   rdfs:subClassOf cviii:interlocutor, evmpo:stakeholder;
   rdfs:comment "marketplace subject, i.e., an interlocutor who acts at one or multiple virtual marketplaces"^^xs:string.
cviii:op_marketplace_subject a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_marketplace_subject a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_interlocutor rdfs:subClassOf cviii:op_marketplace_subject.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_marketplace_subject.
evmpo:op_stakeholder rdfs:subClassOf cviii:op_marketplace_subject.
evmpo:dp_stakeholder rdfs:subClassOf cviii:dp_marketplace_subject.

cviii:marketplace_subject owl:intersectionOf (cviii:interlocutor evmpo:stakeholder).

cviii:consultant a owl:Class;
   rdfs:subClassOf cviii:interlocutor.
cviii:op_consultant a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_consultant a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_interlocutor rdfs:subClassOf cviii:op_consultant.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_consultant.

cviii:data_provider a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:data_provider.
cviii:op_data_provider a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_data_provider.
cviii:dp_data_provider a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_data_provider.
cviii:op_interlocutor rdfs:subClassOf cviii:op_data_provider.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_data_provider.

cviii:end_user a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:end_user.
cviii:op_end_user a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_end_user.
cviii:dp_end_user a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_end_user.
cviii:op_interlocutor rdfs:subClassOf cviii:op_end_user.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_end_user.

cviii:guest a owl:Class;
   rdfs:subClassOf cviii:marketplace_subject.
cviii:op_guest a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_guest a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_marketplace_subject rdfs:subClassOf cviii:op_guest.
cviii:dp_marketplace_subject rdfs:subClassOf cviii:dp_guest.

cviii:manufacturer a owl:Class;
   rdfs:subClassOf cviii:interlocutor.
cviii:op_manufacturer a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_manufacturer a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_interlocutor rdfs:subClassOf cviii:op_manufacturer.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_manufacturer.

cviii:model_provider a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:model_provider.
cviii:op_model_provider a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_model_provider.
cviii:dp_model_provider a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_model_provider.
cviii:op_interlocutor rdfs:subClassOf cviii:op_model_provider.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_model_provider.

cviii:software_owner a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:software_owner.
cviii:op_software_owner a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_software_owner.
cviii:dp_software_owner a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_software_owner.
cviii:op_interlocutor rdfs:subClassOf cviii:op_software_owner.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_software_owner.

cviii:training_provider a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:training_provider.
cviii:op_training_provider a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_training_provider.
cviii:dp_training_provider a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_training_provider.
cviii:op_interlocutor rdfs:subClassOf cviii:op_training_provider.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_training_provider.

cviii:translator a owl:Class;
   rdfs:subClassOf cviii:interlocutor;
   owl:equivalentClass evmpo:translator.
cviii:op_translator a owl:Class;
   rdfs:subClassOf owl:ObjectProperty;
   owl:equivalentClass evmpo:op_translator.
cviii:dp_translator a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty;
   owl:equivalentClass evmpo:dp_translator.
cviii:op_interlocutor rdfs:subClassOf cviii:op_translator.
cviii:dp_interlocutor rdfs:subClassOf cviii:dp_translator.

cviii:pseudonym a owl:Class;
   rdfs:subClassOf cviii:marketplace_subject, cviii:virtual_agent.
cviii:op_pseudonym a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_pseudonym a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_marketplace_subject rdfs:subClassOf cviii:op_pseudonym.
cviii:dp_marketplace_subject rdfs:subClassOf cviii:dp_pseudonym.
cviii:op_virtual_agent rdfs:subClassOf cviii:op_pseudonym.
cviii:dp_virtual_agent rdfs:subClassOf cviii:dp_pseudonym.

cviii:interlocutor_type a owl:Class;
   rdfs:subClassOf evmpo:annotation.
cviii:op_interlocutor_type a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_interlocutor_type a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf cviii:op_interlocutor_type.
evmpo:dp_annotation rdfs:subClassOf cviii:dp_interlocutor_type.

cviii:interlocutor_group a owl:Class;
   rdfs:subClassOf cviii:interlocutor_type.
cviii:op_interlocutor_group a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_interlocutor_group a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_interlocutor_type rdfs:subClassOf cviii:op_interlocutor_group.
cviii:dp_interlocutor_type rdfs:subClassOf cviii:dp_interlocutor_group.

cviii:interlocutor_group owl:oneOf (cviii:IG_CONSULTANT cviii:IG_DATA_PROVIDER cviii:IG_END_USER cviii:IG_GUEST cviii:IG_MANUFACTURER cviii:IG_MODEL_PROVIDER cviii:IG_SOFTWARE_OWNER cviii:IG_TRAINING_PROVIDER cviii:IG_TRANSLATOR).

cviii:interlocutor_tag a owl:Class;
   rdfs:subClassOf cviii:interlocutor_type.
cviii:op_interlocutor_tag a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_interlocutor_tag a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_interlocutor_type rdfs:subClassOf cviii:op_interlocutor_tag.
cviii:dp_interlocutor_type rdfs:subClassOf cviii:dp_interlocutor_tag.

cviii:has_interlocutor_tag a cviii:op_interlocutor;
   rdfs:domain cviii:interlocutor;
   rdfs:range cviii:interlocutor_tag;
   rdfs:subPropertyOf evmpo:has_role_annotation.

cviii:is_based_in a cviii:op_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range cviii:country.
cviii:is_in_group a cviii:op_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range cviii:interlocutor_group;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
cviii:is_academic a cviii:dp_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range xs:boolean.
cviii:is_for_profit a cviii:dp_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range xs:boolean.
cviii:is_military a cviii:dp_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range xs:boolean.
cviii:is_nuclear a cviii:dp_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range xs:boolean.
cviii:is_sme a cviii:dp_interlocutor_tag;
   rdfs:domain cviii:interlocutor_tag;
   rdfs:range xs:boolean.


cviii:country a owl:Class;
   rdfs:subClassOf cviii:role;
   owl:equivalentClass lcc-cr:Country;
   rdfs:comment "country defined following ISO 3166 as implemented by the LCR ontology"^^xs:string.
cviii:op_country a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_country a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_role rdfs:subClassOf cviii:op_country.
evmpo:dp_role rdfs:subClassOf cviii:dp_country.
cviii:op_role rdfs:subClassOf cviii:op_country.
cviii:dp_role rdfs:subClassOf cviii:dp_country.


cviii:personal_title a owl:Class;
   rdfs:subClassOf evmpo:annotation;
   rdfs:comment "personal title, i.e., a title that can be carried by a human agent in association with the name"^^xs:string.
cviii:op_personal_title a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_personal_title a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_annotation rdfs:subClassOf cviii:op_personal_title.
evmpo:dp_annotation rdfs:subClassOf cviii:dp_personal_title.

cviii:corresponds_to a cviii:op_personal_title, owl:TransitiveProperty, owl:SymmetricProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "relation that connects equivalent or approximately equivalent titles (e.g., versions in different languages or from different countries)"^^xs:string;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
cviii:is_extended_form_of a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "the two titles refer to the same degree, only that this one is a longer written form of it than the other"^^xs:string;
   rdfs:subPropertyOf cviii:corresponds_to;
   owl:inverseOf cviii:is_shortened_form_of.
cviii:is_gendered_form_of a cviii:op_personal_title, owl:TransitiveProperty, owl:SymmetricProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "this relation applies if the difference between the two given titles essentially reduces to gender (n.b., this also holds if one of the two forms does not denote gender explicitly)"^^xs:string;
   rdfs:subPropertyOf cviii:corresponds_to.
cviii:is_shortened_form_of a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "the two titles refer to the same degree, only that this one is a shorter written form of it than the other"^^xs:string;
   rdfs:subPropertyOf cviii:corresponds_to;
   owl:inverseOf cviii:is_extended_form_of.
cviii:is_translated_form_of a cviii:op_personal_title, owl:TransitiveProperty, owl:SymmetricProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "this relation applies if the difference between the two given titles essentially reduces to a choice of language or country"^^xs:string;
   rdfs:subPropertyOf cviii:corresponds_to.

cviii:has_prefix_element a cviii:dp_personal_title;
   rdfs:domain cviii:personal_title;
   rdfs:range xs:boolean;
   rdfs:comment "is true if there is a part of the title that goes before the name, such as Dr."^^xs:string.
cviii:has_prefix_text a cviii:dp_personal_title;
   rdfs:domain cviii:personal_title;
   rdfs:range xs:string;
   rdfs:comment "title (or part of it) that goes before the name"^^xs:string.
cviii:has_suffix_element a cviii:dp_personal_title;
   rdfs:domain cviii:personal_title;
   rdfs:range xs:boolean;
   rdfs:comment "is true if there is a part of the title that goes after the name, such as jr."^^xs:string.
cviii:has_suffix_text a cviii:dp_personal_title;
   rdfs:domain cviii:personal_title;
   rdfs:range xs:string;
   rdfs:comment "title (or part of it) that goes after the name"^^xs:string.
cviii:is_local_to a cviii:op_personal_title;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:country;
   rdfs:comment "points to a country where this title is recognized and used in the given form"^^xs:string.
cviii:prefix_is_prior_to a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "relation that determines the order in which title prefixes are written down"^^xs:string;
   owl:inverseOf cviii:prefix_is_posterior_to.
cviii:prefix_is_posterior_to a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "relation that determines the order in which title prefixes are written down"^^xs:string;
   owl:inverseOf cviii:prefix_is_prior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.
cviii:suffix_is_prior_to a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "relation that determines the order in which title suffixes are written down"^^xs:string;
   owl:inverseOf cviii:suffix_is_posterior_to.
cviii:suffix_is_posterior_to a cviii:op_personal_title, owl:TransitiveProperty;
   rdfs:domain cviii:personal_title;
   rdfs:range cviii:personal_title;
   rdfs:comment "relation that determines the order in which title suffixes are written down"^^xs:string;
   owl:inverseOf cviii:suffix_is_prior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

cviii:titular_rank a owl:Class;
   rdfs:subClassOf cviii:personal_title;
   rdfs:comment "titular rank, i.e., a personal title that is part of a hierarchy of titles"^^xs:string.
cviii:op_titular_rank a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_titular_rank a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_personal_title rdfs:subClassOf cviii:op_titular_rank.
cviii:dp_personal_title rdfs:subClassOf cviii:dp_titular_rank.

cviii:is_inferior_to a cviii:op_titular_rank, owl:TransitiveProperty;
   rdfs:domain cviii:titular_rank;
   rdfs:range cviii:titular_rank;
   rdfs:comment "relation corresponding to the hierarchical order of titles"^^xs:string;
   owl:inverseOf cviii:is_superior_to.
cviii:is_superior_to a cviii:op_titular_rank, owl:TransitiveProperty;
   rdfs:domain cviii:titular_rank;
   rdfs:range cviii:titular_rank;
   rdfs:comment "relation corresponding to the hierarchical order of titles"^^xs:string;
   owl:inverseOf cviii:is_inferior_to;
   rdfs:subPropertyOf evmpo:has_meta_annotation.

cviii:academic_title a owl:Class;
   rdfs:subClassOf cviii:titular_rank;
   rdfs:comment "academic title, i.e., a titular rank that signifies possession of an academic degree"^^xs:string.
cviii:op_academic_title a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_academic_title a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_titular_rank rdfs:subClassOf cviii:op_academic_title.
cviii:dp_titular_rank rdfs:subClassOf cviii:dp_academic_title.

cviii:academic_title owl:disjointUnionOf (cviii:nondoctoral_title cviii:doctoral_title cviii:postdoctoral_title).

cviii:postdoctoral_title a owl:Class;
   rdfs:subClassOf cviii:academic_title;
   rdfs:comment "postdoctoral title, i.e., an academic title that is usually only conferred on people with a doctorate"^^xs:string.
cviii:op_postdoctoral_title a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_postdoctoral_title a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_academic_title rdfs:subClassOf cviii:op_postdoctoral_title.
cviii:dp_academic_title rdfs:subClassOf cviii:dp_postdoctoral_title.

cviii:doctoral_title a owl:Class;
   rdfs:subClassOf cviii:academic_title;
   rdfs:comment "doctoral title, i.e., an academic title that signifies the possession of a doctorate degree"^^xs:string.
cviii:op_doctoral_title a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_doctoral_title a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_academic_title rdfs:subClassOf cviii:op_doctoral_title.
cviii:dp_academic_title rdfs:subClassOf cviii:dp_doctoral_title.

cviii:nondoctoral_title a owl:Class;
   rdfs:subClassOf cviii:academic_title;
   rdfs:comment "nondoctoral title, i.e., an academic title which does not confer a doctorate and also usually does not require a doctorate"^^xs:string.
cviii:op_nondoctoral_title a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
cviii:dp_nondoctoral_title a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
cviii:op_academic_title rdfs:subClassOf cviii:op_nondoctoral_title.
cviii:dp_academic_title rdfs:subClassOf cviii:dp_nondoctoral_title.

cviii:TITLE_FEMALE_GENERIC a cviii:personal_title;
   cviii:has_prefix_element "true"^^xs:boolean;
   cviii:has_prefix_text "Ms"^^xs:string;
   cviii:has_suffix_element "false"^^xs:boolean;
   cviii:is_gendered_form_of cviii:TITLE_MALE_GENERIC.
cviii:TITLE_MALE_GENERIC a cviii:personal_title;
   cviii:has_prefix_element "true"^^xs:boolean;
   cviii:has_prefix_text "Mr"^^xs:string;
   cviii:has_suffix_element "false"^^xs:boolean;
   cviii:is_gendered_form_of cviii:TITLE_FEMALE_GENERIC.

cviii:TITLE_DOCTOR_GENERIC a cviii:doctoral_title;
   cviii:has_prefix_element "true"^^xs:boolean;
   cviii:has_prefix_text "Dr"^^xs:string;
   cviii:has_suffix_element "false"^^xs:boolean.

cviii:TITLE_PROFESSOR_GENERIC_LONG a cviii:postdoctoral_title;
   cviii:has_prefix_element "true"^^xs:boolean;
   cviii:has_prefix_text "Professor"^^xs:string;
   cviii:has_suffix_element "false"^^xs:boolean;
   cviii:is_extended_form_of cviii:TITLE_PROFESSOR_GENERIC_SHORT.
cviii:TITLE_PROFESSOR_GENERIC_SHORT a cviii:postdoctoral_title;
   cviii:has_prefix_element "true"^^xs:boolean;
   cviii:has_prefix_text "Prof"^^xs:string;
   cviii:has_suffix_element "false"^^xs:boolean;
   cviii:is_shortened_form_of cviii:TITLE_PROFESSOR_GENERIC_LONG.

# "no nondoctoral title is superior to any postdoctoral title"
#
cviii:is_superior_to_any a cviii:op_titular_rank;
   rdfs:domain cviii:titular_rank;
   rdfs:range owl:Class;
   owl:propertyChainAxiom (cviii:is_superior_to rdf:type).
cviii:nondoctoral_title owl:disjointWith [
   a owl:Restriction;
   owl:onProperty cviii:is_superior_to_any;
   owl:hasValue cviii:postdoctoral_title
].


### EUROPEAN VIRTUAL MARKETPLACE ONTOLOGY (EVMPO) ###

### PROPOSAL DATED 22ND JANUARY 2019 ###

@prefix evmpo: <http://emmc.info/semantics/evmpo/evmpo.ttl#>.

@prefix ccso: <https://w3id.org/ccso/ccso#>.
@prefix emmo: <http://emmc.info/emmo#>.
@prefix iao: <http://purl.obolibrary.org/obo/iao.owl#>.
@prefix ical: <https://www.w3.org/2002/12/cal/icaltzd#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.

<http://emmc.info/semantics/evmpo/evmpo.ttl> a owl:Ontology.

evmpo:marketplace_related_entity a owl:Class.
evmpo:op_marketplace_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_marketplace_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.

evmpo:paradigmatic_entity a owl:Class; rdfs:subClassOf evmpo:marketplace_related_entity.
evmpo:non_paradigmatic_entity a owl:Class; rdfs:subClassOf evmpo:marketplace_related_entity.


###########################################################
###  paradigmatic fundamental category (1): assessment  ###
###########################################################

### sub-ontology ###

evmpo:assessment_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "assessment or non-paradigmatic aspect of an assessment"^^xs:string.
evmpo:op_assessment_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_assessment_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf evmpo:op_assessment_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf evmpo:dp_assessment_related_entity.

### fundamental class ###

evmpo:assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment_related_entity, evmpo:paradigmatic_entity;
   rdfs:comment "assessment, i.e., proposition on accuracy or performance of an entity or on somebody's trust in an entity"^^xs:string.
evmpo:op_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_assessment_related_entity rdfs:subClassOf evmpo:op_assessment.
evmpo:dp_assessment_related_entity rdfs:subClassOf evmpo:dp_assessment.

### subclasses of the fundamental class ###

evmpo:endorsement_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment;
   rdfs:comment "endorsement_assessment, i.e., an assessment (proposition) by which an entity is claimed to be good or fit for a certain purpose"^^xs:string.
evmpo:op_endorsement_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_endorsement_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_assessment rdfs:subClassOf evmpo:op_endorsement_assessment.
evmpo:dp_assessment rdfs:subClassOf evmpo:dp_endorsement_assessment.
evmpo:requirement_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment;
   rdfs:comment "requirement_assessment, i.e., an assessment (proposition) concerning computational requirements (computing time, memory, other hardware, software, and data prerequisites) for an infrastructure or a simulation workflow"^^xs:string.
evmpo:op_requirement_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_requirement_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_assessment rdfs:subClassOf evmpo:op_requirement_assessment.
evmpo:dp_assessment rdfs:subClassOf evmpo:dp_requirement_assessment.
evmpo:validity_assessment a owl:Class;
   rdfs:subClassOf evmpo:assessment;
   rdfs:comment "validity_assessment, i.e., an assessment (proposition) quantifying the uncertainty or error of a model, a simulation workflow, an experimental data point or any other quantitative datum or collection thereof"^^xs:string.
evmpo:op_validity_assessment a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_validity_assessment a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_assessment rdfs:subClassOf evmpo:op_validity_assessment.
evmpo:dp_assessment rdfs:subClassOf evmpo:dp_validity_assessment.

### annotations to the fundamental class ###

evmpo:assessment_annotation a owl:Class;
   rdfs:subClassOf evmpo:annotation, evmpo:assessment_related_entity;
   rdfs:comment "assessment_annotation, i.e., annotation of an assessment"^^xs:string.
evmpo:has_assessment_annotation a evmpo:op_assessment;
   rdfs:domain evmpo:assessment;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.


###############################################################
###  paradigmatic fundamental category (2): calendar_event  ###
###############################################################

### sub-ontology ###

evmpo:calendar_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "calendar_event or non-paradigmatic aspect of a calendar_event"^^xs:string.
evmpo:op_calendar_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_calendar_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf evmpo:op_calendar_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf evmpo:dp_calendar_related_entity.

### fundamental class ###

evmpo:calendar_event a owl:Class;
   rdfs:subClassOf evmpo:calendar_related_entity, evmpo:paradigmatic_entity;
   owl:equivalentClass ical:Vevent;
   rdfs:comment "calendar_event, i.e., a meeting or activity which is scheduled or can be scheduled, equivalent to Vevent (labelled Event) from ICALTZD"^^xs:string.
evmpo:op_calendar_event a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_calendar_event a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_calendar_related_entity rdfs:subClassOf evmpo:op_calendar_event.
evmpo:dp_calendar_related_entity rdfs:subClassOf evmpo:dp_calendar_event.

### subclasses of the fundamental class ###

evmpo:training_event a owl:Class;
   rdfs:subClassOf evmpo:calendar_event;
   owl:equivalentClass ccso:Event;
   rdfs:comment "event at which training activities occur, equivalent to Event from CCSO"^^xs:string.
evmpo:op_training_event a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_training_event a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_calendar_event rdfs:subClassOf evmpo:op_training_event.
evmpo:dp_calendar_event rdfs:subClassOf evmpo:dp_training_event.

### annotations to the fundamental class ###

evmpo:calendar_event_annotation a owl:Class;
   rdfs:subClassOf evmpo:annotation, evmpo:calendar_related_entity;
   rdfs:comment "calendar_event_annotation, i.e., annotation of a calendar_event"^^xs:string.
evmpo:has_calendar_event_annotation a evmpo:op_calendar_event;
   rdfs:domain evmpo:calendar_event;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.


##############################################################
###  paradigmatic fundamental category (3): communication  ###
##############################################################

### sub-ontology ###

evmpo:communication_related_entity a owl:Class;
   rdfs:subClassOf evmpo:marketplace_related_entity;
   rdfs:comment "communication or non-paradigmatic aspect of a communication"^^xs:string.
evmpo:op_communication_related_entity a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_communication_related_entity a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_marketplace_related_entity rdfs:subClassOf evmpo:op_communication_related_entity.
evmpo:dp_marketplace_related_entity rdfs:subClassOf evmpo:dp_communication_related_entity.

### fundamental class ###

evmpo:communication a owl:Class;
   rdfs:subClassOf evmpo:communication_related_entity, evmpo:paradigmatic_entity;
   rdfs:comment "communication, i.e., a statement or sequence of statements that is communicated by a on a virtual marketplace by a stakeholder"^^xs:string.
evmpo:op_communication a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_communication a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication_related_entity rdfs:subClassOf evmpo:op_communication.
evmpo:dp_communication_related_entity rdfs:subClassOf evmpo:dp_communication.

### subclasses of the fundamental class ###

evmpo:declaration a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "declaration, i.e., a communication without a recipient that is processed by a virtual marketplace"^^xs:string.
evmpo:op_declaration a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_declaration a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf evmpo:op_declaration.
evmpo:dp_communication rdfs:subClassOf evmpo:dp_declaration.
evmpo:interlocution a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "interlocution, i.e., a communication which includes one or multiple recipients (not sent only to the marketplace itself, but also to one or multiple stakeholders)"^^xs:string.
evmpo:op_interlocution a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_interlocution a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf evmpo:op_interlocution.
evmpo:dp_communication rdfs:subClassOf evmpo:dp_interlocution.
evmpo:statement a owl:Class;
   rdfs:subClassOf evmpo:communication;
   rdfs:comment "statement, i.e., an elementary communication that cannot be further decomposed into multiple statements"^^xs:string.
evmpo:op_statement a owl:Class;
   rdfs:subClassOf owl:ObjectProperty.
evmpo:dp_statement a owl:Class;
   rdfs:subClassOf owl:DatatypeProperty.
evmpo:op_communication rdfs:subClassOf evmpo:op_statement.
evmpo:dp_communication rdfs:subClassOf evmpo:dp_statement.

### annotations to the fundamental class ###

evmpo:communication_annotation a owl:Class;
   rdfs:subClassOf evmpo:annotation, evmpo:communication_related_entity;
   rdfs:comment "communication_annotation, i.e., annotation of a communication"^^xs:string.
evmpo:has_communication_annotation a evmpo:op_communication;
   rdfs:domain evmpo:communication;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.



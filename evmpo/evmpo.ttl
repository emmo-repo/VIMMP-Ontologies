############################################################
###  paradigmatic fundamental category (6): interpreter  ###
############################################################

### fundamental class ###

evmpo:interpreter a owl:Class;
   rdfs:subClassOf evmpo:paradigmatic_entity;
   rdfs:comment "interpreter, i.e., an interpreter as defined by the EMMO"^^xs:string.
evmpo:op_interpreter a owl:ObjectProperty.
evmpo:dp_interpreter a owl:DatatypeProperty.
evmpo:op_interpreter rdfs:subPropertyOf evmpo:op_paradigmatic_entity.
evmpo:dp_interpreter rdfs:subPropertyOf evmpo:dp_paradigmatic_entity.

### subclasses of the fundamental class ###

evmpo:agent a owl:Class;
   rdfs:subClassOf evmpo:interpreter;
   rdfs:comment "agent, i.e., an entity that has (in principle) the potential of acting at a virtual marketplace, whether that capability is exercised or not"^^xs:string;
   owl:equivalentClass evmpo:agent.
evmpo:op_agent a owl:ObjectProperty.
evmpo:dp_agent a owl:DatatypeProperty.
evmpo:op_agent rdfs:subPropertyOf evmpo:op_interpreter.
evmpo:dp_agent rdfs:subPropertyOf evmpo:dp_interpreter.

evmpo:stakeholder a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "stakeholder, i.e., an agent that acts at one or multiple virtual marketplaces"^^xs:string.
evmpo:op_stakeholder a owl:ObjectProperty.
evmpo:dp_stakeholder a owl:DatatypeProperty.
evmpo:op_stakeholder rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_stakeholder rdfs:subPropertyOf evmpo:dp_agent.

evmpo:agent_group a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "agent group, i.e., an agent that consists of multiple constituent agents, but not in an institutionalized form"^^xs:string.
evmpo:op_agent_group a owl:ObjectProperty.
evmpo:dp_agent_group a owl:DatatypeProperty.
evmpo:op_agent_group rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_agent_group rdfs:subPropertyOf evmpo:dp_agent.
evmpo:institution a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "institution, i.e., any permanent association, organization, office, or comparable entity that has the capability of acting at a virtual marketplace"^^xs:string.
evmpo:op_institution a owl:ObjectProperty.
evmpo:dp_institution a owl:DatatypeProperty.
evmpo:op_institution rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_institution rdfs:subPropertyOf evmpo:dp_agent.
evmpo:human_agent a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "human agent, i.e., a single human being whose identity is disclosed and who has the potential/ability to act at a virtual marketplace"^^xs:string.
evmpo:op_human_agent a owl:ObjectProperty.
evmpo:dp_human_agent a owl:DatatypeProperty.
evmpo:op_human_agent rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_human_agent rdfs:subPropertyOf evmpo:dp_agent.

evmpo:data_provider a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "a data provider"^^xs:string.
evmpo:op_data_provider a owl:ObjectProperty.
evmpo:dp_data_provider a owl:DatatypeProperty.
evmpo:op_data_provider rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_data_provider rdfs:subPropertyOf evmpo:dp_agent.
evmpo:end_user a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "an end user"^^xs:string.
evmpo:op_end_user a owl:ObjectProperty.
evmpo:dp_end_user a owl:DatatypeProperty.
evmpo:op_end_user rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_end_user rdfs:subPropertyOf evmpo:dp_agent.
evmpo:expert a owl:Class;
   rdfs:subClassOf evmpo:human_agent;
   rdfs:comment "expert, i.e., a human agent who can provide competencies and expertise"^^xs:string.
evmpo:op_expert a owl:ObjectProperty.
evmpo:dp_expert a owl:DatatypeProperty.
evmpo:op_expert rdfs:subPropertyOf evmpo:op_human_agent.
evmpo:dp_expert rdfs:subPropertyOf evmpo:dp_human_agent.
evmpo:model_provider a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "a model provider"^^xs:string.
evmpo:op_model_provider a owl:ObjectProperty.
evmpo:dp_model_provider a owl:DatatypeProperty.
evmpo:op_model_provider rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_model_provider rdfs:subPropertyOf evmpo:dp_agent.
evmpo:software_owner a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "a software owner"^^xs:string.
evmpo:op_software_owner a owl:ObjectProperty.
evmpo:dp_software_owner a owl:DatatypeProperty.
evmpo:op_software_owner rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_software_owner rdfs:subPropertyOf evmpo:dp_agent.
evmpo:training_provider a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "entity that can be accessed to acquire/develop competencies in the field of materials modelling and simulation"^^xs:string.
evmpo:op_training_provider a owl:ObjectProperty.
evmpo:dp_training_provider a owl:DatatypeProperty.
evmpo:op_training_provider rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_training_provider rdfs:subPropertyOf evmpo:dp_agent.
evmpo:translator a owl:Class;
   rdfs:subClassOf evmpo:agent;
   rdfs:comment "a translator"^^xs:string.
evmpo:op_translator a owl:ObjectProperty.
evmpo:dp_translator a owl:DatatypeProperty.
evmpo:op_translator rdfs:subPropertyOf evmpo:op_agent.
evmpo:dp_translator rdfs:subPropertyOf evmpo:dp_agent.

### annotations to the fundamental class ###

evmpo:has_interpreter_annotation a owl:ObjectProperty;
   rdfs:subPropertyOf evmpo:op_interpreter;
   rdfs:domain evmpo:interpreter;
   rdfs:range evmpo:annotation;
   rdfs:subPropertyOf evmpo:has_annotation.


